dark:bg-slate-800/50\",\n        borderClass: \"border-slate-200 dark:border-slate-700\",\n        textClass: \"text-slate-400 dark:text-slate-500\",\n        icon: Lock,\n        iconClass: \"text-slate-300 dark:text-slate-600\",\n    },\n};\n\nexport const RoadmapNode: React.FC<RoadmapNodeProps> = ({\n    topic,\n    isSelected,\n    onSelect,\n    scale,\n}) => {\n    const config = statusConfig[topic.status];\n    const Icon = config.icon;\n    const isInteractive = topic.status !== \"locked\";\n\n    // Node width based on title length, min 140, max 200\n    const nodeWidth = Math.min(200, Math.max(140, topic.title.length * 8 + 60));\n\n    return (\n        <motion.div\n            className={cn(\n                \"absolute -translate-x-1/2 -translate-y-1/2 z-10\",\n                isInteractive ? \"cursor-pointer\" : \"cursor-not-allowed\"\n            )}\n            style={{\n                left: topic.position.x,\n                top: topic.position.y,\n                width: nodeWidth,\n            }}\n            initial={{ opacity: 0, scale: 0.8 }}\n            animate={{ opacity: 1, scale: 1 }}\n            transition={{ duration: 0.3 }}\n            whileHover={isInteractive ? { scale: 1.05, zIndex: 50 } : {}}\n            onClick={() => isInteractive && onSelect(topic)}\n            data-testid={`roadmap-node-${topic.id}`}\n        >\n            {/* Glow effect for selected node */}\n            {isSelected && (\n                <motion.div\n                    className=\"absolute -inset-2 bg-indigo-400/20 dark:bg-indigo-500/30 rounded-2xl blur-md\"\n                    layoutId=\"selectedGlow\"\n                />\n            )}\n\n            {/* Main Node */}\n            <div\n                className={cn(\n                    \"relative px-4 py-3 rounded-xl border-2 shadow-lg backdrop-blur-sm transition-all duration-200\",\n                    config.bgClass,\n                    config.borderClass,\n                    isSelected && \"ring-2 ring-indigo-400 ring-offset-2 dark:ring-offset-slate-900\"\n                )}\n            >\n                {/* Status Icon & Title Row */}\n                <div className=\"flex items-center gap-2 mb-1\">\n                    <div\n                        className={cn(\n                            \"w-5 h-5 rounded-full flex items-center justify-center flex-shrink-0\",\n                            topic.status === \"completed\" && \"bg-emerald-100 dark:bg-emerald-800\",\n                            topic.status === \"in_progress\" && \"bg-indigo-100 dark:bg-indigo-800\",\n                            topic.status === \"available\" && \"bg-slate-100 dark:bg-slate-700\",\n                            topic.status === \"locked\" && \"bg-slate-100 dark:bg-slate-700\",\n                            config.pulseClass\n                        )}\n                    >\n                        <Icon size={12} className={config.iconClass} />\n                    </div>\n                    <h3\n                        className={cn(\n                            \"text-xs font-bold truncate flex-1\",\n                            config.textClass\n                        )}\n                        title={topic.title}\n                    >\n                        {topic.title}\n                    </h3>\n                </div>\n\n                {/* Progress/Hours indicator */}\n                <div className=\"flex items-center gap-1 text-[10px] text-slate-400 dark:text-slate-500\">\n                    <Clock size={10} />\n                    <span>{topic.estimatedHours}h</span>\n                    {topic.skills.length > 0 && (\n                        <>\n                            <span className=\"mx-1\">·</span>\n                            <span className=\"truncate\">{topic.skills.slice(0, 2).join(\", \")}</span>\n                        </>\n                    )}\n                </div>\n\n                {/* Progress bar for in_progress */}\n               