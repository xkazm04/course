framer-motion\";\nimport { ZoomIn, ZoomOut, Filter, MapPin, Info, Monitor, Layers, Server, Database, Gamepad2, Smartphone } from \"lucide-react\";\nimport { PrismaticCard } from \"@/app/shared/components\";\nimport { cn } from \"@/app/shared/lib/utils\";\nimport { learningPaths } from \"@/app/shared/lib/mockData\";\n\n// Knowledge Map variant - adapted from gemi/app courses-overview with Spatial theming\ninterface NodePosition { x: number; y: number; }\n\nconst iconMap: Record<string, any> = {\n    Monitor, Layers, Server, Database, Gamepad2, Smartphone\n};\n\nconst connections = [\n    { from: \"frontend\", to: \"fullstack\" },\n    { from: \"backend\", to: \"fullstack\" },\n    { from: \"databases\", to: \"backend\" },\n    { from: \"games\", to: \"frontend\" },\n    { from: \"mobile\", to: \"fullstack\" },\n];\n\nexport const VariantD = () => {\n    const [selectedCategory, setSelectedCategory] = useState<string | null>(null);\n    const [zoom, setZoom] = useState(1);\n    const [hoveredPath, setHoveredPath] = useState<string | null>(null);\n\n    const positions: Record<string, NodePosition> = {\n        \"frontend\": { x: 50, y: 15 },\n        \"fullstack\": { x: 50, y: 50 },\n        \"backend\": { x: 50, y: 85 },\n        \"databases\": { x: 15, y: 70 },\n        \"games\": { x: 20, y: 30 },\n        \"mobile\": { x: 80, y: 30 },\n    };\n\n    const handleZoomIn = () => setZoom(prev => Math.min(prev + 0.2, 2));\n    const handleZoomOut = () => setZoom(prev => Math.max(prev - 0.2, 0.5));\n\n    const categories = [...new Set(learningPaths.map(p => p.name.split(' ')[0]))];\n\n    return (\n        <div className=\"space-y-6\">\n            {/* Header */}\n            <div className=\"flex justify-between items-center\">\n                <div>\n                    <h2 className=\"text-3xl font-black text-slate-900 mb-2\">Knowledge Map</h2>\n                    <p className=\"text-slate-600\">Navigate the universe of skills. Click nodes to explore.</p>\n                </div>\n\n                {/* Filter Controls */}\n                <div className=\"flex gap-2\">\n                    <div className=\"relative group\">\n                        <button className=\"p-3 bg-white/60 backdrop-blur-md border border-white/50 rounded-xl hover:bg-white transition-colors\">\n                            <Filter className=\"w-5 h-5 text-slate-600 group-hover:text-slate-900\" />\n                        </button>\n                        <div className=\"absolute right-0 top-full mt-2 w-48 bg-white/90 backdrop-blur-md border border-white/50 rounded-xl p-2 hidden group-hover:block shadow-xl z-30\">\n                            {categories.map(cat => (\n                                <button\n                                    key={cat}\n                                    onClick={() => setSelectedCategory(selectedCategory === cat ? null : cat)}\n                                    className={cn(\n                                        \"w-full text-left px-3 py-2 rounded-lg text-sm transition-colors\",\n                                        selectedCategory === cat\n                                            ? \"bg-indigo-500 text-white\"\n                                            : \"text-slate-600 hover:bg-slate-100\"\n                                    )}\n                                >\n                                    {cat}\n                                </button>\n                            ))}\n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            {/* Map Container */}\n            <PrismaticCard>\n                <div className=\"relative h-[600px] overflow-hidden\">\n                    {/* Background Grid */}\n                    <div\n                        className=\"absolute inset-0 opacity-30\"\n                        style={{\n                            backgroundSize: '40px 40px',\n                            backgroundImage: 'linear-gradient(to right, rgba(0,0,0,0.05) 1px, transparent 1px), linear-gradient(to bottom, rgba(0,0,0,0.05) 1px, transparent 1px)'\n                        }}\n                    />\n\n                    {/* Radial Gradient */}\n                    <div className=\"absolute inset-0 bg-gradient-radial from-indigo-50/50 via-transparent to-transparent opacity-50\" />\n\n                    {/* Zoom Controls */}\n                    <div className=\"absolute top-4 right-4 z-20 flex flex-col gap-2\">\n                        <button\n                            onClick={handleZoomIn}\n                            className=\"p-2 bg-white/80 backdrop-blur-md rounded-lg hover:bg-white shadow-lg border border-white/50\"\n                        >\n                            <ZoomIn className=\"w-4 h-4 text-slate-600\" />\n                        </button>\n                        <button\n                            onClick={handleZoomOut}\n                            className=\"p-2 bg-white/80 backdrop-blur-md rounded-lg hover:bg-white shadow-lg border border-white/50\"\n                        >\n                            <ZoomOut className=\"w-4 h-4 text-slate-600\" />\n                        </button>\n                    </div>\n\n                    {/* \"You are here\" marker */}\n                    <div className=\"absolute bottom-4 left-4 z-20 bg-white/80 backdrop-blur-md px-4 py-2 rounded-full border border-white/50 flex items-center gap-2 text-xs text-slate-600 shadow-lg\">\n                        <MapPin className=\"w-3 h-3 text-indigo-500\" /> Start: Full Stack\n                    </div>\n\n                    {/* Zoomable Container */}\n                    <motion.div\n                        className=\"w-full h-full relative\"\n                        animate={{ scale: zoom }}\n                        transition={{ type: \"spring\", stiffness: 200, damping: 30 }}\n                        drag\n                        dragConstraints={{ left: -200, right: 200, top: -200, bottom: 200 }}\n                    >\n                        {/* Connection Lines */}\n                        <svg className=\"absolute inset-0 w-full h-full pointer-events-none\">\n                            <defs>\n                                <linearGradient id=\"lineGradient\" x1=\"0%\" y1=\"0%\" x2=\"100%\" y2=\"0%\">\n                                    <stop offset=\"0%\" stopColor=\"rgba(99, 102, 241, 0.3)\" />\n                                    <stop offset=\"100%\" stopColor=\"rgba(168, 85, 247, 0.3)\" />\n                                </linearGradient>\n                            </defs>\n                            {connections.map((conn, i) => {\n                                const start = positions[conn.from];\n                                const end = positions[conn.to];\n                                if (!start || !end) return null;\n                                return (\n                                    <motion.line\n                                        key={i}\n                                        x1={`${start.x}%`} y1={`${start.y}%`}\n                                        x2={`${end.x}%`} y2={`${end.y}%`}\n                                        stroke=\"url(#lineGradient)\"\n                                        strokeWidth=\"3\"\n                                        initial={{ pathLength: 0, opacity: 0 }}\n                                        animate={{ pathLength: 1, opacity: 1 }}\n                                        transition={{ duration: 1, delay: 0.5 + i * 0.1 }}\n                                    />\n                                );\n                            })}\n                        </svg>\n\n                        {/* Path Nodes */}\n                        {learningPaths.map((path, index) => {\n                            const pos = positions[path.id];\n                            if (!pos) return null;\n                            const Icon = iconMap[path.icon];\n                            const isDimmed = selectedCategory && !path.name.startsWith(selectedCategory);\n                            const isHovered = hoveredPath === path.id;\n\n                            return (\n                                <motion.div\n                                    key={path.id}\n                                    className={cn(\n                                        \"absolute -translate-x-1/2 -translate-y-1/2 cursor-pointer z-10\",\n                                        isDimmed ? \"opacity-20 blur-sm scale-90\" : \"opacity-100\"\n                                    )}\n                                    style={{ left: `${pos.x}%`, top: `${pos.y}%` }}\n                                    initial={{ opacity: 0, scale: 0 }}\n                                    animate={{ opacity: isDimmed ? 0.2 : 1, scale: isDimmed ? 0.9 : 1 }}\n                                    transition={{ delay: index * 0.1 }}\n                                    whileHover={{ scale: isDimmed ? 0.9 : 1.15, zIndex: 50 }}\n                                    onMouseEnter={() => setHoveredPath(path.id)}\n                                    onMouseLeave={() => setHoveredPath(null)}\n                                >\n                                    {/* Node */}\n                                    <div className=\"relative\">\n                                        {/* Orbit Ring */}\n                                        <motion.div\n                                            animate={{ rotate: 360 }}\n                                            transition={{ duration: 20, repeat: Infinity, ease: \"linear\" }}\n                                            className=\"absolute -inset-2 border border-dashed border-slate-200/50 rounded-full\"\n                                        />\n\n                                        {/* Main Circle */}\n                                        <div className={cn(\n                                            \"w-24 h-24 rounded-full flex flex-col items-center justify-center text-center transition-all shadow-xl\",\n                                            \"bg-white/90 backdrop-blur-md border-2\",\n                                            isHovered\n                                                ? \"border-indigo-400 shadow-indigo-200/50\"\n                                                : \"border-white/50\"\n                                        )}>\n                                            <div className={cn(\n                                                \"w-10 h-10 rounded-xl flex items-center justify-center mb-1\",\n                                                `bg-${path.color}-100`\n                                            )}>\n                                                {Icon && <Icon size={20} className={`text-${path.color}-600`} />}\n                                            </div>\n                                            <span className=\"text-[10px] font-bold text-slate-700 leading-tight px-2\">\n                                                {path.name.split(' ')[0]}\n                                            </span>\n                                            <div className={cn(\n                                                \"w-2 h-2 rounded-full mt-1\",\n                                                path.id === \"fullstack\" ? \"bg-green-500\" : \"bg-slate-300\"\n                                            )} />\n                                        </div>\n\n                                        {/* Tooltip */}\n                                        <AnimatePresence>\n                                            {isHovered && (\n                                                <motion.div\n                                                    initial={{ opacity: 0, y: 10, scale: 0.9 }}\n                                                    animate={{ opacity: 1, y: 0, scale: 1 }}\n                                                    exit={{ opacity: 0, scale: 0.9 }}\n                                                    className=\"absolute top-full mt-4 left-1/2 -translate-x-1/2 w-56 bg-white/95 backdrop-blur-md border border-white/50 p-4 rounded-2xl shadow-2xl z-50 text-left\"\n                                                >\n                                                    <h4 className=\"font-bold text-sm text-slate-900 mb-1\">{path.name}</h4>\n                                                    <p className=\"text-xs text-slate-500 mb-3\">{path.description}</p>\n                                                    <div className=\"flex gap-2 text-[10px] text-slate-500\">\n                                                        <span className=\"bg-slate-100 px-2 py-0.5 rounded-full\">{path.courses} courses</span>\n                                                        <span className=\"bg-slate-100 px-2 py-0.5 rounded-full\">{path.hours}h</span>\n                                                    </div>\n                                                    <div className=\"flex flex-wrap gap-1 mt-2\">\n                                                        {path.skills.slice(0, 3).map((skill, i) => (\n                                                            <span key={i} className=\"text-[10px] bg-indigo-50 text-indigo-600 px-2 py-0.5 rounded-full\">\n                                                                {skill}\n                                                            </span>\n                                                        ))}\n                                                    </div>\n                                                </motion.div>\n                                            )}\n                                        </AnimatePresence>\n                                    </div>\n                                </motion.div>\n                            );\n                        })}\n                    </motion.div>\n                </div>\n            </PrismaticCard>\n\n            {/* Info Panel */}\n            <div className=\"grid grid-cols-3 gap-4 text-center\">\n                <PrismaticCard glowColor=\"indigo\">\n                    <div className=\"p-4\">\n                        <div className=\"text-2xl font-black text-slate-900\">6</div>\n                        <div className=\"text-sm text-slate-500\">Learning Paths</div>\n                    </div>\n                </PrismaticCard>\n                <PrismaticCard glowColor=\"purple\">\n                    <div className=\"p-4\">\n                        <div className=\"text-2xl font-black text-slate-900\">166</div>\n                        <div className=\"text-sm text-slate-500\">Total Courses</div>\n                    </div>\n                </PrismaticCard>\n                <PrismaticCard glowColor=\"emerald\">\n                    <div className=\"p-4\">\n                        <div className=\"text-2xl font-black text-slate-900\">1,280</div>\n                        <div className=\"text-sm text-slate-500\">Hours Content</div>\n                    </div>\n                </PrismaticCard>\n            </div>\n        </div>\n    );\n};\n","numLines":262,"startLine":1,"totalLines":262}}}
