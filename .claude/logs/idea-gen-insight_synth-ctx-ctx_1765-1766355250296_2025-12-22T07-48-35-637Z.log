ional AI guidance\",\n};\n\n// ============================================================================\n// Types\n// ============================================================================\n\nexport interface GoalPathAdaptiveProps {\n    /** Initial mode (defaults to wizard for progressive disclosure) */\n    initialMode?: GoalPathMode;\n    /** Whether to enable friction detection and escalation suggestions */\n    enableAdaptiveUX?: boolean;\n    /** Whether to show the mode selector in header */\n    showModeSelector?: boolean;\n    /** Callback when mode changes */\n    onModeChange?: (mode: GoalPathMode, reason: \"user\" | \"escalation\") => void;\n    /** Custom className */\n    className?: string;\n}\n\n// ============================================================================\n// Mode Selector Component\n// ============================================================================\n\ninterface ModeSelectorProps {\n    currentMode: GoalPathMode;\n    onModeChange: (mode: GoalPathMode) => void;\n    isOpen: boolean;\n    onToggle: () => void;\n}\n\nconst ModeSelector = ({\n    currentMode,\n    onModeChange,\n    isOpen,\n    onToggle,\n}: ModeSelectorProps) => {\n    const prefersReducedMotion = useReducedMotion();\n\n    return (\n        <div className=\"relative\" data-testid=\"adaptive-mode-selector\">\n            <button\n                onClick={onToggle}\n                className=\"flex items-center gap-2 px-3 py-1.5 rounded-lg bg-[var(--surface-inset)] hover:bg-[var(--surface-overlay)] border border-[var(--border-default)] transition-colors text-sm\"\n                data-testid=\"mode-selector-trigger\"\n            >\n                <Settings size={ICON_SIZES.sm} className=\"text-[var(--text-muted)]\" />\n                <span className=\"font-medium text-[var(--text-secondary)]\">\n                    {MODE_LABELS[currentMode]}\n                </span>\n                <ChevronDown\n                    size={ICON_SIZES.xs}\n                    className={cn(\n                        \"text-[var(--text-muted)] transition-transform\",\n                        isOpen && \"rotate-180\"\n                    )}\n                />\n            </button>\n\n            <AnimatePresence>\n                {isOpen && (\n                    <motion.div\n                        initial={prefersReducedMotion ? false : { opacity: 0, y: -10, scale: 0.95 }}\n                        animate={{ opacity: 1, y: 0, scale: 1 }}\n                        exit={prefersReducedMotion ? { opacity: 0 } : { opacity: 0, y: -10, scale: 0.95 }}\n                        transition={prefersReducedMotion ? { duration: 0 } : { duration: 0.15 }}\n                        className=\"absolute top-full left-0 mt-2 w-64 bg-[var(--surface-overlay)] border border-[var(--border-strong)] rounded-xl shadow-xl z-50 overflow-hidden\"\n                        data-testid=\"mode-selector-dropdown\"\n                    >\n                        <div className=\"p-2 space-y-1\">\n                            {MODE_COMPLEXITY_ORDER.map((mode) => (\n                                <button\n                                    key={mode}\n                                    onClick={() => {\n                                        onModeChange(mode);\n                                        onToggle();\n                                    }}\n                                    className={cn(\n                                        \"w-full p-3 rounded-lg text-left transition-colors\",\n                                        currentMode === mode\n                                            ? \"bg-indigo-50 dark:bg-indigo-900/30 border border-indigo-200 dark:border-indigo-800\"\n                                            : \"hover:bg-[var(--surface-inset)]\"\n                                    )}\n                                    data-testid={`mode-option-${mode}`}\n                                >\n                                    <