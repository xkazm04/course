                       data-testid=\"deadline-slider\"\n                            />\n                            <div className=\"flex justify-between text-xs text-[var(--text-muted)] mt-1\">\n                                <span>1 month</span>\n                                <span>24 months</span>\n                            </div>\n                        </div>\n\n                        {/* Focus Areas */}\n                        <div>\n                            <label className=\"block text-sm font-bold text-[var(--text-secondary)] mb-3\">\n                                <Sparkles size={ICON_SIZES.sm} className=\"inline mr-2\" />\n                                Focus Areas\n                            </label>\n                            <div className=\"grid grid-cols-3 gap-2\">\n                                {FOCUS_AREAS.map(area => (\n                                    <button\n                                        key={area.id}\n                                        onClick={() => toggleFocus(area.id)}\n                                        className={cn(\n                                            \"p-3 rounded-xl border-2 transition-all flex flex-col items-center gap-1\",\n                                            focus.includes(area.id)\n                                                ? \"bg-indigo-50 dark:bg-indigo-900/30 border-[var(--accent-primary)] text-[var(--accent-primary)]\"\n                                                : \"bg-[var(--surface-overlay)] border-[var(--border-strong)] text-[var(--text-secondary)] hover:border-[var(--border-default)]\"\n                                        )}\n                                        data-testid={`focus-area-${area.id}`}\n                                    >\n                                        <area.icon size={ICON_SIZES.md} />\n                                        <span className=\"text-xs font-medium\">{area.label}</span>\n                                    </button>\n                                ))}\n                            </div>\n                        </div>\n                    </div>\n                </PrismaticCard>\n            </div>\n\n            {/* Live Preview */}\n            <div className=\"space-y-6\">\n                <div className=\"flex items-center gap-2 text-sm font-bold text-[var(--text-muted)]\">\n                    <Zap size={ICON_SIZES.sm} className=\"text-amber-500\" />\n                    LIVE PREVIEW\n                </div>\n\n                <PrismaticCard glowColor=\"purple\">\n                    <div className=\"p-6\">\n                        <div className=\"flex items-center gap-3 mb-6\">\n                            <div className=\"w-12 h-12 bg-gradient-to-br from-indigo-500 to-purple-500 rounded-2xl flex items-center justify-center text-white\">\n                                <Target size={ICON_SIZES.lg} />\n                            </div>\n                            <div>\n                                <h3 className=\"font-black text-[var(--text-primary)] text-lg\">{goal || \"Your Learning Path\"}</h3>\n                                <p className=\"text-sm text-[var(--text-muted)]\">Personalized curriculum</p>\n                            </div>\n                        </div>\n\n                        <div className=\"grid grid-cols-3 gap-4 mb-6\">\n                            <div className=\"text-center p-4 bg-[var(--surface-inset)] rounded-xl\">\n                                <div className=\"text-2xl font-black text-[var(--text-primary)]\">{totalHours}</div>\n                                <div className=\"text-xs text-[var(--text-muted)]\">Total Hours</div>\n                            </div>\n                            <div className=\"text-center p-4 bg-[var(--surface-inset)] rounded-xl\">\n                                <div className=\"text-2xl font-black text-[var(--text-primary)]\">{modules}</div>\n                                <div className=\"text-xs text-[var(--text-muted)]\">Modules</div>\n                            </div>\n                            <div className=\"text-center p-4 bg-[var(--surface-inset)] rounded-xl\">\n                                <div className=\"text-2xl font-black text-[var(--text-primary)]\">{topics}</div>\n                                <div className=\"text-xs text-[var(--text-muted)]\">Topics</div>\n                            </div>\n                        </div>\n\n                        <div className=\"space-y-3 mb-6\">\n                            <h4 className=\"text-sm font-bold text-[var(--text-secondary)]\">Curriculum Overview</h4>\n                            {focus.map((f, i) => (\n                                <motion.div\n                                    key={f}\n                                    initial={prefersReducedMotion ? false : { opacity: 0, x: -10 }}\n                                    animate={{ opacity: 1, x: 0 }}\n                                    transition={prefersReducedMotion ? { duration: 0 } : { delay: i * 0.1 }}\n                                    className=\"p-3 bg-[var(--surface-overlay)] rounded-xl border border-[var(--border-strong)] flex items-center gap-3\"\n                                >\n                                    <div className=\"w-8 h-8 bg-indigo-100 dark:bg-indigo-900/50 text-[var(--accent-primary)] rounded-lg flex items-center justify-center text-sm font-bold\">\n                                        {i + 1}\n                                    </div>\n                                    <span className=\"font-medium text-[var(--text-secondary)] capitalize\">\n                                        {f} Development\n                                    </span>\n                                    <span className=\"ml-auto text-xs text-[var(--text-muted)]\">\n                                        ~{hoursPerFocusArea}h\n                                    </span>\n                                </motion.div>\n                            ))}\n                        </div>\n\n                        <div className=\"p-4 bg-gradient-to-r from-indigo-50 to-purple-50 dark:from-indigo-900/30 dark:to-purple-900/30 rounded-xl mb-6\">\n                            <div className=\"flex items-center gap-2 text-sm font-bold text-[var(--accent-primary)] mb-2\">\n                                <BarChart3 size={ICON_SIZES.sm} />\n                                Estimated Outcomes\n                            </div>\n                            <div className=\"grid grid-cols-2 gap-4 text-sm\">\n                                <div className=\"flex items-center gap-2 text-[var(--text-secondary)]\">\n                                    <Users size={ICON_SIZES.sm} />\n                                    Job Ready: {isJobReady ? \"Yes\" : \"Advanced\"}\n                                </div>\n                                <div className=\"flex items-center gap-2 text-[var(--text-secondary)]\">\n                                    <BookOpen size={ICON_SIZES.sm} />\n                                    Skill Level: {skillLevel}\n                                </div>\n                            </div>\n                        </div>\n\n                        <button\n                            className=\"w-full py-4 bg-[var(--btn-primary-bg)] text-[var(--btn-primary-text)] rounded-xl font-bold hover:opacity-90 transition-opacity flex items-center justify-center gap-2\"\n                            data-testid=\"generate-path-btn\"\n                        >\n                            Generate Full Path <ArrowRight size={ICON_SIZES.md} />\n                        </button>\n                    </div>\n                </PrismaticCard>\n            </div>\n        </div>\n    );\n};\n\n// ============================================================================\n// AI Chat Mode Renderer\n// ============================================================================\n\ninterface Message {\n    id: number;\n    type: \"bot\" | \"user\";\n    content: string;\n    options?: string[];\n    /** Optional field name that this message updates in GoalFormState */\n    field?: keyof GoalFormState;\n}\n\n/** Helper to parse time commitment from chat option */\nconst parseTimeCommitment = (option: string): number => {\n    if (option.includes(\"5-10\")) return 7;\n    if (option.includes(\"10-20\")) return 15;\n    if (option.includes(\"20-30\")) return 25;\n    if (option.includes(\"30+\")) return 35;\n    return 15; // default\n};\n\n/** Helper to parse deadline from chat option */\nconst parseDeadline = (option: string): number => {\n    if (option.includes(\"3 months\")) return 3;\n    if (option.includes(\"6 months\")) return 6;\n    if (option.includes(\"12 months\")) return 12;\n    if (option.includes(\"No deadline\")) return 24;\n    return 6; // default\n};\n\n/** Helper to determine which chat step a given formState represents */\nconst getInitialChatStep = (formState: GoalFormState): number => {\n    // If goal is set and not default, user has answered question 1\n    if (formState.goal && formState.goal !== defaultFormState.goal) {\n        // If timeCommitment is not default, user has answered question 2\n        if (formState.timeCommitment !== defaultFormState.timeCommitment) {\n            // If learningStyle is set, user has answered question 3 (skill level maps to learningStyle)\n            if (formState.learningStyle) {\n                // If deadline is not default, user has answered question 4\n                if (formState.deadline !== defaultFormState.deadline) {\n                    return 4; // All questions answered\n                }\n                return 3; // Skill level answered, deadline pending\n            }\n            return 2; // Time commitment answered, skill level pending\n        }\n        return 1; // Goal answered, time commitment pending\n    }\n    return 0; // Starting fresh\n};\n\n/** Create initial messages based on current form state */\nconst createInitialMessages = (formState: GoalFormState): Message[] => {\n    const chatStep = getInitialChatStep(formState);\n    const messages: Message[] = [];\n    let msgId = 1;\n\n    // Always add the initial bot greeting\n    messages.push({\n        id: msgId++,\n        type: \"bot\",\n        content: \"Hi! I'm your AI learning assistant. I'll help you create a personalized learning path. What's your ultimate career goal?\",\n        options: chatStep === 0 ? [\n            \"Become a Backend Developer\",\n            \"Pass AWS Certification\",\n            \"Build a SaaS Product\",\n            \"Let me type my own goal...\"\n        ] : undefined,\n        field: \"goal\"\n    });\n\n    // If we have a goal, add it as user message and continue\n    if (chatStep >= 1 && formState.goal) {\n        messages.push({\n            id: msgId++,\n            type: \"user\",\n            content: formState.goal\n        });\n        messages.push({\n            id: msgId++,\n            type: \"bot\",\n            content: `Great choice! \"${formState.goal}\" is an exciting goal. How many hours per week can you dedicate to learning?`,\n            options: chatStep === 1 ? [\"5-10 hours\", \"10-20 hours\", \"20-30 hours\", \"30+ hours\"] : undefined,\n            field: \"timeCommitment\"\n        });\n    }\n\n    // If we have time commitment, add it and continue\n    if (chatStep >= 2) {\n        const timeStr = formState.timeCommitment <= 10 ? \"5-10 hours\" :\n                       formState.timeCommitment <= 20 ? \"10-20 hours\" :\n                       formState.timeCommitment <= 30 ? \"20-30 hours\" : \"30+ hours\";\n        messages.push({\n            id: msgId++,\n            type: \"user\",\n            content: timeStr\n        });\n        messages.push({\n            id: msgId++,\n            type: \"bot\",\n            content: `Perfect, ${timeStr} is a solid commitment. What's your current skill level?`,\n            options: chatStep === 2 ? [\"Complete Beginner\", \"Some Basics\", \"Intermediate\", \"Advanced\"] : undefined,\n            field: \"learningStyle\"\n        });\n    }\n\n    // If we have learning style / skill level, add it and continue\n    if (chatStep >= 3 && formState.learningStyle) {\n        messages.push({\n            id: msgId++,\n            type: \"user\",\n            content: formState.learningStyle\n        });\n        messages.push({\n            id: msgId++,\n            type: \"bot\",\n            content: `Got it! Last question - when do you want to achieve this goal?`,\n            options: chatStep === 3 ? [\"3 months\", \"6 months\", \"12 months\", \"No deadline\"] : undefined,\n            field: \"deadline\"\n        });\n    }\n\n    // If we have deadline, add it and show completion\n    if (chatStep >= 4) {\n        const deadlineStr = formState.deadline <= 3 ? \"3 months\" :\n                          formState.deadline <= 6 ? \"6 months\" :\n                          formState.deadline <= 12 ? \"12 months\" : \"No deadline\";\n        messages.push({\n            id: msgId++,\n            type: \"user\",\n            content: deadlineStr\n        });\n        messages.push({\n            id: msgId++,\n            type: \"bot\",\n            content: `Excellent! Based on your responses, I've generated a personalized learning path. Here's your roadmap:`\n        });\n    }\n\n    return messages;\n};\n\ninterface AIChatModeProps {\n    formState: GoalFormState;\n    onFormChange: (updates: Partial<GoalFormState>) => void;\n}\n\nconst AIChatMode = ({ formState, onFormChange }: AIChatModeProps) => {\n    // Initialize messages based on current form state (for mode switching continuity)\n    const initialChatStep = getInitialChatStep(formState);\n    const [messages, setMessages] = useState<Message[]>(() => createInitialMessages(formState));\n    const [inputValue, setInputValue] = useState(\"\");\n    const [isTyping, setIsTyping] = useState(false);\n    const [pathGenerated, setPathGenerated] = useState(initialChatStep >= 4);\n    const [chatStep, setChatStep] = useState(initialChatStep);\n    const prefersReducedMotion = useReducedMotion();\n\n    // Use shared path calculator hook for consistent metrics\n    const { estimatedWeeks, lessons, projects, totalHours, modules, topics } = usePathCalculator(formState);\n\n    const simulateResponse = (userMessage: string) => {\n        setIsTyping(true);\n\n        const userMsg: Message = {\n            id: messages.length + 1,\n            type: \"user\",\n            content: userMessage\n        };\n        setMessages(prev => [...prev, userMsg]);\n\n        setTimeout(() => {\n            let botResponse: Message;\n            const currentStep = chatStep;\n\n            if (currentStep === 0) {\n                // User just answered the goal question\n                onFormChange({ goal: userMessage });\n                setChatStep(1);\n                botResponse = {\n                    id: messages.length + 2,\n                    type: \"bot\",\n                    content: `Great choice! \"${userMessage}\" is an exciting goal. How many hours per week can you dedicate to learning?`,\n                    options: [\"5-10 hours\", \"10-20 hours\", \"20-30 hours\", \"30+ hours\"],\n                    field: \"timeCommitment\"\n                };\n            } else if (currentStep === 1) {\n                // User just answered the time commitment question\n                const timeCommitment = parseTimeCommitment(userMessage);\n                onFormChange({ timeCommitment });\n                setChatStep(2);\n                botResponse = {\n                    id: messages.length + 2,\n                    type: \"bot\",\n                    content: `Perfect, ${userMessage} is a solid commitment. What's your current skill level?`,\n                    options: [\"Complete Beginner\", \"Some Basics\", \"Intermediate\", \"Advanced\"],\n                    field: \"learningStyle\"\n                };\n            } else if (currentStep === 2) {\n                // User just answered the skill level question (maps to learningStyle)\n                onFormChange({ learningStyle: userMessage });\n                setChatStep(3);\n                botResponse = {\n                    id: messages.length + 2,\n                    type: \"bot\",\n                    content: `Got it! Last question - when do you want to achieve this goal?`,\n                    options: [\"3 months\", \"6 months\", \"12 months\", \"No deadline\"],\n                    field: \"deadline\"\n                };\n            } else if (currentStep === 3) {\n                // User just answered the deadline question\n                const deadline = parseDeadline(userMessage);\n                onFormChange({ deadline });\n                setChatStep(4);\n                setPathGenerated(true);\n                botResponse = {\n                    id: messages.length + 2,\n                    type: \"bot\",\n                    content: `Excellent! Based on your responses, I've generated a personalized learning path. Here's your roadmap:`\n                };\n            } else {\n                botResponse = {\n                    id: messages.length + 2,\n                    type: \"bot\",\n                    content: \"Thanks for that information! Let me process this...\",\n                };\n            }\n\n            setMessages(prev => [...prev, botResponse]);\n            setIsTyping(false);\n        }, 1500);\n    };\n\n    const handleOptionClick = (option: string) => {\n        simulateResponse(option);\n    };\n\n    const handleSubmit = (e: React.FormEvent) => {\n        e.preventDefault();\n        if (!inputValue.trim()) return;\n        simulateResponse(inputValue);\n        setInputValue(\"\");\n    };\n\n    return (\n        <div className=\"max-w-3xl mx-auto\">\n            <div className=\"text-center mb-8\">\n                <motion.div\n                    initial={prefersReducedMotion ? false : { opacity: 0, y: 20 }}\n                    animate={{ opacity: 1, y: 0 }}\n                    transition={prefersReducedMotion ? { duration: 0 } : undefined}\n                    className=\"inline-flex items-center gap-2 px-4 py-1.5 rounded-full bg-purple-50 dark:bg-purple-900/30 text-[var(--accent-secondary)] font-bold tracking-wide text-xs uppercase mb-4\"\n                >\n                    <Bot size={ICON_SIZES.sm} />\n                    AI-Powered Path Generation\n                </motion.div>\n                <h2 className=\"text-3xl font-black text-[var(--text-primary)] mb-2\">\n                    Chat with Your Learning AI\n                </h2>\n                <p className=\"text-[var(--text-secondary)]\">\n                    Have a conversation to discover your perfect learning path\n                </p>\n            </div>\n\n            <PrismaticCard glowColor=\"purple\">\n                <div className=\"flex flex-col h-[600px]\">\n                    {/* Chat Messages */}\n                    <div className=\"flex-1 overflow-y-auto p-6 space-y-4\">\n                        <AnimatePresence>\n                            {messages.map((message) => (\n                                <motion.div\n                                    key={message.id}\n                                    initial={prefersReducedMotion ? false : { opacity: 0, y: 10 }}\n                                    animate={{ opacity: 1, y: 0 }}\n                                    transition={prefersReducedMotion ? { duration: 0 } : undefined}\n                                    className={cn(\n                                        \"flex gap-3\",\n                                        message.type === \"user\" ? \"flex-row-reverse\" : \"\"\n                                    )}\n                                >\n                                    <div className={cn(\n                                        \"w-8 h-8 rounded-full flex items-center justify-center shrink-0\",\n                                        message.type === \"bot\"\n                                            ? \"bg-gradient-to-br from-purple-500 to-indigo-500 text-white\"\n                                            : \"bg-slate-200 dark:bg-slate-700 text-slate-600 dark:text-slate-300\"\n                                    )}>\n                                        {message.type === \"bot\" ? <Bot size={ICON_SIZES.sm} /> : <User size={ICON_SIZES.sm} />}\n                                    </div>\n                                    <div className={cn(\n                                        \"max-w-[80%]\",\n                                        message.type === \"user\" ? \"text-right\" : \"\"\n                                    )}>\n                                        <div className={cn(\n                                            \"p-4 rounded-2xl\",\n                                            message.type === \"bot\"\n                                                ? \"bg-white dark:bg-slate-800 border border-slate-100 dark:border-slate-700 rounded-tl-sm\"\n                                                : \"bg-slate-900 text-white rounded-tr-sm\"\n                                        )}>\n                                            <p className=\"text-sm\">{message.content}</p>\n                                        </div>\n                                        {message.options && (\n                                            <div className=\"mt-3 flex flex-wrap gap-2\">\n                                                {message.options.map((option, i) => (\n                                                    <button\n                                                        key={i}\n                                                        onClick={() => handleOptionClick(option)}\n                                                        className=\"px-4 py-2 bg-indigo-50 dark:bg-indigo-900/30 hover:bg-indigo-100 dark:hover:bg-indigo-900/50 text-indigo-700 dark:text-indigo-400 rounded-full text-sm font-medium transition-colors\"\n                                                        data-testid={`chat-option-${i}`}\n                                                    >\n                                                        {option}\n                                                    </button>\n                                                ))}\n                                            </div>\n                                        )}\n                                    </div>\n                                </motion.div>\n                            ))}\n                        </AnimatePresence>\n\n                        {isTyping && (\n                            <motion.div\n                                initial={prefersReducedMotion ? false : { opacity: 0 }}\n                                animate={{ opacity: 1 }}\n                                transition={prefersReducedMotion ? { duration: 0 } : undefined}\n                                className=\"flex gap-3\"\n                            >\n                                <div className=\"w-8 h-8 rounded-full bg-gradient-to-br from-purple-500 to-indigo-500 text-white flex items-center justify-center\">\n                                    <Bot size={ICON_SIZES.sm} />\n                                </div>\n                                <div className=\"p-4 bg-white dark:bg-slate-800 border border-slate-100 dark:border-slate-700 rounded-2xl rounded-tl-sm\">\n                                    <div className=\"flex gap-1\">\n                                        <span className={cn(\"w-2 h-2 bg-slate-300 dark:bg-slate-600 rounded-full\", !prefersReducedMotion && \"animate-bounce\")} style={{ animationDelay: \"0ms\" }} />\n                                        <span className={cn(\"w-2 h-2 bg-slate-300 dark:bg-slate-600 rounded-full\", !prefersReducedMotion && \"animate-bounce\")} style={{ animationDelay: \"150ms\" }} />\n                                        <span className={cn(\"w-2 h-2 bg-slate-300 dark:bg-slate-600 rounded-full\", !prefersReducedMotion && \"animate-bounce\")} style={{ animationDelay: \"300ms\" }} />\n                                    </div>\n                                </div>\n                            </motion.div>\n                        )}\n\n                        {/* Generated Path Card - uses metrics from shared GoalFormState */}\n                        {pathGenerated && (\n                            <motion.div\n                                initial={prefersReducedMotion ? false : { opacity: 0, scale: 0.95 }}\n                                animate={{ opacity: 1, scale: 1 }}\n                                transition={prefersReducedMotion ? { duration: 0 } : undefined}\n                                className=\"mt-4 p-6 bg-gradient-to-br from-indigo-500 to-purple-600 rounded-2xl text-white\"\n                                data-testid=\"chat-generated-path\"\n                            >\n                                <div className=\"flex items-center gap-2 mb-4\">\n                                    <Sparkles size={ICON_SIZES.md} />\n                                    <h4 className=\"font-bold\">Your Personalized Path</h4>\n                                </div>\n\n                                {/* Summary showing goal from shared state */}\n                                <div className=\"p-3 bg-white/10 rounded-xl mb-4\">\n                                    <div className=\"text-xs text-white/70 mb-1\">Goal</div>\n                                    <div className=\"font-medium\">{formState.goal}</div>\n                                </div>\n\n                                {/* Metrics calculated from shared GoalFormState */}\n                                <div className=\"grid grid-cols-3 gap-3 mb-4\">\n                                    <div className=\"text-center p-3 bg-white/10 rounded-xl\">\n                                        <div className=\"text-xl font-black\" data-testid=\"chat-result-weeks\">{estimatedWeeks}</div>\n                                        <div className=\"text-xs text-white/70\">Weeks</div>\n                                    </div>\n                                    <div className=\"text-center p-3 bg-white/10 rounded-xl\">\n                                        <div className=\"text-xl font-black\" data-testid=\"chat-result-lessons\">{lessons}</div>\n                                        <div className=\"text-xs text-white/70\">Lessons</div>\n                                    </div>\n                                    <div className=\"text-center p-3 bg-white/10 rounded-xl\">\n                                        <div className=\"text-xl font-black\" data-testid=\"chat-result-projects\">{projects}</div>\n                                        <div className=\"text-xs text-white/70\">Projects</div>\n                                    </div>\n                                </div>\n\n                                <div className=\"space-y-3\">\n                                    {[\n                                        { title: \"Foundations\", duration: `${Math.ceil(estimatedWeeks * 0.18)} weeks` },\n                                        { title: \"Core Skills\", duration: `${Math.ceil(estimatedWeeks * 0.36)} weeks` },\n                                        { title: \"Advanced Concepts\", duration: `${Math.ceil(estimatedWeeks * 0.27)} weeks` },\n                                        { title: \"Projects & Portfolio\", duration: `${Math.ceil(estimatedWeeks * 0.19)} weeks` },\n                                    ].map((item, i) => (\n                                        <div key={i} className=\"flex items-center gap-3 p-3 bg-white/10 rounded-xl\">\n                                            <CheckCircle2 size={ICON_SIZES.md} />\n                                            <span className=\"flex-1 font-medium\">{item.title}</span>\n                                            <span className=\"text-sm text-white/70\">{item.duration}</span>\n                                        </div>\n                                    ))}\n                                </div>\n                                <button\n                                    className=\"w-full mt-4 py-3 bg-white text-indigo-600 rounded-xl font-bold hover:bg-white/90 transition-colors flex items-center justify-center gap-2\"\n                                    data-testid=\"chat-start-journey-btn\"\n                                >\n                                    Start My Journey <ArrowRight size={ICON_SIZES.md} />\n                                </button>\n                            </motion.div>\n                        )}\n                    </div>\n\n                    {/* Input Form */}\n                    <form onSubmit={handleSubmit} className=\"p-4 border-t border-slate-100 dark:border-slate-700\">\n                        <div className=\"flex gap-3\">\n                            <input\n                                type=\"text\"\n                                value={inputValue}\n                                onChange={(e) => setInputValue(e.target.value)}\n                                placeholder=\"Type your message...\"\n                                className=\"flex-1 p-4 bg-slate-50 dark:bg-slate-800 rounded-xl border border-slate-200 dark:border-slate-700 focus:border-indigo-500 focus:ring-2 focus:ring-indigo-100 dark:focus:ring-indigo-900 outline-none transition-all\"\n                                data-testid=\"chat-input\"\n                            />\n                            <button\n                                type=\"submit\"\n                                disabled={!inputValue.trim() || isTyping}\n                                className=\"px-6 bg-slate-900 dark:bg-white text-white dark:text-slate-900 rounded-xl font-bold hover:bg-slate-800 dark:hover:bg-slate-100 transition-colors disabled:opacity-50 disabled:cursor-not-allowed\"\n                                data-testid=\"chat-send-btn\"\n                            >\n                                <Send size={ICON_SIZES.md} />\n                            </button>\n                        </div>\n                    </form>\n                </div>\n            </PrismaticCard>\n        </div>\n    );\n};\n\n// ============================================================================\n// Wizard Mode Renderer\n// ============================================================================\n\ninterface Selection {\n    [key: number]: string;\n}\n\nconst contextHelp: Record<number, string> = {\n    1: \"Define the north star of your learning journey. Be specific!\",\n    2: \"We'll adapt the content format to match how you learn best.\",\n    3: \"This helps us estimate a realistic timeline for you.\",\n};\n\ninterface OptionButtonProps {\n    label: string;\n    onClick: () => void;\n    centered?: boolean;\n    icon?: string;\n    isFirst?: boolean;\n    prefersReducedMotion?: boolean | null;\n}\n\nconst OptionButton = ({ label, onClick, centered, icon, isFirst, prefersReducedMotion }: OptionButtonProps) => {\n    return (\n        <motion.button\n            onClick={onClick}\n            whileHover={prefersReducedMotion ? undefined : { scale: 1.02 }}\n            whileTap={prefersReducedMotion ? undefined : { scale: 0.98 }}\n            className={cn(\n                \"group relative p-4 rounded-xl bg-white dark:bg-slate-800 border-2 border-slate-100 dark:border-slate-700 hover:border-indigo-400 transition-all hover:shadow-lg hover:shadow-indigo-100 dark:hover:shadow-indigo-900/20\",\n                centered ? \"flex flex-col items-center justify-center text-center h-28\" : \"text-left flex items-center justify-between\"\n            )}\n            data-testid={`wizard-option-${label.toLowerCase().replace(/\\s+/g, '-')}`}\n        >\n            {icon && <span className=\"text-3xl mb-2\">{icon}</span>}\n            <span className=\"font-bold text-slate-900 dark:text-white group-hover:text-indigo-600 dark:group-hover:text-indigo-400 transition-colors\">\n                {label}\n            </span>\n            {!centered && (\n                <ArrowRight size={ICON_SIZES.sm} className=\"opacity-0 -translate-x-2 group-hover:opacity-100 group-hover:translate-x-0 transition-all text-indigo-500\" />\n            )}\n            {isFirst && (\n                <span className=\"absolute -top-2 -right-2 px-2 py-0.5 bg-indigo-500 text-white text-[10px] font-bold rounded-full\">\n                    Recommended\n                </span>\n            )}\n        </motion.button>\n    );\n};\n\n/** Helper to determine initial wizard step from form state */\nconst getInitialWizardStep = (formState: GoalFormState): number => {\n    // Check if goal was already selected (not default)\n    if (formState.goal && formState.goal !== defaultFormState.goal) {\n        // Check if learning style was selected\n        if (formState.learningStyle) {\n            // Check if time commitment was changed from default\n            if (formState.timeCommitment !== defaultFormState.timeCommitment) {\n                return 4; // All done\n            }\n            return 3; // Time commitment pending\n        }\n        return 2; // Learning style pending\n    }\n    return 1; // Starting from goal\n};\n\n/** Helper to parse time commitment from wizard slider selection */\nconst parseWizardTimeCommitment = (selection: string): number => {\n    const match = selection.match(/(\\d+)/);\n    return match ? parseInt(match[1], 10) : 10;\n};\n\ninterface WizardModeProps {\n    formState: GoalFormState;\n    onFormChange: (updates: Partial<GoalFormState>) => void;\n}\n\nconst WizardMode = ({ formState, onFormChange }: WizardModeProps) => {\n    // Initialize step based on existing form state (for mode switching continuity)\n    const initialStep = getInitialWizardStep(formState);\n    const [step, setStep] = useState(initialStep);\n    const [history, setHistory] = useState<number[]>([]);\n    const prefersReducedMotion = useReducedMotion();\n    const [selections, setSelections] = useState<Selection>(() => {\n        // Pre-populate selections from form state for display purposes\n        const initial: Selection = {};\n        if (formState.goal && formState.goal !== defaultFormState.goal) {\n            initial[1] = formState.goal;\n        }\n        if (formState.learningStyle) {\n            initial[2] = formState.learningStyle;\n        }\n        if (formState.timeCommitment !== defaultFormState.timeCommitment) {\n            initial[3] = `${formState.timeCommitment} hours/week`;\n        }\n        return initial;\n    });\n    const [timeCommitment, setTimeCommitment] = useState(formState.timeCommitment || 10);\n    const [showConfetti, setShowConfetti] = useState(initialStep >= 4);\n\n    // Use React Context for friction tracking instead of window global\n    const { trackBackNavigation } = useFrictionTracking();\n\n    // Use shared path calculator hook for consistent metrics - now uses actual shared formState\n    const { estimatedWeeks, lessons, projects } = usePathCalculator(formState);\n\n    const nextStep = useCallback((selection: string) => {\n        // Update local selections for display\n        setSelections(prev => ({ ...prev, [step]: selection }));\n        setHistory(prev => [...prev, step]);\n\n        // Update shared form state based on which step we're completing\n        if (step === 1) {\n            // Goal selection\n            onFormChange({ goal: selection });\n        } else if (step === 2) {\n            // Learning style selection\n            onFormChange({ learningStyle: selection });\n        } else if (step === 3) {\n            // Time commitment selection\n            const time = parseWizardTimeCommitment(selection);\n            onFormChange({ timeCommitment: time });\n        }\n\n        setStep(prev => prev + 1);\n    }, [step, onFormChange]);\n\n    const prevStep = useCallback(() => {\n        const last = history[history.length - 1];\n        if (last) {\n            setStep(last);\n            setHistory(prev => prev.slice(0, -1));\n            // Track back navigation for friction detection via React Context\n            trackBackNavigation();\n        }\n    }, [history, trackBackNavigation]);\n\n    const handleFinish = useCallback(() => {\n        setShowConfetti(true);\n    }, []);\n\n    // Keep local time commitment in sync with form state\n    const handleTimeCommitmentChange = useCallback((value: number) => {\n        setTimeCommitment(value);\n    }, []);\n\n    // Keyboard navigation\n    useEffect(() => {\n        const handleKeyDown = (e: KeyboardEvent) => {\n            if (e.key === \"Enter\" && step < 4) {\n                if (step === 1) nextStep(goalOptions[0]);\n                if (step === 2) nextStep(learningStyles[0].label);\n                if (step === 3) nextStep(`${timeCommitment} hours/week`);\n            }\n            if (e.key === \"Escape\" && step > 1) {\n                prevStep();\n            }\n        };\n\n        window.addEventListener(\"keydown\", handleKeyDown);\n        return () => window.removeEventListener(\"keydown\", handleKeyDown);\n    }, [step, timeCommitment, nextStep, prevStep]);\n\n    return (\n        <div className=\"max-w-2xl mx-auto\">\n            {/* Animated Progress Bar */}\n            <div className=\"mb-8\">\n                <div className=\"flex justify-between text-xs text-slate-500 mb-2 uppercase tracking-widest font-bold\">\n                    <span className={cn(step >= 1 && \"text-indigo-600 dark:text-indigo-400\")}>Goal</span>\n                    <span className={cn(step >= 2 && \"text-indigo-600 dark:text-indigo-400\")}>Style</span>\n                    <span className={cn(step >= 3 && \"text-indigo-600 dark:text-indigo-400\")}>Time</span>\n                    <span className={cn(step >= 4 && \"text-indigo-600 dark:text-indigo-400\")}>Done</span>\n                </div>\n                <div className=\"h-2 bg-slate-100 dark:bg-slate-800 rounded-full overflow-hidden\">\n                    <motion.div\n                        className=\"h-full bg-gradient-to-r from-indigo-500 via-purple-500 to-cyan-500 rounded-full\"\n                        initial={prefersReducedMotion ? false : { width: 0 }}\n                        animate={{ width: `${(step / 4) * 100}%` }}\n                        transition={prefersReducedMotion ? { duration: 0 } : { duration: 0.5, ease: \"easeInOut\" }}\n                    />\n                </div>\n            </div>\n\n            <PrismaticCard className=\"min-h-[450px] relative overflow-visible\">\n                <div className=\"p-8 relative\">\n                    {/* Auto-save indicator */}\n                    <div className=\"absolute top-6 right-6 flex items-center gap-2 text-xs text-slate-400\">\n                        <Save size={ICON_SIZES.xs} /> Auto-saved\n                    </div>\n\n                    {/* Back Button */}\n                    {step > 1 && step < 4 && (\n                        <button\n                            onClick={prevStep}\n                            className=\"absolute top-6 left-6 text-slate-400 hover:text-slate-700 dark:hover:text-slate-300 flex items-center gap-1 text-sm font-medium transition-colors\"\n                            data-testid=\"wizard-back-btn\"\n                        >\n                            <ChevronLeft size={ICON_SIZES.sm} /> Back\n                        </button>\n                    )}\n\n                    <AnimatePresence mode=\"wait\">\n                        <motion.div\n                            key={step}\n                            initial={prefersReducedMotion ? false : { opacity: 0, x: 20 }}\n                            animate={{ opacity: 1, x: 0 }}\n                            exit={prefersReducedMotion ? { opacity: 0 } : { opacity: 0, x: -20 }}\n                            transition={prefersReducedMotion ? { duration: 0 } : { duration: 0.3 }}\n                            className=\"h-full flex flex-col justify-center pt-8\"\n                        >\n                            {step === 1 && (\n                                <>\n                                    <div className=\"flex items-center gap-2 mb-2\">\n                                        <Target size={ICON_SIZES.md} className=\"text-[var(--accent-primary)]\" />\n                                        <h2 className=\"text-2xl font-black text-[var(--text-primary)]\">What's your primary goal?</h2>\n                                    </div>\n                                    <p className=\"text-[var(--text-muted)] mb-6 flex items-center gap-2\">\n                                        <Info size={ICON_SIZES.sm} />\n                                        {contextHelp[1]}\n                                    </p>\n                                    <div className=\"grid gap-3\">\n                                        {goalOptions.map((opt, i) => (\n                                            <OptionButton\n                                                key={opt}\n                                                label={opt}\n                                                onClick={() => nextStep(opt)}\n                                                isFirst={i === 0}\n                                                prefersReducedMotion={prefersReducedMotion}\n                                            />\n                                        ))}\n                                    </div>\n                                </>\n                            )}\n\n                            {step === 2 && (\n                                <>\n                                    <div className=\"flex items-center gap-2 mb-2\">\n                                        <BookOpen size={ICON_SIZES.md} className=\"text-purple-500\" />\n                                        <h2 className=\"text-2xl font-black text-slate-900 dark:text-white\">How do you learn best?</h2>\n                                    </div>\n                                    <p className=\"text-slate-500 mb-6 flex items-center gap-2\">\n                                        <Info size={ICON_SIZES.sm} />\n                                        {contextHelp[2]}\n                                    </p>\n                                    <div className=\"grid grid-cols-2 gap-4\">\n                                        {learningStyles.map((style, i) => (\n                                            <OptionButton\n                                                key={style.label}\n                                                label={style.label}\n                                                onClick={() => nextStep(style.label)}\n                                                centered\n                                                icon={style.icon}\n                                                isFirst={i === 0}\n                                                prefersReducedMotion={prefersReducedMotion}\n                                            />\n                                        ))}\n                                    </div>\n                                </>\n                            )}\n\n                            {step === 3 && (\n                                <>\n                                    <div className=\"flex items-center gap-2 mb-2\">\n                                        <Clock size={ICON_SIZES.md} className=\"text-cyan-500\" />\n                                        <h2 className=\"text-2xl font-black text-slate-900 dark:text-white\">Weekly time commitment?</h2>\n                                    </div>\n                                    <p className=\"text-slate-500 mb-6 flex items-center gap-2\">\n                                        <Info size={ICON_SIZES.sm} />\n                                        {contextHelp[3]}\n                                    </p>\n                                    <div className=\"space-y-6\">\n                                        <div className=\"flex justify-between text-sm font-medium text-slate-600 dark:text-slate-400\">\n                                            <span>Casual (2h/week)</span>\n                                            <span>Intense (20h/week)</span>\n                                        </div>\n                                        <div className=\"relative\">\n                                            <input\n                                                type=\"range\"\n                                                min=\"2\"\n                                                max=\"20\"\n                                                value={timeCommitment}\n                                                onChange={(e) => setTimeCommitment(Number(e.target.value))}\n                                                className=\"w-full h-3 bg-slate-100 dark:bg-slate-700 rounded-full appearance-none cursor-pointer accent-indigo-500\"\n                                                data-testid=\"wizard-time-slider\"\n                                            />\n                                            <div className=\"absolute -top-8 left-1/2 -translate-x-1/2 px-3 py-1 bg-slate-900 dark:bg-white text-white dark:text-slate-900 rounded-lg text-sm font-bold\">\n                                                {timeCommitment}h/week\n                                            </div>\n                                        </div>\n                                        <div className=\"pt-4\">\n                                            <button\n                                                onClick={() => nextStep(`${timeCommitment} hours/week`)}\n                                                className=\"w-full py-4 bg-gradient-to-r from-indigo-600 to-purple-600 text-white rounded-xl font-bold hover:opacity-90 transition-opacity flex items-center justify-center gap-2\"\n                                                data-testid=\"wizard-generate-btn\"\n                                            >\n                                                <Sparkles size={ICON_SIZES.sm} />\n                                                Generate My Path\n                                            </button>\n                                        </div>\n                                    </div>\n                                </>\n                            )}\n\n                            {step === 4 && (\n                                <div className=\"text-center\">\n                                    <motion.div\n                                        initial={prefersReducedMotion ? false : { scale: 0 }}\n                                        animate={{ scale: 1 }}\n                                        transition={prefersReducedMotion ? { duration: 0 } : { type: \"spring\", bounce: 0.5 }}\n                                        className=\"w-20 h-20 bg-gradient-to-br from-emerald-400 to-teal-500 rounded-full flex items-center justify-center mx-auto mb-6 shadow-lg shadow-emerald-200 dark:shadow-emerald-900/50\"\n                                        onAnimationComplete={handleFinish}\n                                    >\n                                        <Check size={ICON_SIZES.xl} className=\"text-white\" />\n                                    </motion.div>\n\n                                    <h2 className=\"text-3xl font-black text-slate-900 dark:text-white mb-4\">\n                                        Path Generated!\n                                    </h2>\n\n                                    {/* Summary Card */}\n                                    <div className=\"bg-slate-50 dark:bg-slate-800 rounded-2xl p-6 text-left mb-6 border border-slate-100 dark:border-slate-700 space-y-3\">\n                                        <div className=\"flex justify-between border-b border-slate-100 dark:border-slate-700 pb-3\">\n                                            <span className=\"text-slate-500\">Goal</span>\n                                            <span className=\"font-bold text-slate-900 dark:text-white\">{selections[1]}</span>\n                                        </div>\n                                        <div className=\"flex justify-between border-b border-slate-100 dark:border-slate-700 pb-3\">\n                                            <span className=\"text-slate-500\">Learning Style</span>\n                                            <span className=\"font-bold text-slate-900 dark:text-white\">{selections[2]}</span>\n                                        </div>\n                                        <div className=\"flex justify-between\">\n                                            <span className=\"text-slate-500\">Time Commitment</span>\n                                            <span className=\"font-bold text-slate-900 dark:text-white\">{selections[3]}</span>\n                                        </div>\n                                    </div>\n\n                                    {/* Estimated Results - calculated from timeCommitment */}\n                                    <div className=\"grid grid-cols-3 gap-3 mb-6\">\n                                        <div className=\"p-3 bg-indigo-50 dark:bg-indigo-900/30 rounded-xl\">\n                                            <div className=\"text-xl font-black text-indigo-600 dark:text-indigo-400\" data-testid=\"wizard-result-weeks\">{estimatedWeeks}</div>\n                                            <div className=\"text-xs text-slate-600 dark:text-slate-400\">Weeks</div>\n                                        </div>\n                                        <div className=\"p-3 bg-purple-50 dark:bg-purple-900/30 rounded-xl\">\n                                            <div className=\"text-xl font-black text-purple-600 dark:text-purple-400\" data-testid=\"wizard-result-lessons\">{lessons}</div>\n                                            <div className=\"text-xs text-slate-600 dark:text-slate-400\">Lessons</div>\n                                        </div>\n                                        <div className=\"p-3 bg-emerald-50 dark:bg-emerald-900/30 rounded-xl\">\n                                            <div className=\"text-xl font-black text-emerald-600 dark:text-emerald-400\" data-testid=\"wizard-result-projects\">{projects}</div>\n                                            <div className=\"text-xs text-slate-600 dark:text-slate-400\">Projects</div>\n                                        </div>\n                                    </div>\n\n                                    <button\n                                        onClick={handleFinish}\n                                        className=\"px-8 py-4 bg-slate-900 dark:bg-white text-white dark:text-slate-900 rounded-xl font-bold hover:bg-slate-800 dark:hover:bg-slate-100 transition-colors flex items-center gap-2 mx-auto\"\n                                        data-testid=\"wizard-start-btn\"\n                                    >\n                                        <Zap size={ICON_SIZES.sm} /> Start Learning Now\n                                    </button>\n                                </div>\n                            )}\n                        </motion.div>\n                    </AnimatePresence>\n                </div>\n            </PrismaticCard>\n\n            {/* Keyboard hint */}\n            {step < 4 && (\n                <motion.div\n                    initial={prefersReducedMotion ? false : { opacity: 0, y: 10 }}\n                    animate={{ opacity: 1, y: 0 }}\n                    transition={prefersReducedMotion ? { duration: 0 } : { delay: 0.5 }}\n                    className=\"mt-6 text-center text-xs text-slate-400 flex items-center justify-center gap-2\"\n                >\n                    <Keyboard size={ICON_SIZES.xs} />\n                    Press <kbd className=\"px-1.5 py-0.5 bg-slate-100 dark:bg-slate-800 border border-slate-200 dark:border-slate-700 rounded mx-1 font-mono\">Enter</kbd> to select first option\n                    {step > 1 && (\n                        <>\n                            {\" • \"}\n                            <kbd className=\"px-1.5 py-0.5 bg-slate-100 dark:bg-slate-800 border border-slate-200 dark:border-slate-700 rounded mx-1 font-mono\">Esc</kbd> to go back\n                        </>\n                    )}\n                </motion.div>\n            )}\n        </div>\n    );\n};\n\n// ============================================================================\n// Polymorphic Goal Path Component\n// ============================================================================\n\nexport interface GoalPathPolymorphicProps extends Omit<PolymorphicVariantProps<GoalPathMode>, 'data'> {}\n\n/**\n * Polymorphic Goal Path Component\n *\n * A single component that can render in multiple modes (live-form, ai-chat, wizard).\n * Each mode represents a different interaction strategy for defining learning goals,\n * but all modes work with the same underlying data and produce the same outputs.\n */\nexport const GoalPathPolymorphic = ({\n    mode,\n    onModeChange,\n    className,\n}: GoalPathPolymorphicProps) => {\n    const [formState, setFormState] = useState<GoalFormState>(defaultFormState);\n\n    const handleFormChange = (updates: Partial<GoalFormState>) => {\n        setFormState(prev => ({ ...prev, ...updates }));\n    };\n\n    return (\n        <div\n            className={cn(\"w-full\", className)}\n            data-testid={`goal-path-${mode}`}\n            data-mode={mode}\n        >\n            {mode === \"live-form\" && (\n                <LiveFormMode formState={formState} onFormChange={handleFormChange} />\n            )}\n            {mode === \"ai-chat\" && (\n                <AIChatMode formState={formState} onFormChange={handleFormChange} />\n            )}\n            {mode === \"wizard\" && (\n                <WizardMode formState={formState} onFormChange={handleFormChange} />\n            )}\n        </div>\n    );\n};\n\n// ============================================================================\n// User Preference-Aware Component\n// ============================================================================\n\nimport { useInteractionMode } from \"@/app/features/interaction-mode\";\n\n/**\n * Goal Path component that automatically uses the user's preferred interaction mode.\n * This component reads from the InteractionModeContext and renders the appropriate mode.\n * Falls back to the defaultMode if no preference is set or context is not available.\n */\nexport const GoalPathWithPreference = ({ className }: { className?: string }) => {\n    const { mode, isLoaded } = useInteractionMode();\n\n    // Show a loading state while preference is loading from localStorage\n    if (!isLoaded) {\n        return (\n            <div className={cn(\"w-full animate-pulse\", className)}>\n                <div className=\"h-96 bg-[var(--surface-inset)] rounded-3xl\" />\n            </div>\n        );\n    }\n\n    return <GoalPathPolymorphic mode={mode} className={className} />;\n};\n\n// ============================================================================\n// Convenience Exports for Backward Compatibility\n// ============================================================================\n\n/** Live Form mode - maintains backward compatibility with VariantB */\nexport const GoalPathLiveForm = () => <GoalPathPolymorphic mode=\"live-form\" />;\n\n/** AI Chat mode - mai