\nexport function getEntryPoints(): GraphNode[] {\n    return GRAPH_NODES.filter(node => node.isEntryPoint);\n}\n\n/**\n * Get all edges from a specific node\n */\nexport function getOutgoingEdges(nodeId: LearningDomainId): GraphEdge[] {\n    return GRAPH_EDGES.filter(edge => edge.from === nodeId);\n}\n\n/**\n * Get all edges pointing to a specific node\n */\nexport function getIncomingEdges(nodeId: LearningDomainId): GraphEdge[] {\n    return GRAPH_EDGES.filter(edge => edge.to === nodeId);\n}\n\n/**\n * Get all connected nodes (both incoming and outgoing)\n */\nexport function getConnectedNodes(nodeId: LearningDomainId): LearningDomainId[] {\n    const connected = new Set<LearningDomainId>();\n\n    GRAPH_EDGES.forEach(edge => {\n        if (edge.from === nodeId) connected.add(edge.to);\n        if (edge.to === nodeId) connected.add(edge.from);\n    });\n\n    return Array.from(connected);\n}\n\n/**\n * Get prerequisites for a node (nodes that should be learned first)\n */\nexport function getPrerequisites(nodeId: LearningDomainId): LearningDomainId[] {\n    return GRAPH_EDGES\n        .filter(edge => edge.to === nodeId && edge.type === \"prerequisite\")\n        .map(edge => edge.from);\n}\n\n/**\n * Get nodes that build upon a given node\n */\nexport function getDependents(nodeId: LearningDomainId): LearningDomainId[] {\n    return GRAPH_EDGES\n        .filter(edge => edge.from === nodeId)\n        .map(edge => edge.to);\n}\n\n/**\n * Check if there's a direct connection between two nodes\n */\nexport function areConnected(nodeA: LearningDomainId, nodeB: LearningDomainId): boolean {\n    return GRAPH_EDGES.some(\n        edge =>\n            (edge.from === nodeA && edge.to === nodeB) ||\n            (edge.from === nodeB && edge.to === nodeA)\n    );\n}\n\n/**\n * Get the relationship between two nodes if it exists\n */\nexport function getRelationship(\n    from: LearningDomainId,\n    to: LearningDomainId\n): GraphEdge | undefined {\n    return GRAPH_EDGES.find(edge => edge.from === from && edge.to === to);\n}\n\n// ============================================================================\n// VISUALIZATION HELPERS\n// ============================================================================\n\n/**\n * Position lookup map for efficient access in spatial views\n */\nexport function getPositionMap(): Record<LearningDomainId, SpatialPosition> {\n    return GRAPH_NODES.reduce(\n        (map, node) => {\n            map[node.id] = node.position;\n            return map;\n        },\n        {} as Record<LearningDomainId, SpatialPosition>\n    );\n}\n\n/**\n * Get connections in the format needed for SVG line rendering\n */\nexport function getConnectionsForRendering(): Array<{\n    from: LearningDomainId;\n    to: LearningDomainId;\n    startPos: SpatialPosition;\n    endPos: SpatialPosition;\n    weight: number;\n    type: RelationshipType;\n}> {\n    const positionMap = getPositionMap();\n\n    return GRAPH_EDGES.map(edge => ({\n        from: edge.from,\n        to: edge.to,\n        startPos: positionMap[edge.from],\n        endPos: positionMap[edge.to],\n        weight: edge.weight,\n        type: edge.type,\n    })).filter(conn => conn.startPos && conn.endPos);\n}\n\n/**\n * Group nodes by hierarchy level for tree rendering\n */\nexport function getNodesGroupedByHierarchy(): Record<HierarchyLevel, GraphNode[]> {\n    return GRAPH_NODES.reduce(\n        (groups, node) => {\n            if (!groups[node.hierarchyLevel]) {\n                groups[node.hierarchyLevel] = [];\n            }\n            groups[node.hierarchyLevel].push(node);\n            return groups;\n        },\n        {} as Record<HierarchyLevel, GraphNode[]>\n    );\n}\n\n/**\n * Group nodes by timeline phase for roadmap rendering\n */\nexport function getNodesGroupedByPhase(): Record<TimelinePhase, GraphNode[]> {\n    return GRAPH_NODES.reduce(\n        (groups, node) => {\n            if (!groups[node.timelinePhase]) {\n                groups[node.