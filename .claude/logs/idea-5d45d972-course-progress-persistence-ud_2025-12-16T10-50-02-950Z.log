            animate={{ opacity: 1 }}\n                        exit={{ opacity: 0 }}\n                        transition={{ duration: 0.3 }}\n                    >\n                        <HomeView\n                            onSelectModule={handleSelectModule}\n                            continueLearning={transformedContinueLearning}\n                            totalWatchTime={totalWatchTime}\n                            overallCompletion={overallCompletion}\n                            courseProgress={courseProgress}\n                            onExport={handleExport}\n                            onClearAll={handleClearAll}\n                        />\n                    </motion.div>\n                )}\n            </AnimatePresence>\n\n            {/* Export Modal */}\n            <ProgressExportModal\n                isOpen={isExportModalOpen}\n                onClose={() => setIsExportModalOpen(false)}\n                onExport={() => {\n                    const { exportProgressData } = require(\"@/app/features/progress\");\n                    return exportProgressData();\n                }}\n                onDownload={() => {\n                    const { downloadProgressData } = require(\"@/app/features/progress\");\n                    downloadProgressData();\n                }}\n                onImport={(jsonString) => {\n                    const { importProgressData } = require(\"@/app/features/progress\");\n                    const success = importProgressData(jsonString);\n                    if (success) refreshProgress();\n                    return success;\n                }}\n            />\n        </div>\n    );\n}","originalFile":"\"use client\";\n\nimport React, { useState, useRef, useEffect } from \"react\";\nimport { motion, AnimatePresence, useMotionValue, useSpring, useTransform } from \"framer-motion\";\nimport {\n    Compass, Target, Briefcase, BookOpen, Layers, ArrowRight, ArrowLeft,\n    Sparkles, Cpu, Globe, Hexagon, Zap, Gr