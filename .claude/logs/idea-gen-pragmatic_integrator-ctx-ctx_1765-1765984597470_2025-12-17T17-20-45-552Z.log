, [trackAbandonmentAttempt, enableAdaptiveUX]);\n   245→\n   246→    // Handle mode change\n   247→    const handleModeChange = useCallback(\n   248→        (mode: GoalPathMode, reason: \"user\" | \"escalation\" = \"user\") => {\n   249→            setCurrentMode(mode);\n   250→            resetSignals();\n   251→            setShowEscalation(false);\n   252→            setCurrentSuggestion(null);\n   253→            onModeChange?.(mode, reason);\n   254→        },\n   255→        [resetSignals, onModeChange]\n   256→    );\n   257→\n   258→    // Handle escalation acceptance\n   259→    const handleAcceptEscalation = useCallback(\n   260→        (mode: GoalPathMode) => {\n   261→            handleModeChange(mode, \"escalation\");\n   262→        },\n   263→        [handleModeChange]\n   264→    );\n   265→\n   266→    // Handle escalation dismissal\n   267→    const handleDismissEscalation = useCallback(() => {\n   268→        setShowEscalation(false);\n   269→        setCurrentSuggestion(null);\n   270→        dismissSuggestion();\n   271→    }, [dismissSuggestion]);\n   272→\n   273→    // Close selector when clicking outside\n   274→    useEffect(() => {\n   275→        if (!selectorOpen) return;\n   276→\n   277→        const handleClickOutside = (e: MouseEvent) => {\n   278→            const target = e.target as HTMLElement;\n   279→            if (!target.closest(\"[data-testid='adaptive-mode-selector']\")) {\n   280→                setSelectorOpen(false);\n   281→            }\n   282→        };\n   283→\n   284→        document.addEventListener(\"click\", handleClickOutside);\n   285→        return () => document.removeEventListener(\"click\", handleClickOutside);\n   286→    }, [selectorOpen]);\n   287→\n   288→    return (\n   289→        <div\n   290→            className={cn(\"w-full\", className)}\n   291→            data-testid=\"goal-path-adaptive\"\n   292→            data-current-mode={currentMode}\n   293→        >\n   294→            {/* Header w