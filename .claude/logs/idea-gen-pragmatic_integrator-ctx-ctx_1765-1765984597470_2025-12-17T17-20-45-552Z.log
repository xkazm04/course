       ]\n   304→    }\n   305→];\n   306→\n   307→const AIChatMode = () => {\n   308→    const [messages, setMessages] = useState<Message[]>(initialMessages);\n   309→    const [inputValue, setInputValue] = useState(\"\");\n   310→    const [isTyping, setIsTyping] = useState(false);\n   311→    const [pathGenerated, setPathGenerated] = useState(false);\n   312→\n   313→    const simulateResponse = (userMessage: string) => {\n   314→        setIsTyping(true);\n   315→\n   316→        const userMsg: Message = {\n   317→            id: messages.length + 1,\n   318→            type: \"user\",\n   319→            content: userMessage\n   320→        };\n   321→        setMessages(prev => [...prev, userMsg]);\n   322→\n   323→        setTimeout(() => {\n   324→            let botResponse: Message;\n   325→\n   326→            if (messages.length === 1) {\n   327→                botResponse = {\n   328→                    id: messages.length + 2,\n   329→                    type: \"bot\",\n   330→                    content: `Great choice! \"${userMessage}\" is an exciting goal. How many hours per week can you dedicate to learning?`,\n   331→                    options: [\"5-10 hours\", \"10-20 hours\", \"20-30 hours\", \"30+ hours\"]\n   332→                };\n   333→            } else if (messages.length === 3) {\n   334→                botResponse = {\n   335→                    id: messages.length + 2,\n   336→                    type: \"bot\",\n   337→                    content: `Perfect, ${userMessage} is a solid commitment. What's your current skill level?`,\n   338→                    options: [\"Complete Beginner\", \"Some Basics\", \"Intermediate\", \"Advanced\"]\n   339→                };\n   340→            } else if (messages.length === 5) {\n   341→                botResponse = {\n   342→                    id: messages.length + 2,\n   343→                    type: \"bot\",\n   344→                    content: `Got it! Last question - when do you want to achieve this goal?`,\n   345→                    options: [\"3 months\", \"6 months\", \"12 months\", \"No deadline\"]\n   346→                };\n   347→            } else if (messages.length === 7) {\n   348→                setPathGenerated(true);\n   349→                botResponse = {\n   350→                    id: messages.length + 2,\n   351→                    type: \"bot\",\n   352→                    content: `Excellent! Based on your responses, I've generated a personalized learning path. Here's your roadmap:`\n   353→                };\n   354→            } else {\n   355→                botResponse = {\n   356→                    id: messages.length + 2,\n   357→                    type: \"bot\",\n   358→                    content: \"Thanks for that information! Let me process this...\",\n   359→                };\n   360→            }\n   361→\n   362→            setMessages(prev => [...prev, botResponse]);\n   363→            setIsTyping(false);\n   364→        }, 1500);\n   365→    };\n   366→\n   367→    const handleOptionClick = (option: string) => {\n   368→        simulateResponse(option);\n   369→    };\n   370→\n   371→    const handleSubmit = (e: React.FormEvent) => {\n   372→        e.preventDefault();\n   373→        if (!inputValue.trim()) return;\n   374→        simulateResponse(inputValue);\n   375→        setInputValue(\"\");\n   376→    };\n   377→\n   378→    return (\n   379→        <div className=\"max-w-3xl mx-auto\">\n   380→            <div className=\"text-center mb-8\">\n   381→                <motion.div\n   382→                    initial={{ opacity: 0, y: 20 }}\n   383→                    animate={{ opacity: 1, y: 0 }}\n   384→                    className=\"inline-flex items-center gap-2 px-4 py-1.5 rounded-full bg-purple-50 dark:bg-purple-900/30 text-[var(--accent-secondary)] font-bold tracking-wide text-xs uppercase mb-4\"\n   385→                >\n   386→                    <Bot size={ICON_SIZES.sm} />\n   387→          