r text-center h-28\" : \"text-left flex items-center justify-between\"\n   563→            )}\n   564→            data-testid={`wizard-option-${label.toLowerCase().replace(/\\s+/g, '-')}`}\n   565→        >\n   566→            {icon && <span className=\"text-3xl mb-2\">{icon}</span>}\n   567→            <span className=\"font-bold text-slate-900 dark:text-white group-hover:text-indigo-600 dark:group-hover:text-indigo-400 transition-colors\">\n   568→                {label}\n   569→            </span>\n   570→            {!centered && (\n   571→                <ArrowRight size={ICON_SIZES.sm} className=\"opacity-0 -translate-x-2 group-hover:opacity-100 group-hover:translate-x-0 transition-all text-indigo-500\" />\n   572→            )}\n   573→            {isFirst && (\n   574→                <span className=\"absolute -top-2 -right-2 px-2 py-0.5 bg-indigo-500 text-white text-[10px] font-bold rounded-full\">\n   575→                    Recommended\n   576→                </span>\n   577→            )}\n   578→        </motion.button>\n   579→    );\n   580→};\n   581→\n   582→const WizardMode = () => {\n   583→    const [step, setStep] = useState(1);\n   584→    const [history, setHistory] = useState<number[]>([]);\n   585→    const [selections, setSelections] = useState<Selection>({});\n   586→    const [timeCommitment, setTimeCommitment] = useState(10);\n   587→    const [showConfetti, setShowConfetti] = useState(false);\n   588→\n   589→    const nextStep = useCallback((selection: string) => {\n   590→        setSelections(prev => ({ ...prev, [step]: selection }));\n   591→        setHistory(prev => [...prev, step]);\n   592→        setStep(prev => prev + 1);\n   593→    }, [step]);\n   594→\n   595→    const prevStep = useCallback(() => {\n   596→        const last = history[history.length - 1];\n   597→        if (last) {\n   598→            setStep(last);\n   599→            setHistory(prev => prev.slice(0, -1));\n   600→            // Track back navigation for