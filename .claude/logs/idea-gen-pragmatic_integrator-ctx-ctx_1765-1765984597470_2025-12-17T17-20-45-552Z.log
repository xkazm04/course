    timeOnCurrentStep: number;        // Time (ms) spent on current step\n    rapidClicks: number;              // Rapid/frustrated clicks detected\n    inactivityPeriods: number;        // Long pauses indicating confusion\n    errorCount: number;               // Number of validation errors\n}\n\n/**\n * Mode escalation suggestion based on detected friction\n */\nexport interface EscalationSuggestion {\n    currentMode: GoalPathMode;\n    suggestedMode: GoalPathMode;\n    reason: string;\n    confidence: number; // 0-1 scale\n}\n\nconst FRICTION_THRESHOLDS = {\n    backNavigationCount: 2,           // Going back 2+ times suggests need for more control\n    abandonmentAttempts: 1,           // Any abandonment attempt is significant\n    timeOnCurrentStep: 30000,         // 30s+ on a step suggests confusion\n    rapidClicks: 5,                   // 5+ rapid clicks suggests frustration\n    inactivityPeriods: 2,             // 2+ long pauses suggests hesitation\n    errorCount: 2,                    // 2+ errors suggests guidance needed\n};\n\nconst RAPID_CLICK_WINDOW = 2000;      // 2 second window for rapid clicks\nconst INACTIVITY_THRESHOLD = 15000;   // 15 seconds counts as inactivity\n\n/**\n * Mode escalation logic based on current mode and friction type\n */\nfunction getEscalationSuggestion(\n    currentMode: GoalPathMode,\n    signals: FrictionSignals\n): EscalationSuggestion | null {\n    // Wizard mode friction -> suggest Live Form (more control) or AI Chat (more guidance)\n    if (currentMode === \"wizard\") {\n        // High back navigation suggests need for overview/control\n        if (signals.backNavigationCount >= FRICTION_THRESHOLDS.backNavigationCount) {\n            return {\n                currentMode,\n                suggestedMode: \"live-form\",\n                reason: \"You seem to be going back frequently. Would you prefer a live form where you can see and adjust everything at once?\",\n                confidence: Math.min(signals.backNavigationCount / 4, 1),\n            };\n        }\n\n        // Long time on step + inactivity suggests need for guidance\n        if (\n            signals.timeOnCurrentStep >= FRICTION_THRESHOLDS.timeOnCurrentStep &&\n            signals.inactivityPeriods >= FRICTION_THRESHOLDS.inactivityPeriods\n        ) {\n            return {\n                currentMode,\n                suggestedMode: \"ai-chat\",\n                reason: \"Need help deciding? Try the AI chat mode for personalized guidance.\",\n                confidence: Math.min((signals.inactivityPeriods / 3), 1),\n            };\n        }\n\n        // Multiple errors suggest need for help\n        if (signals.errorCount >= FRICTION_THRESHOLDS.errorCount) {\n            return {\n                currentMode,\n                suggestedMode: \"ai-chat\",\n                reason: \"Having trouble? The AI assistant can help guide you through the process.\",\n                confidence: Math.min(signals.errorCount / 4, 1),\n            };\n        }\n    }\n\n    // Live Form mode friction -> suggest AI Chat (guidance) or Wizard (simpler mental model)\n    if (currentMode === \"live-form\") {\n        // Many rapid clicks or errors suggest overwhelm\n        if (\n            signals.rapidClicks >= FRICTION_THRESHOLDS.rapidClicks ||\n            signals.errorCount >= FRICTION_THRESHOLDS.errorCount\n        ) {\n            return {\n                currentMode,\n                suggestedMode: \"wizard\",\n                reason: \"Too many options? Try the step-by-step wizard for a simpler experience.\",\n                confidence: Math.min((signals.rapidClicks + signals.errorCount) / 8, 1),\n            };\n        }\n\n        // Long inactivity suggests need for guidance\n        if (signals.inactivityPeriods >= FRICTION_THRESHOLDS.inactivityPeriods) {\n            return {\n                currentMode,\n                suggestedMode: \"ai-chat\",\n                reason: \"Not sure what to choose? Let our