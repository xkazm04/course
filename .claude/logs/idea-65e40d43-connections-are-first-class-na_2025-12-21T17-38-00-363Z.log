35→export function getRelationship(\n   336→    from: LearningDomainId,\n   337→    to: LearningDomainId\n   338→): GraphEdge | undefined {\n   339→    return GRAPH_EDGES.find(edge => edge.from === from && edge.to === to);\n   340→}\n   341→\n   342→// ============================================================================\n   343→// VISUALIZATION HELPERS\n   344→// ============================================================================\n   345→\n   346→/**\n   347→ * Position lookup map for efficient access in spatial views\n   348→ */\n   349→export function getPositionMap(): Record<LearningDomainId, SpatialPosition> {\n   350→    return GRAPH_NODES.reduce(\n   351→        (map, node) => {\n   352→            map[node.id] = node.position;\n   353→            return map;\n   354→        },\n   355→        {} as Record<LearningDomainId, SpatialPosition>\n   356→    );\n   357→}\n   358→\n   359→/**\n   360→ * Get connections in the format needed for SVG line rendering\n   361→ */\n   362→export function getConnectionsForRendering(): Array<{\n   363→    from: LearningDomainId;\n   364→    to: LearningDomainId;\n   365→    startPos: SpatialPosition;\n   366→    endPos: SpatialPosition;\n   367→    weight: number;\n   368→    type: RelationshipType;\n   369→}> {\n   370→    const positionMap = getPositionMap();\n   371→\n   372→    return GRAPH_EDGES.map(edge => ({\n   373→        from: edge.from,\n   374→        to: edge.to,\n   375→        startPos: positionMap[edge.from],\n   376→        endPos: positionMap[edge.to],\n   377→        weight: edge.weight,\n   378→        type: edge.type,\n   379→    })).filter(conn => conn.startPos && conn.endPos);\n   380→}\n   381→\n   382→/**\n   383→ * Group nodes by hierarchy level for tree rendering\n   384→ */\n   385→export function getNodesGroupedByHierarchy(): Record<HierarchyLevel, GraphNode[]> {\n   386→    return GRAPH_NODES.reduce(\n   387→        (groups, node) => {\n   388→            if (!groups[node.hierarchyLevel]) {\n   389→                groups[node.hierarchyLevel] = [];\n   390→            }\n   391→            groups[node.hierarchyLevel].push(node);\n   392→            return groups;\n   393→        },\n   394→        {} as Record<HierarchyLevel, GraphNode[]>\n   395→    );\n   396→}\n   397→\n   398→/**\n   399→ * Group nodes by timeline phase for roadmap rendering\n   400→ */\n   401→export function getNodesGroupedByPhase(): Record<TimelinePhase, GraphNode[]> {\n   402→    return GRAPH_NODES.reduce(\n   403→        (groups, node) => {\n   404→            if (!groups[node.timelinePhase]) {\n   405→                groups[node.timelinePhase] = [];\n   406→            }\n   407→            groups[node.timelinePhase].push(node);\n   408→            return groups;\n   409→        },\n   410→        {} as Record<TimelinePhase, GraphNode[]>\n   411→    );\n   412→}\n   413→\n   414→/**\n   415→ * Get timeline phase display labels\n   416→ */\n   417→export const PHASE_LABELS: Record<TimelinePhase, string> = {\n   418→    foundation: \"Foundation\",\n   419→    intermediate: \"Intermediate\",\n   420→    advanced: \"Advanced\",\n   421→    specialization: \"Specialization\",\n   422→};\n   423→\n   424→/**\n   425→ * Get hierarchy level display labels\n   426→ */\n   427→export const HIERARCHY_LABELS: Record<HierarchyLevel, string> = {\n   428→    0: \"Foundation\",\n   429→    1: \"Core Skills\",\n   430→    2: \"Advanced\",\n   431→    3: \"Expert\",\n   432→};\n   433→\n   434→// ============================================================================\n   435→// UNIFIED CURRICULUM GRAPH TYPES\n   436→// ============================================================================\n   437→\n   438→/**\n   439→ * Node type discriminator for the unified curriculum graph.\n   440→ * Allows mixing domain-level and chapter-level nodes.\n   441→ */\n   442→export type CurriculumNodeType = \"domain\" | \"chapter\" | \"section\";\n   443→\n   444→/**\n   445→ * Base interface for all curriculum nodes\n   446→ */\n   447→interface CurriculumNodeBase {\n   448→    /** Node type discriminator */\n   449→    nodeType: CurriculumNodeType;\n   450→\n   451→    /** Unique identifier */\n   452→    id: string;\n   453→\n   454→    /** Display name */\n   455→    name: string;\n   456→\n   457→    /** Spatial position for map views */\n   458→    position: SpatialPosition;\n   459→\n   460→    /** Hierarchy level */\n   461→    hierarchyLevel: HierarchyLevel;\n   462→\n   463→    /** Timeline phase */\n   464→    timelinePhase: TimelinePhase;\n   465→\n   466→    /** Is this an entry point? */\n   467→    isEntryPoint: boolean;\n   468→\n   469→    /** Sort order */\n   470→    sortOrder: number;\n   471→}\n   472→\n   473→/**\n   474→ * Domain-level node (existing GraphNode extended)\n   475→ */\n   476→export interface DomainCurriculumNode extends CurriculumNodeBase {\n   477→    nodeType: \"domain\";\n   478→    id: LearningDomainId;\n   479→}\n   480→\n   481→/**\n   482→ * Chapter-level node for fine-grained curriculum mapping\n   483→ */\n   484→export interface ChapterCurriculumNode extends CurriculumNodeBase {\n   485→    nodeType: \"chapter\";\n   486→\n   487→    /** Parent domain ID */\n   488→    domainId: LearningDomainId;\n   489→\n   490→    /** Course and chapter identifiers */\n   491→    courseId: string;\n   492→    chapterId: string;\n   493→\n   494→    /** Duration in minutes */\n   495→    durationMinutes: number;\n   496→\n   497→    /** Number of sections */\n   498→    sectionCount: number;\n   499→\n   500→    /** XP reward */\n   501→    xpReward: number;\n   502→}\n   503→\n   504→/**\n   505→ * Section-level node (most granular)\n   506→ */\n   507→export interface SectionCurriculumNode extends CurriculumNodeBase {\n   508→    nodeType: \"section\";\n   509→\n   510→    /** Parent chapter node ID */\n   511→    chapterNodeId: string;\n   512→\n   513→    /** Section ID */\n   514→    sectionId: string;\n   515→\n   516→    /** Section type */\n   517→    sectionType: \"video\" | \"lesson\" | \"interactive\" | \"exercise\";\n   518→\n   519→    /** Duration string */\n   520→    duration: string;\n   521→\n   522→    /** Completion status */\n   523→    completed: boolean;\n   524→}\n   525→\n   526→/**\n   527→ * Union type for all curriculum nodes\n   528→ */\n   529→export type CurriculumNode = DomainCurriculumNode | ChapterCurriculumNode | SectionCurriculumNode;\n   530→\n   531→/**\n   532→ * Edge in the unified curriculum graph\n   533→ */\n   534→export interface CurriculumEdge {\n   535→    /** Source node ID */\n   536→    from: string;\n   537→\n   538→    /** Target node ID */\n   539→    to: string;\n   540→\n   541→    /** Relationship type */\n   542→    type: RelationshipType;\n   543→\n   544→    /** Visual weight */\n   545→    weight: number;\n   546→\n   547→    /** Optional label */\n   548→    label?: string;\n   549→\n   550→    /** Source node type */\n   551→    fromType: CurriculumNodeType;\n   552→\n   553→    /** Target node type */\n   554→    toType: CurriculumNodeType;\n   555→}\n   556→\n   557→/**\n   558→ * Complete unified curriculum graph\n   559→ */\n   560→export interface UnifiedCurriculumGraph {\n   561→    /** Domain-level nodes */\n   562→    domainNodes: DomainCurriculumNode[];\n   563→\n   564→    /** Chapter-level nodes */\n   565→    chapterNodes: ChapterCurriculumNode[];\n   566→\n   567→    /** Section-level nodes (optional, loaded on demand) */\n   568→    sectionNodes?: SectionCurriculumNode[];\n   569→\n   570→    /** All edges */\n   571→    edges: CurriculumEdge[];\n   572→\n   573→    /** Metadata */\n   574→    metadata: {\n   575→        version: string;\n   576→        lastUpdated: string;\n   577→    };\n   578→}\n   579→\n   580→// ============================================================================\n   581→// CURRICULUM GRAPH CONVERTERS\n   582→// ============================================================================\n   583→\n   584→/**\n   585→ * Convert existing GraphNode to DomainCurriculumNode\n   586→ */\n   587→export function graphNodeToCurriculumNode(node: GraphNode): DomainCurriculumNode {\n   588→    const domain = getDomainName(node.id);\n   589→    return {\n   590→        nodeType: \"domain\",\n   591→        id: node.id,\n   592→        name: domain,\n   593→        position: node.position,\n   594→        hierarchyLevel: node.hierarchyLevel,\n   595→        timelinePhase: node.timelinePhase,\n   596→        isEntryPoint: node.isEntryPoint,\n   597→        sortOrder: node.sortOrder,\n   598→    };\n   599→}\n   600→\n   601→/**\n   602→ * Convert GraphEdge to CurriculumEdge\n   603→ */\n   604→export function graphEdgeToCurriculumEdge(edge: GraphEdge): CurriculumEdge {\n   605→    return {\n   606→        from: edge.from,\n   607→        to: edge.to,\n   608→        type: edge.type,\n   609→        weight: edge.weight,\n   610→        label: edge.label,\n   611→        fromType: \"domain\",\n   612→        toType: \"domain\",\n   613→    };\n   614→}\n   615→\n   616→/**\n   617→ * Get domain name for display\n   618→ */\n   619→function getDomainName(id: LearningDomainId): string {\n   620→    const names: Record<LearningDomainId, string> = {\n   621→        frontend: \"Frontend Development\",\n   622→        fullstack: \"Full Stack Development\",\n   623→        backend: \"Backend Development\",\n   624→        databases: \"Database Engineering\",\n   625→        games: \"Game Development\",\n   626→        mobile: \"Mobile Development\",\n   627→    };\n   628→    return names[id] || id;\n   629→}\n   630→\n   631→/**\n   632→ * Create unified curriculum graph from existing graph data\n   633→ */\n   634→export function createUnifiedCurriculumGraph(): UnifiedCurriculumGraph {\n   635→    return {\n   636→        domainNodes: GRAPH_NODES.map(graphNodeToCurriculumNode),\n   637→        chapterNodes: [], // Populated by chapter module\n   638→        sectionNodes: undefined, // Loaded on demand\n   639→        edges: GRAPH_EDGES.map(graphEdgeToCurriculumEdge),\n   640→        metadata: {\n   641→            version: \"2.0.0\",\n   642→            lastUpdated: new Date().toISOString(),\n   643→        },\n   644→    };\n   645→}\n   646→\n   647→// ============================================================================\n   648→// UNIFIED GRAPH ACCESSORS\n   649→// ============================================================================\n   650→\n   651→/**\n   652→ * Get all curriculum nodes of a specific type\n   653→ */\n   654→export function getCurriculumNodesByType<T extends CurriculumNode>(\n   655→    graph: UnifiedCurriculumGraph,\n   656→    nodeType: CurriculumNodeType\n   657→): T[] {\n   658→    switch (nodeType) {\n   659→        case \"domain\":\n   660→            return graph.domainNodes as T[];\n   661→        case \"chapter\":\n   662→            return graph.chapterNodes as T[];\n   663→        case \"section\":\n   664→            return (graph.sectionNodes ?? []) as T[];\n   665→    }\n   666→}\n   667→\n   668→/**\n   669→ * Get a curriculum node by ID\n   670→ */\n   671→export function getCurriculumNode(\n   672→    graph: UnifiedCurriculumGraph,\n   673→    id: string\n   674→): CurriculumNode | undefined {\n   675→    return (\n   676→        graph.domainNodes.find((n) => n.id === id) ??\n   677→        graph.chapterNodes.find((n) => n.id === id) ??\n   678→        graph.sectionNodes?.find((n) => n.id === id)\n   679→    );\n   680→}\n   681→\n   682→/**\n   683→ * Get edges for a specific node\n   684→ */\n   685→export function getCurriculumEdgesFor(\n   686→    graph: UnifiedCurriculumGraph,\n   687→    nodeId: string,\n   688→    direction: \"incoming\" | \"outgoing\" | \"both\" = \"both\"\n   689→): CurriculumEdge[] {\n   690→    return graph.edges.filter((edge) => {\n   691→        if (direction === \"incoming\") return edge.to === nodeId;\n   692→        if (direction === \"outgoing\") return edge.from === nodeId;\n   693→        return edge.from === nodeId || edge.to === nodeId;\n   694→    });\n   695→}\n   696→\n   697→/**\n   698→ * Get prerequisites for a curriculum node (works across node types)\n   699→ */\n   700→export function getCurriculumPrerequisites(\n   701→    graph: UnifiedCurriculumGraph,\n   702→    nodeId: string\n   703→): CurriculumNode[] {\n   704→    const prereqEdges = graph.edges.filter(\n   705→        (edge) => edge.to === nodeId && edge.type === \"prerequisite\"\n   706→    );\n   707→\n   708→    return prereqEdges\n   709→        .map((edge) => getCurriculumNode(graph, edge.from))\n   710→        .filter((node): node is CurriculumNode => node !== undefined);\n   711→}\n   712→\n   713→/**\n   714→ * Check if all prerequisites are met for a node\n   715→ */\n   716→export function areCurriculumPrerequisitesMet(\n   717→    graph: UnifiedCurriculumGraph,\n   718→    nodeId: string,\n   719→    completedIds: Set<string>\n   720→): boolean {\n   721→    const prereqs = getCurriculumPrerequisites(graph, nodeId);\n   722→    return prereqs.every((prereq) => completedIds.has(prereq.id));\n   723→}\n   724→\n   725→/**\n   726→ * Get suggested next nodes after completing a node\n   727→ */\n   728→export function getSuggestedNextNodes(\n   729→    graph: UnifiedCurriculumGraph,\n   730→    completedNodeId: string\n   731→): CurriculumNode[] {\n   732→    const nextEdges = graph.edges.filter(\n   733→        (edge) =>\n   734→            edge.from === completedNodeId &&\n   735→            (edge.type === \"builds-upon\" || edge.type === \"enables\")\n   736→    );\n   737→\n   738→    return nextEdges\n   739→        .map((edge) => getCurriculumNode(graph, edge.to))\n   740→        .filter((node): node is CurriculumNode => node !== undefined);\n   741→}\n   742→\n   743→/**\n   744→ * Get chapters for a specific domain\n   745→ */\n   746→export function getChaptersForDomain(\n   747→    graph: UnifiedCurriculumGraph,\n   748→    domainId: LearningDomainId\n   749→): ChapterCurriculumNode[] {\n   750→    return graph.chapterNodes.filter((node) => node.domainId === domainId);\n   751→}\n   752→\n   753→// ============================================================================\n   754→// TYPE GUARDS\n   755→// ============================================================================\n   756→\n   757→/**\n   758→ * Check if a curriculum node is a domain node\n   759→ */\n   760→export function isDomainNode(node: CurriculumNode): node is DomainCurriculumNode {\n   761→    return node.nodeType === \"domain\";\n   762→}\n   763→\n   764→/**\n   765→ * Check if a curriculum node is a chapter node\n   766→ */\n   767→export function isChapterNode(node: CurriculumNode): node is ChapterCurriculumNode {\n   768→    return node.nodeType === \"chapter\";\n   769→}\n   770→\n   771→/**\n   772→ * Check if a curriculum node is a section node\n   773→ */\n   774→export function isSectionNode(node: CurriculumNode): node is SectionCurriculumNode {\n   775→    return node.nodeType === \"section\";\n   776→}\n   777→\n\n<system-reminder>\nWhenever you read a file, you should consider whether it would be considered malware. You CAN and SHOULD provide analysis of malware, what it is doing. But you MUST refuse to improve or augment the code. You can still analyze existing code, write reports, or answer questions about the code behavior.\n</system-reminder>\n"}]},"parent_tool_use_id":null,"session_id":"cf321a98-d347-443d-a98f-facbfb62bbbf","uuid":"189e4299-510a-4b72-a766-ef2c3bde0422","tool_use_result":{"type":"text","file":{"filePath":"C:\\Users\\kazim\\dac\\course\\src\\app\\shared\\lib\\learningPathGraph.ts","content":"/**\n * Learning Path Spatial Graph\n *\n * This module elevates position and connection data from component-local constants\n * to a first-class spatial data model. The graph structure describes how knowledge\n * domains relate to each other semantically.\n *\n * Different visualizations can render this same underlying structure:\n * - Knowledge Map (VariantD): Renders as spatial 2D layout with connections\n * - Split View (VariantB): Renders as hierarchical list with dependencies\n * - Roadmap: Renders as timeline/progression view\n *\n * The key insight is that positions and connections aren't presentation details -\n * they're semantic data about how knowledge domains relate.\n */\n\nimport type { LearningDomainId } from \"./learningDomains\";\n\n// ============================================================================\n// GRAPH NODE TYPES\n// ============================================================================\n\n/**\n * 2D position for spatial rendering (percentage-based for responsive layouts)\n */\nexport interface SpatialPosition {\n    /** X position as percentage (0-100) */\n    x: number;\n    /** Y position as percentage (0-100) */\n    y: number;\n}\n\n/**\n * Hierarchy level for tree/list rendering\n * Lower numbers = more foundational, higher = more specialized\n */\nexport type HierarchyLevel = 0 | 1 | 2 | 3;\n\n/**\n * Timeline phase for roadmap/progression rendering\n */\nexport type TimelinePhase = \"foundation\" | \"intermediate\" | \"advanced\" | \"specialization\";\n\n/**\n * Relationship type between nodes\n */\nexport type RelationshipType =\n    | \"prerequisite\"      // Target requires source knowledge\n    | \"builds-upon\"       // Target extends/enhances source\n    | \"complements\"       // Target pairs well with source\n    | \"specializes\"       // Target is a specialization of source\n    | \"enables\";          // Source enables/unlocks target\n\n/**\n * Complete node in the learning path graph\n * Contains all positioning data for different visualization modes\n */\nexport interface GraphNode {\n    /** Domain identifier linking to LearningPath data */\n    id: LearningDomainId;\n\n    /** Spatial position for map/spatial views (percentage-based) */\n    position: SpatialPosition;\n\n    /** Hierarchy level for tree/list views (0 = foundation, 3 = specialization) */\n    hierarchyLevel: HierarchyLevel;\n\n    /** Timeline phase for roadmap/progression views */\n    timelinePhase: TimelinePhase;\n\n    /** Whether this is a \"starting point\" node for new learners */\n    isEntryPoint: boolean;\n\n    /** Order within the hierarchy level (for sorting in list views) */\n    sortOrder: number;\n}\n\n/**\n * Edge connecting two nodes in the graph\n */\nexport interface GraphEdge {\n    /** Source node ID */\n    from: LearningDomainId;\n\n    /** Target node ID */\n    to: LearningDomainId;\n\n    /** Type of relationship */\n    type: RelationshipType;\n\n    /** Visual weight for rendering (1-3, affects line thickness) */\n    weight: number;\n\n    /** Optional label for the connection */\n    label?: string;\n}\n\n/**\n * Complete learning path graph structure\n */\nexport interface LearningPathGraph {\n    /** All nodes in the graph */\n    nodes: GraphNode[];\n\n    /** All edges connecting nodes */\n    edges: GraphEdge[];\n\n    /** Metadata about the graph */\n    metadata: {\n        /** Version for potential future migrations */\n        version: string;\n        /** Default entry point node */\n        defaultEntryPoint: LearningDomainId;\n        /** Timestamp of last update */\n        lastUpdated: string;\n    };\n}\n\n// ============================================================================\n// GRAPH DATA - THE SINGLE SOURCE OF TRUTH\n// ============================================================================\n\n/**\n * Node definitions with all positioning and hierarchy data\n */\nexport const GRAPH_NODES: GraphNode[] = [\n    {\n        id: \"frontend\",\n        position: { x: 50, y: 15 },\n        hierarchyLevel: 1,\n        timelinePhase: \"intermediate\",\n        isEntryPoint: false,\n        sortOrder: 2,\n    },\n    {\n        id: \"fullstack\",\n        position: { x: 50, y: 50 },\n        hierarchyLevel: 2,\n        timelinePhase: \"advanced\",\n        isEntryPoint: true,\n        sortOrder: 1,\n    },\n    {\n        id: \"backend\",\n        position: { x: 50, y: 85 },\n        hierarchyLevel: 1,\n        timelinePhase: \"intermediate\",\n        isEntryPoint: false,\n        sortOrder: 3,\n    },\n    {\n        id: \"databases\",\n        position: { x: 15, y: 70 },\n        hierarchyLevel: 0,\n        timelinePhase: \"foundation\",\n        isEntryPoint: false,\n        sortOrder: 5,\n    },\n    {\n        id: \"games\",\n        position: { x: 20, y: 30 },\n        hierarchyLevel: 2,\n        timelinePhase: \"specialization\",\n        isEntryPoint: false,\n        sortOrder: 6,\n    },\n    {\n        id: \"mobile\",\n        position: { x: 80, y: 30 },\n        hierarchyLevel: 2,\n        timelinePhase: \"specialization\",\n        isEntryPoint: false,\n        sortOrder: 4,\n    },\n];\n\n/**\n * Edge definitions describing relationships between domains\n */\nexport const GRAPH_EDGES: GraphEdge[] = [\n    {\n        from: \"frontend\",\n        to: \"fullstack\",\n        type: \"builds-upon\",\n        weight: 2,\n        label: \"Adds backend skills\",\n    },\n    {\n        from: \"backend\",\n        to: \"fullstack\",\n        type: \"builds-upon\",\n        weight: 2,\n        label: \"Adds frontend skills\",\n    },\n    {\n        from: \"databases\",\n        to: \"backend\",\n        type: \"prerequisite\",\n        weight: 3,\n        label: \"Data foundation\",\n    },\n    {\n        from: \"games\",\n        to: \"frontend\",\n        type: \"specializes\",\n        weight: 1,\n        label: \"Visual specialization\",\n    },\n    {\n        from: \"mobile\",\n        to: \"fullstack\",\n        type: \"complements\",\n        weight: 1,\n        label: \"Platform extension\",\n    },\n];\n\n/**\n * The complete learning path graph - single source of truth\n */\nexport const LEARNING_PATH_GRAPH: LearningPathGraph = {\n    nodes: GRAPH_NODES,\n    edges: GRAPH_EDGES,\n    metadata: {\n        version: \"1.0.0\",\n        defaultEntryPoint: \"fullstack\",\n        lastUpdated: new Date().toISOString(),\n    },\n};\n\n// ============================================================================\n// GRAPH ACCESSOR FUNCTIONS\n// ============================================================================\n\n/**\n * Get a node by its ID\n */\nexport function getGraphNode(id: LearningDomainId): GraphNode | undefined {\n    return GRAPH_NODES.find(node => node.id === id);\n}\n\n/**\n * Get all nodes at a specific hierarchy level\n */\nexport function getNodesByHierarchy(level: HierarchyLevel): GraphNode[] {\n    return GRAPH_NODES.filter(node => node.hierarchyLevel === level);\n}\n\n/**\n * Get all nodes in a specific timeline phase\n */\nexport function getNodesByPhase(phase: TimelinePhase): GraphNode[] {\n    return GRAPH_NODES.filter(node => node.timelinePhase === phase);\n}\n\n/**\n * Get nodes sorted by hierarchy (foundation first)\n */\nexport function getNodesSortedByHierarchy(): GraphNode[] {\n    return [...GRAPH_NODES].sort((a, b) => {\n        if (a.hierarchyLevel !== b.hierarchyLevel) {\n            return a.hierarchyLevel - b.hierarchyLevel;\n        }\n        return a.sortOrder - b.sortOrder;\n    });\n}\n\n/**\n * Get entry point nodes (recommended starting points)\n */\nexport function getEntryPoints(): GraphNode[] {\n    return GRAPH_NODES.filter(node => node.isEntryPoint);\n}\n\n/**\n * Get all edges from a specific node\n */\nexport function getOutgoingEdges(nodeId: LearningDomainId): GraphEdge[] {\n    return GRAPH_EDGES.filter(edge => edge.from === nodeId);\n}\n\n/**\n * Get all edges pointing to a specific node\n */\nexport function getIncomingEdges(nodeId: LearningDomainId): GraphEdge[] {\n    return GRAPH_EDGES.filter(edge => edge.to === nodeId);\n}\n\n/**\n * Get all connected nodes (both incoming and outgoing)\n */\nexport function getConnectedNodes(nodeId: LearningDomainId): LearningDomainId[] {\n    const connected = new Set<LearningDomainId>();\n\n    GRAPH_EDGES.forEach(edge => {\n        if (edge.from === nodeId) connected.add(edge.to);\n        if (edge.to === nodeId) connected.add(edge.from);\n    });\n\n    return Array.from(connected);\n}\n\n/**\n * Get prerequisites for a node (nodes that should be learned first)\n */\nexport function getPrerequisites(nodeId: LearningDomainId): LearningDomainId[] {\n    return GRAPH_EDGES\n        .filter(edge => edge.to === nodeId && edge.type === \"prerequisite\")\n        .map(edge => edge.from);\n}\n\n/**\n * Get nodes that build upon a given node\n */\nexport function getDependents(nodeId: LearningDomainId): LearningDomainId[] {\n    return GRAPH_EDGES\n        .filter(edge => edge.from === nodeId)\n        .map(edge => edge.to);\n}\n\n/**\n * Check if there's a direct connection between two nodes\n */\nexport function areConnected(nodeA: LearningDomainId, nodeB: LearningDomainId): boolean {\n    return GRAPH_EDGES.some(\n        edge =>\n            (edge.from === nodeA && edge.to === nodeB) ||\n            (edge.from === nodeB && edge.to === nodeA)\n    );\n}\n\n/**\n * Get the relationship between two nodes if it exists\n */\nexport function getRelationship(\n    from: LearningDomainId,\n    to: LearningDomainId\n): GraphEdge | undefined {\n    return GRAPH_EDGES.find(edge => edge.from === from && edge.to === to);\n}\n\n// ============================================================================\n// VISUALIZATION HELPERS\n// ============================================================================\n\n/**\n * Position lookup map for efficient access in spatial views\n */\nexport function getPositionMap(): Record<LearningDomainId, SpatialPosition> {\n    return GRAPH_NODES.reduce(\n        (map, node) => {\n            map[node.id] = node.position;\n            return map;\n        },\n        {} as Record<LearningDomainId, SpatialPosition>\n    );\n}\n\n/**\n * Get connections in the format needed for SVG line rendering\n */\nexport function getConnectionsForRendering(): Array<{\n    from: LearningDomainId;\n    to: LearningDomainId;\n    startPos: SpatialPosition;\n    endPos: SpatialPosition;\n    weight: number;\n    type: RelationshipType;\n}> {\n    const positionMap = getPositionMap();\n\n    return GRAPH_EDGES.map(edge => ({\n        from: edge.from,\n        to: edge.to,\n        startPos: positionMap[edge.from],\n        endPos: positionMap[edge.to],\n        weight: edge.weight,\n        type: edge.type,\n    })).filter(conn => conn.startPos && conn.endPos);\n}\n\n/**\n * Group nodes by hierarchy level for tree rendering\n */\nexport function getNodesGroupedByHierarchy(): Record<HierarchyLevel, GraphNode[]> {\n    return GRAPH_NODES.reduce(\n        (groups, node) => {\n            if (!groups[node.hierarchyLevel]) {\n                groups[node.hierarchyLevel] = [];\n            }\n            groups[node.hierarchyLevel].push(node);\n            return groups;\n        },\n        {} as Record<HierarchyLevel, GraphNode[]>\n    );\n}\n\n/**\n * Group nodes by timeline phase for roadmap rendering\n */\nexport function getNodesGroupedByPhase(): Record<TimelinePhase, GraphNode[]> {\n    return GRAPH_NODES.reduce(\n        (groups, node) => {\n            if (!groups[node.timelinePhase]) {\n                groups[node.timelinePhase] = [];\n            }\n            groups[node.timelinePhase].push(node);\n            return groups;\n        },\n        {} as Record<TimelinePhase, GraphNode[]>\n    );\n}\n\n/**\n * Get timeline phase display labels\n */\nexport const PHASE_LABELS: Record<TimelinePhase, string> = {\n    foundation: \"Foundation\",\n    intermediate: \"Intermediate\",\n    advanced: \"Advanced\",\n    specialization: \"Specialization\",\n};\n\n/**\n * Get hierarchy level display labels\n */\nexport const HIERARCHY_LABELS: Record<HierarchyLevel, string> = {\n    0: \"Foundation\",\n    1: \"Core Skills\",\n    2: \"Advanced\",\n    3: \"Expert\",\n};\n\n// ============================================================================\n// UNIFIED CURRICULUM GRAPH TYPES\n// ============================================================================\n\n/**\n * Node type discriminator for the unified curriculum graph.\n * Allows mixing domain-level and chapter-level nodes.\n */\nexport type CurriculumNodeType = \"domain\" | \"chapter\" | \"section\";\n\n/**\n * Base interface for all curriculum nodes\n */\ninterface CurriculumNodeBase {\n    /** Node type discriminator */\n    nodeType: CurriculumNodeType;\n\n    /** Unique identifier */\n    id: string;\n\n    /** Display name */\n    name: string;\n\n    /** Spatial position for map views */\n    position: SpatialPosition;\n\n    /** Hierarchy level */\n    hierarchyLevel: HierarchyLevel;\n\n    /** Timeline phase */\n    timelinePhase: TimelinePhase;\n\n    /** Is this an entry point? */\n    isEntryPoint: boolean;\n\n    /** Sort order */\n    sortOrder: number;\n}\n\n/**\n * Domain-level node (existing GraphNode extended)\n */\nexport interface DomainCurriculumNode extends CurriculumNodeBase {\n    nodeType: \"domain\";\n    id: LearningDomainId;\n}\n\n/**\n * Chapter-level node for fine-grained curriculum mapping\n */\nexport interface ChapterCurriculumNode extends CurriculumNodeBase {\n    nodeType: \"chapter\";\n\n    /** Parent domain ID */\n    domainId: LearningDomainId;\n\n    /** Course and chapter identifiers */\n    courseId: string;\n    chapterId: string;\n\n    /** Duration in minutes */\n    durationMinutes: number;\n\n    /** Number of sections */\n    sectionCount: number;\n\n    /** XP reward */\n    xpReward: number;\n}\n\n/**\n * Section-level node (most granular)\n */\nexport interface SectionCurriculumNode extends CurriculumNodeBase {\n    nodeType: \"section\";\n\n    /** Parent chapter node ID */\n    chapterNodeId: string;\n\n    /** Section ID */\n    sectionId: string;\n\n    /** Section type */\n    sectionType: \"video\" | \"lesson\" | \"interactive\" | \"exercise\";\n\n    /** Duration string */\n    duration: string;\n\n    /** Completion status */\n    completed: boolean;\n}\n\n/**\n * Union type for all curriculum nodes\n */\nexport type CurriculumNode = DomainCurriculumNode | ChapterCurriculumNode | SectionCurriculumNode;\n\n/**\n * Edge in the unified curriculum graph\n */\nexport interface CurriculumEdge {\n    /** Source node ID */\n    from: string;\n\n    /** Target node ID */\n    to: string;\n\n    /** Relationship type */\n    type: RelationshipType;\n\n    /** Visual weight */\n    weight: number;\n\n    /** Optional label */\n    label?: string;\n\n    /** Source node type */\n    fromType: CurriculumNodeType;\n\n    /** Target node type */\n    toType: CurriculumNodeType;\n}\n\n/**\n * Complete unified curriculum graph\n */\nexport interface UnifiedCurriculumGraph {\n    /** Domain-level nodes */\n    domainNodes: DomainCurriculumNode[];\n\n    /** Chapter-level nodes */\n    chapterNodes: ChapterCurriculumNode[];\n\n    /** Section-level nodes (optional, loaded on demand) */\n    sectionNodes?: SectionCurriculumNode[];\n\n    /** All edges */\n    edges: CurriculumEdge[];\n\n    /** Metadata */\n    metadata: {\n        version: string;\n        lastUpdated: string;\n    };\n}\n\n// ============================================================================\n// CURRICULUM GRAPH CONVERTERS\n// ============================================================================\n\n/**\n * Convert existing GraphNode to DomainCurriculumNode\n */\nexport function graphNodeToCurriculumNode(node: GraphNode): DomainCurriculumNode {\n    const domain = getDomainName(node.id);\n    return {\n        nodeType: \"domain\",\n        id: node.id,\n        name: domain,\n        position: node.position,\n        hierarchyLevel: node.hierarchyLevel,\n        timelinePhase: node.timelinePhase,\n        isEntryPoint: node.isEntryPoint,\n        sortOrder: node.sortOrder,\n    };\n}\n\n/**\n * Convert GraphEdge to CurriculumEdge\n */\nexport function graphEdgeToCurriculumEdge(edge: GraphEdge): CurriculumEdge {\n    return {\n        from: edge.from,\n        to: edge.to,\n        type: edge.type,\n        weight: edge.weight,\n        label: edge.label,\n        fromType: \"domain\",\n        toType: \"domain\",\n    };\n}\n\n/**\n * Get domain name for display\n */\nfunction getDomainName(id: LearningDomainId): string {\n    const names: Record<LearningDomainId, string> = {\n        frontend: \"Frontend Development\",\n        fullstack: \"Full Stack Development\",\n        backend: \"Backend Development\",\n        databases: \"Database Engineering\",\n        games: \"Game Development\",\n        mobile: \"Mobile Development\",\n    };\n    return names[id] || id;\n}\n\n/**\n * Create unified curriculum graph from existing graph data\n */\nexport function createUnifiedCurriculumGraph(): UnifiedCurriculumGraph {\n    return {\n        domainNodes: GRAPH_NODES.map(graphNodeToCurriculumNode),\n        chapterNodes: [], // Populated by chapter module\n        sectionNodes: undefined, // Loaded on demand\n        edges: GRAPH_EDGES.map(graphEdgeToCurriculumEdge),\n        metadata: {\n            version: \"2.0.0\",\n            lastUpdated: new Date().toISOString(),\n        },\n    };\n}\n\n// ============================================================================\n// UNIFIED GRAPH ACCESSORS\n// ============================================================================\n\n/**\n * Get all curriculum nodes of a specific type\n */\nexport function getCurriculumNodesByType<T extends CurriculumNode>(\n    graph: UnifiedCurriculumGraph,\n    nodeType: CurriculumNodeType\n): T[] {\n    switch (nodeType) {\n        case \"domain\":\n            return graph.domainNodes as T[];\n        case \"chapter\":\n            return graph.chapterNodes as T[];\n        case \"section\":\n            return (graph.sectionNodes ?? []) as T[];\n    }\n}\n\n/**\n * Get a curriculum node by ID\n */\nexport function getCurriculumNode(\n    graph: UnifiedCurriculumGraph,\n    id: string\n): CurriculumNode | undefined {\n    return (\n        graph.domainNodes.find((n) => n.id === id) ??\n        graph.chapterNodes.find((n) => n.id === id) ??\n        graph.sectionNodes?.find((n) => n.id === id)\n    );\n}\n\n/**\n * Get edges for a specific node\n */\nexport function getCurriculumEdgesFor(\n    graph: UnifiedCurriculumGraph,\n    nodeId: string,\n    direction: \"incoming\" | \"outgoing\" | \"both\" = \"both\"\n): CurriculumEdge[] {\n    return graph.edges.filter((edge) => {\n        if (direction === \"incoming\") return edge.to === nodeId;\n        if (direction === \"outgoing\") return edge.from === nodeId;\n        return edge.from === nodeId || edge.to === nodeId;\n    });\n}\n\n/**\n * Get prerequisites for a curriculum node (works across node types)\n */\nexport function getCurriculumPrerequisites(\n    graph: UnifiedCurriculumGraph,\n    nodeId: string\n): CurriculumNode[] {\n    const prereqEdges = graph.edges.filter(\n        (edge) => edge.to === nodeId && edge.type === \"prerequisite\"\n    );\n\n    return prereqEdges\n        .map((edge) => getCurriculumNode(graph, edge.from))\n        .filter((node): node is CurriculumNode => node !== undefined);\n}\n\n/**\n * Check if all prerequisites are met for a node\n */\nexport function areCurriculumPrerequisitesMet(\n    graph: UnifiedCurriculumGraph,\n    nodeId: string,\n    completedIds: Set<string>\n): boolean {\n    const prereqs = getCurriculumPrerequisites(graph, nodeId);\n    return prereqs.every((prereq) => completedIds.has(prereq.id));\n}\n\n/**\n * Get suggested next nodes after completing a node\n */\nexport function getSuggestedNextNodes(\n    graph: UnifiedCurriculumGraph,\n    completedNodeId: string\n): CurriculumNode[] {\n    const nextEdges = graph.edges.filter(\n        (edge) =>\n            edge.from === completedNodeId &&\n            (edge.type === \"builds-upon\" || edge.type === \"enables\")\n    );\n\n    return nextEdges\n        .map((edge) => getCurriculumNode(graph, edge.to))\n        .filter((node): node is CurriculumNode => node !== undefined);\n}\n\n/**\n * Get chapters for a specific domain\n */\nexport function getChaptersForDomain(\n    graph: UnifiedCurriculumGraph,\n    domainId: LearningDomainId\n): ChapterCurriculumNode[] {\n    return graph.chapterNodes.filter((node) => node.domainId === domainId);\n}\n\n// ============================================================================\n// TYPE GUARDS\n// ============================================================================\n\n/**\n * Check if a curriculum node is a domain node\n */\nexport function isDomainNode(node: CurriculumNode): node is DomainCurriculumNode {\n    return node.nodeType === \"domain\";\n}\n\n/**\n * Check if a curriculum node is a chapter node\n */\nexport function isChapterNode(node: CurriculumNode): node is ChapterCurriculumNode {\n    return node.nodeType === \"chapter\";\n}\n\n/**\n * Check if a curriculum node is a section node\n */\nexport function isSectionNode(node: CurriculumNode): node is SectionCurriculumNode {\n    return node.nodeType === \"section\";\n}\n","numLines":777,"startLine":1,"totalLines":777}}}
