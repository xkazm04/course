ss, Target, Briefcase, BookOpen, Layers, ArrowRight, ArrowLeft,\n    Sparkles, Cpu, Globe, Hexagon, Zap, GraduationCap, Home, FileText\n} from \"lucide-react\";\nimport { cn } from \"@/app/shared/lib/utils\";\nimport { BackgroundAtmosphere, VariantTabs, Breadcrumb, BreadcrumbItem } from \"@/app/shared/components\";\nimport { ThemeToggle } from \"@/app/features/theme\";\n\n// Feature imports\nimport {\n    VariantA as OverviewA, VariantB as OverviewB,\n    VariantC as OverviewC, VariantD as OverviewD,\n    VariantE as OverviewE, VariantF as OverviewF\n} from \"@/app/features/overview\";\nimport {\n    VariantA as CareerA, VariantB as CareerB,\n    VariantC as CareerC, VariantD as CareerD\n} from \"@/app/features/career-mapping\";\nimport {\n    VariantA as ChapterA, VariantB as ChapterB,\n    VariantC as ChapterC, VariantD as ChapterD\n} from \"@/app/features/chapter\";\nimport {\n    VariantA as GoalA, VariantB as GoalB,\n    VariantC as GoalC, VariantD as GoalD\n} from \"@/app/features/goal-path\";\nimport {\n    VariantA as LandingA, VariantB as LandingB,\n    VariantC as LandingC, VariantD as LandingD\n} from \"@/app/features/landing\";\nimport { MyNotesPage } from \"@/app/features/bookmarks\";\n\n// Progress tracking imports\nimport {\n    useProgressOverview,\n    ContinueLearningSection,\n    ProgressExportModal,\n    ProgressBar,\n    ContinueLearningItem,\n    exportProgressData,\n    downloadProgressData,\n    importProgressData,\n} from \"@/app/features/progress\";\n\n// Module definitions\nconst modules = [\n    {\n        id: \"landing\",\n        title: \"Landing Page\",\n        subtitle: \"Platform Showcase\",\n        description: \"Spatial-style showcase summarizing all platform features\",\n        icon: Layers,\n        color: \"orange\",\n        gradient: \"from-orange-500 to-orange-600\",\n        features: [\"3D Effects\", \"Parallax\", \"Dark Theme\", \"Hero Sections\"],\n        atmosphereVariant: \"default\" as const,\n        variants: [\n            { name: \"Cards\", component: <LandingA /> },\n            { name: \"Scroll\", component: <LandingB /> },\n            { name: \"Spatial\", component: <LandingC /> },\n            { name: \"Dark\", component: <LandingD /> },\n        ],\n    },\n    {\n        id: \"overview\",\n        title: \"Courses Overview\",\n        subtitle: \"Learning Paths\",\n        description: \"Pick a learning path from the world of software development\",\n        icon: Compass,\n        color: \"indigo\",\n        gradient: \"from-indigo-500 to-indigo-600\",\n        features: [\"Frontend\", \"Fullstack\", \"Knowledge Map\", \"Roadmap\", \"Skill Progress\"],\n        atmosphereVariant: \"default\" as const,\n        variants: [\n            { name: \"Grid Cards\", component: <OverviewA /> },\n            { name: \"Split View\", component: <OverviewB /> },\n            { name: \"Orbital\", component: <OverviewC /> },\n            { name: \"Knowledge Map\", component: <OverviewD /> },\n            { name: \"Roadmap\", component: <OverviewE /> },\n            { name: \"Skill Progress\", component: <OverviewF /> },\n        ],\n    },\n    {\n        id: \"goal-path\",\n        title: \"Goal Generation\",\n        subtitle: \"Path Builder\",\n        description: \"Define your end goal and get a reverse-engineered curriculum\",\n        icon: Target,\n        color: \"purple\",\n        gradient: \"from-purple-500 to-purple-600\",\n        features: [\"AI Generation\", \"Wizard Flow\", \"AI Chat\", \"Enhanced UX\"],\n        atmosphereVariant: \"cool\" as const,\n        variants: [\n            { name: \"Wizard\", component: <GoalA /> },\n            { name: \"Live Form\", component: <GoalB /> },\n            { name: \"AI Chat\", component: <GoalC /> },\n            { name: \"Enhanced\", component: <GoalD /> },\n        ],\n    },\n    {\n        id: \"career-mapping\",\n        title: \"Career Mapping\",\n        subtitle: \"Career Outcomes\",\n        description: \"Connect learning to real jobs, salaries, and communities\",\n        icon: Briefcase,\n        color: \"emerald\",\n        gradient: \"from-emerald-500 to-emerald-600\",\n        features: [\"Job Postings\", \"Salary Data\", \"Gamified\", \"Communities\"],\n        atmosphereVariant: \"warm\" as const,\n        variants: [\n            { name: \"Dashboard\", component: <CareerA /> },\n            { name: \"Path View\", component: <CareerB /> },\n            { name: \"Explorer\", component: <CareerC /> },\n            { name: \"Gamified\", component: <CareerD /> },\n        ],\n    },\n    {\n        id: \"chapter\",\n        title: \"Course Chapter\",\n        subtitle: \"Interactive Learning\",\n        description: \"Interactive syllabus with dynamic snippets and examples\",\n        icon: BookOpen,\n        color: \"cyan\",\n        gradient: \"from-cyan-500 to-cyan-600\",\n        features: [\"Video Lessons\", \"Code IDE\", \"Expandable\", \"Progress\"],\n        atmosphereVariant: \"cool\" as const,\n        variants: [\n            { name: \"Classic\", component: <ChapterA /> },\n            { name: \"Dynamic\", component: <ChapterB /> },\n            { name: \"Expandable\", component: <ChapterC /> },\n            { name: \"IDE\", component: <ChapterD /> },\n        ],\n    },\n    {\n        id: \"my-notes\",\n        title: \"My Notes\",\n        subtitle: \"Personal Library\",\n        description: \"All your bookmarks and notes in one place with search and export\",\n        icon: FileText,\n        color: \"purple\",\n        gradient: \"from-purple-500 to-indigo-600\",\n        features: [\"Bookmarks\", \"Tags\", \"Search\", \"Export MD\"],\n        atmosphereVariant: \"cool\" as const,\n        variants: [\n            { name: \"Library\", component: <MyNotesPage /> },\n        ],\n    },\n];\n\n// Prismatic Card Component\nconst PrismaticCard = ({ children, className, glowColor = \"indigo\" }: {\n    children: React.ReactNode;\n    className?: string;\n    glowColor?: string;\n}) => {\n    const ref = useRef<HTMLDivElement>(null);\n    const x = useMotionValue(0);\n    const y = useMotionValue(0);\n\n    const mouseXSpring = useSpring(x, { stiffness: 300, damping: 30 });\n    const mouseYSpring = useSpring(y, { stiffness: 300, damping: 30 });\n\n    const rotateX = useTransform(mouseYSpring, [-0.5, 0.5], [\"5deg\", \"-5deg\"]);\n    const rotateY = useTransform(mouseXSpring, [-0.5, 0.5], [\"-5deg\", \"5deg\"]);\n\n    const handleMouseMove = (e: React.MouseEvent<HTMLDivElement>) => {\n        const rect = ref.current?.getBoundingClientRect();\n        if (!rect) return;\n        const xPct = (e.clientX - rect.left) / rect.width - 0.5;\n        const yPct = (e.clientY - rect.top) / rect.height - 0.5;\n        x.set(xPct);\n        y.set(yPct);\n    };\n\n    const handleMouseLeave = () => {\n        x.set(0);\n        y.set(0);\n    };\n\n    const glowColors: Record<string, string> = {\n        indigo: \"from-indigo-500/20 to-purple-500/0 dark:from-indigo-400/30 dark:to-purple-400/0\",\n        purple: \"from-purple-500/20 to-pink-500/0 dark:from-purple-400/30 dark:to-pink-400/0\",\n        emerald: \"from-emerald-500/20 to-teal-500/0 dark:from-emerald-400/30 dark:to-teal-400/0\",\n        cyan: \"from-cyan-500/20 to-blue-500/0 dark:from-cyan-400/30 dark:to-blue-400/0\",\n        orange: \"from-orange-500/20 to-red-500/0 dark:from-orange-400/30 dark:to-red-400/0\",\n    };\n\n    return (\n        <motion.div\n            ref={ref}\n            onMouseMove={handleMouseMove}\n            onMouseLeave={handleMouseLeave}\n            style={{ rotateY, rotateX, transformStyle: \"preserve-3d\" }}\n            className={cn(\"relative group perspective-[2000px]\", className)}\n        >\n            <div\n                style={{ transform: \"translateZ(-20px)\" }}\n                className={cn(\n                    \"absolute inset-0 bg-gradient-to-br rounded-3xl blur-xl opacity-0 group-hover:opacity-100 transition-opacity duration-500 pointer-events-none\",\n                    glowColors[glowColor] || glowColors.indigo\n                )}\n            />\n            <div\n                style={{ transform: \"translateZ(20px)\" }}\n                className=\"relative w-full h-full bg-white/60 dark:bg-slate-800/60 backdrop-blur-xl border border-white/50 dark:border-slate-700/50 shadow-[0_8px_32px_rgba(0,0,0,0.05)] dark:shadow-[0_8px_32px_rgba(0,0,0,0.3)] rounded-3xl overflow-hidden\"\n            >\n                <div className=\"absolute inset-0 opacity-[0.03] bg-[url('https://grainy-gradients.vercel.app/noise.svg')] pointer-events-none\" />\n                <div className=\"absolute inset-0 bg-gradient-to-br from-white/40 dark:from-slate-700/40 to-transparent pointer-events-none\" />\n                <div className=\"relative z-10 h-full flex flex-col\">\n                    {children}\n                </div>\n            </div>\n        </motion.div>\n    );\n};\n\n// Floating Badge\nconst FloatingBadge = ({ icon: Icon, text, x, y, delay }: {\n    icon: React.ElementType;\n    text: string;\n    x: string;\n    y: string;\n    delay: number;\n}) => (\n    <motion.div\n        initial={{ opacity: 0, scale: 0 }}\n        animate={{\n            opacity: 1,\n            scale: 1,\n            x: [0, 8, 0],\n            y: [0, -8, 0]\n        }}\n        transition={{\n            opacity: { delay, duration: 0.5 },\n            scale: { delay, duration: 0.5 },\n            default: { repeat: Infinity, duration: 5 + Math.random() * 3, ease: \"easeInOut\" }\n        }}\n        className=\"absolute px-4 py-2 bg-white/80 dark:bg-slate-800/80 backdrop-blur-md rounded-full shadow-lg dark:shadow-[0_4px_16px_rgba(0,0,0,0.3)] border border-white/50 dark:border-slate-700/50 flex items-center gap-2 text-xs font-semibold text-slate-600 dark:text-slate-300 z-10 pointer-events-none\"\n        style={{ left: x, top: y }}\n    >\n        <Icon size={14} className=\"text-indigo-500 dark:text-indigo-400\" />\n        {text}\n    </motion.div>\n);\n\n// Module Card for Home View\nconst ModuleCard = ({ module, index, onClick, progress = 0 }: {\n    module: typeof modules[0];\n    index: number;\n    onClick: () => void;\n    progress?: number;\n}) => (\n    <motion.div\n        initial={{ opacity: 0, y: 30 }}\n        animate={{ opacity: 1, y: 0 }}\n        transition={{ delay: index * 0.1 }}\n    >\n        <button onClick={onClick} className=\"w-full text-left\" data-testid={`module-card-${module.id}`}>\n            <PrismaticCard className=\"h-full cursor-pointer\" glowColor={module.color}>\n                <div className=\"p-6 h-full flex flex-col\">\n                    <div className=\"flex items-start justify-between mb-4\">\n                        <div className={cn(\n                            \"w-14 h-14 rounded-2xl flex items-center justify-center text-white bg-gradient-to-br\",\n                            module.gradient\n                        )}>\n                            <module.icon size={28} />\n                        </div>\n                        <div className=\"flex flex-col items-end gap-1\">\n                            <span className=\"px-3 py-1 bg-slate-100 dark:bg-slate-700 rounded-full text-xs font-bold text-slate-500 dark:text-slate-300\">\n                                {module.subtitle}\n                            </span>\n                            {progress > 0 && (\n                                <span className=\"text-xs font-bold text-indigo-600 dark:text-indigo-400\">\n                                    {progress}% complete\n                                </span>\n                            )}\n                        </div>\n                    </div>\n\n                    <h3 className=\"text-xl font-bold text-slate-900 dark:text-slate-100 mb-2\">\n                        {module.title}\n                    </h3>\n                    <p className=\"text-sm text-slate-600 dark:text-slate-400 mb-4 flex-grow\">\n                        {module.description}\n                    </p>\n\n                    {progress > 0 && (\n                        <div className=\"mb-4\">\n                            <ProgressBar\n                                progress={progress}\n                                size=\"sm\"\n                                color={module.color as \"indigo\" | \"emerald\" | \"purple\" | \"cyan\" | \"orange\"}\n                            />\n                        </div>\n                    )}\n\n                    <div className=\"flex flex-wrap gap-2 mb-4\">\n                        {module.features.slice(0, 3).map((feature, i) => (\n                            <span\n                                key={i}\n                                className=\"px-2 py-1 bg-slate-50 dark:bg-slate-700/50 rounded-lg text-xs font-medium text-slate-600 dark:text-slate-300\"\n                            >\n                                {feature}\n                            </span>\n                        ))}\n                        {module.features.length > 3 && (\n                            <span className=\"px-2 py-1 bg-slate-50 dark:bg-slate-700/50 rounded-lg text-xs font-medium text-slate-400\">\n                                +{module.features.length - 3}\n                            </span>\n                        )}\n                    </div>\n\n                    <div className=\"flex items-center gap-2 text-sm font-bold text-slate-700 dark:text-slate-300 group-hover:text-indigo-600 dark:group-hover:text-indigo-400 transition-colors\">\n                        {progress > 0 ? \"Continue Learning\" : `Explore ${module.variants.length} Variants`} <ArrowRight size={16} className=\"group-hover:translate-x-1 transition-transform\" />\n                    </div>\n                </div>\n            </PrismaticCard>\n        </button>\n    </motion.div>\n);\n\n// Home View Component\nconst HomeView = ({\n    onSelectModule,\n    continueLearning,\n    totalWatchTime,\n    overallCompletion,\n    courseProgress,\n    onExport,\n    onClearAll,\n}: {\n    onSelectModule: (moduleId: string) => void;\n    continueLearning: ContinueLearningItem[];\n    totalWatchTime: number;\n    overallCompletion: number;\n    courseProgress: Record<string, number>;\n    onExport: () => void;\n    onClearAll: () => void;\n}) => (\n    <>\n        {/* Theme Toggle in top right */}\n        <div className=\"fixed top-4 right-4 z-50\">\n            <ThemeToggle />\n        </div>\n\n        {/* Floating Badges */}\n        <FloatingBadge icon={Cpu} text=\"AI Powered\" x=\"3%\" y=\"15%\" delay={0.2} />\n        <FloatingBadge icon={Globe} text=\"6 Modules\" x=\"88%\" y=\"12%\" delay={0.5} />\n        <FloatingBadge icon={Zap} text=\"4 Variants Each\" x=\"5%\" y=\"85%\" delay={0.8} />\n        <FloatingBadge icon={Hexagon} text=\"Spatial Design\" x=\"85%\" y=\"80%\" delay={1.1} />\n\n        <div className=\"relative z-10 max-w-7xl mx-auto px-4 py-16\">\n            {/* Header */}\n            <div className=\"text-center mb-16\">\n                <motion.div\n                    initial={{ opacity: 0, y: 20 }}\n                    animate={{ opacity: 1, y: 0 }}\n                    className=\"inline-flex items-center gap-2 px-5 py-2 bg-white/50 dark:bg-slate-800/50 backdrop-blur-md rounded-full border border-white/50 dark:border-slate-700/50 mb-8\"\n                >\n                    <GraduationCap size={18} className=\"text-indigo-600 dark:text-indigo-400\" />\n                    <span className=\"text-sm font-bold text-slate-700 dark:text-slate-300\">Course Platform Prototype</span>\n                </motion.div>\n\n                <motion.h1\n                    initial={{ opacity: 0, y: 20 }}\n                    animate={{ opacity: 1, y: 0 }}\n                    transition={{ delay: 0.1 }}\n                    className=\"text-5xl md:text-7xl font-black tracking-tight text-transparent bg-clip-text bg-gradient-to-br from-slate-900 via-slate-700 to-slate-900 dark:from-slate-100 dark:via-slate-300 dark:to-slate-100 mb-6\"\n                >\n                    Module Prototypes\n                </motion.h1>\n\n                <motion.p\n                    initial={{ opacity: 0, y: 20 }}\n                    animate={{ opacity: 1, y: 0 }}\n                    transition={{ delay: 0.2 }}\n                    className=\"text-xl text-slate-600 dark:text-slate-400 max-w-3xl mx-auto mb-4\"\n                >\n                    Explore 6 UI modules for a dynamically generated education platform.\n                    Each module features multiple variants to compare different approaches.\n                </motion.p>\n\n                <motion.div\n                    initial={{ opacity: 0, y: 20 }}\n                    animate={{ opacity: 1, y: 0 }}\n                    transition={{ delay: 0.3 }}\n                    className=\"flex items-center justify-center gap-4 text-sm text-slate-500 dark:text-slate-400\"\n                >\n                    <span className=\"flex items-center gap-1\">\n                        <Sparkles size={16} className=\"text-indigo-500 dark:text-indigo-400\" />\n                        Spatial Theme\n                    </span>\n                    <span className=\"w-1 h-1 bg-slate-300 dark:bg-slate-600 rounded-full\" />\n                    <span>3D Effects</span>\n                    <span className=\"w-1 h-1 bg-slate-300 dark:bg-slate-600 rounded-full\" />\n                    <span>Framer Motion</span>\n                </motion.div>\n            </div>\n\n            {/* Continue Learning Section */}\n            {continueLearning.length > 0 && (\n                <motion.div\n                    initial={{ opacity: 0, y: 20 }}\n                    animate={{ opacity: 1, y: 0 }}\n                    transition={{ delay: 0.4 }}\n                    className=\"mb-12\"\n                >\n                    <ContinueLearningSection\n                        items={continueLearning}\n                        totalWatchTime={totalWatchTime}\n                        overallCompletion={overallCompletion}\n                        onContinue={(item) => onSelectModule(item.courseId)}\n                        onExport={onExport}\n                        onClearAll={onClearAll}\n                    />\n                </motion.div>\n            )}\n\n            {/* Module Grid */}\n            <div className=\"grid grid-cols-1 md:grid-cols-2 lg:grid-cols-3 gap-6 mb-16\">\n                {modules.map((module, index) => (\n                    <ModuleCard\n                        key={module.id}\n                        module={module}\n                        index={index}\n                        onClick={() => onSelectModule(module.id)}\n                        progress={courseProgress[module.id] || 0}\n                    />\n                ))}\n            </div>\n\n            {/* Footer */}\n            <div className=\"text-center\">\n                <span className=\"text-[10px] bg-white/30 dark:bg-slate-800/30 px-3 py-1 rounded-full text-slate-500 dark:text-slate-400 backdrop-blur-sm\">\n                    Designed for the Future of Learning\n                </span>\n            </div>\n        </div>\n    </>\n);\n\n// Module View Component\nconst ModuleView = ({\n    module,\n    activeVariant,\n    onVariantChange,\n    onBack\n}: {\n    module: typeof modules[0];\n    activeVariant: number;\n    onVariantChange: (index: number) => void;\n    onBack: () => void;\n}) => {\n    const breadcrumbItems: BreadcrumbItem[] = [\n        { label: \"Home\", onClick: onBack },\n        { label: module.title, onClick: module.variants.length > 1 ? undefined : undefined },\n        { label: module.variants[activeVariant].name, active: true },\n    ];\n\n    return (\n        <>\n            <BackgroundAtmosphere variant={module.atmosphereVariant} />\n\n            <header className=\"sticky top-0 z-50 px-4 lg:px-8 py-4\">\n                <div className=\"max-w-7xl mx-auto flex items-center justify-between\">\n                    <div className=\"flex items-center gap-4\">\n                        <Breadcrumb items={breadcrumbItems} />\n                    </div>\n\n                    <div className=\"flex items-center gap-4\">\n                        <VariantTabs\n                            variants={module.variants.map(v => v.name)}\n                            activeVariant={activeVariant}\n                            onVariantChange={onVariantChange}\n                        />\n                        <ThemeToggle />\n                    </div>\n                </div>\n            </header>\n\n        <main className=\"relative z-10 p-4 lg:p-8\">\n            <AnimatePresence mode=\"wait\">\n                <motion.div\n                    key={activeVariant}\n                    initial={{ opacity: 0, y: 20 }}\n                    animate={{ opacity: 1, y: 0 }}\n                    exit={{ opacity: 0, y: -20 }}\n                    transition={{ duration: 0.3 }}\n                    className=\"max-w-7xl mx-auto\"\n                >\n                    {module.variants[activeVariant].component}\n                </motion.div>\n            </AnimatePresence>\n        </main>\n        </>\n    );\n};\n\n// Main App Component\nexport default function HomePage() {\n    const [activeModule, setActiveModule] = useState<string | null>(null);\n    const [activeVariant, setActiveVariant] = useState(0);\n    const [isExportModalOpen, setIsExportModalOpen] = useState(false);\n\n    // Progress tracking\n    const {\n        courses,\n        continueLearning,\n        totalWatchTime,\n        overallCompletion,\n        refresh: refreshProgress,\n        clearAll,\n    } = useProgressOverview();\n\n    // Build course progress map\n    const courseProgress: Record<string, number> = {};\n    courses.forEach((course) => {\n        courseProgress[course.courseId] = course.overallProgress;\n    });\n\n    // Transform continue learning items to match module IDs\n    const transformedContinueLearning: ContinueLearningItem[] = continueLearning.map((item) => ({\n        ...item,\n        // Map the course to a module if possible\n        courseId: modules.find((m) => m.id === item.courseId)?.id || item.courseId,\n    }));\n\n    const currentModule = modules.find(m => m.id === activeModule);\n\n    const handleSelectModule = (moduleId: string) => {\n        setActiveModule(moduleId);\n        setActiveVariant(0);\n    };\n\n    const handleBack = () => {\n        setActiveModule(null);\n        setActiveVariant(0);\n        refreshProgress();\n    };\n\n    const handleExport = () => {\n        setIsExportModalOpen(true);\n    };\n\n    const handleClearAll = () => {\n        if (window.confirm(\"Are you sure you want to clear all progress data? This cannot be undone.\")) {\n            clearAll();\n        }\n    };\n\n    return (\n        <div className=\"min-h-screen bg-[#F0F2F5] dark:bg-[#0a0f1a] font-sans overflow-x-hidden selection:bg-indigo-100 dark:selection:bg-indigo-900 transition-colors duration-300\">\n            {/* Background for home view */}\n            {!activeModule && (\n                <div className=\"fixed inset-0 overflow-hidden pointer-events-none\">\n                    <motion.div\n                        animate={{ rotate: 360, scale: [1, 1.1, 1] }}\n                        transition={{ duration: 60, repeat: Infinity, ease: \"linear\" }}\n                        className=\"absolute -top-[30%] -left-[20%] w-[80vw] h-[80vw] bg-gradient-to-br from-indigo-200/30 via-purple-200/30 to-blue-200/30 dark:from-indigo-900/30 dark:via-purple-900/30 dark:to-blue-900/30 rounded-full blur-[120px]\"\n                    />\n                    <motion.div\n                        animate={{ rotate: -360, scale: [1, 1.2, 1] }}\n                        transition={{ duration: 80, repeat: Infinity, ease: \"linear\" }}\n                       