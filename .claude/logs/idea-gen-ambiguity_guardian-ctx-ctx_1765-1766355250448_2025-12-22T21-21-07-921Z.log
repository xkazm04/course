ing = \"main\", order?: number): LayoutBuilder {\n   363→        return this.addSlot(codeSlot(`code-${this.placements.length}`, code, filename), region, order);\n   364→    }\n   365→\n   366→    addKeyPoints(points: string[], region: string = \"main\", order?: number): LayoutBuilder {\n   367→        return this.addSlot(keyPointsSlot(`keypoints-${this.placements.length}`, points), region, order);\n   368→    }\n   369→\n   370→    addQuizButton(sectionId: string, region: string = \"main\", order?: number): LayoutBuilder {\n   371→        return this.addSlot(quizButtonSlot(`quiz-btn-${this.placements.length}`, sectionId), region, order);\n   372→    }\n   373→\n   374→    addNavigation(compact: boolean = false, region: string = \"footer\", order?: number): LayoutBuilder {\n   375→        return this.addSlot(navigationSlot(`nav-${this.placements.length}`, compact), region, order);\n   376→    }\n   377→\n   378→    addProgress(variant: \"sidebar\" | \"header\" = \"sidebar\", region: string = \"sidebar\", order?: number): LayoutBuilder {\n   379→        const slot = variant === \"header\" ? progressHeaderSlot() : progressSidebarSlot();\n   380→        return this.addSlot(slot, region, order);\n   381→    }\n   382→\n   383→    addActions(region: string = \"sidebar\", order?: number): LayoutBuilder {\n   384→        return this.addSlot(actionsSlot(), region, order);\n   385→    }\n   386→\n   387→    addSectionList(region: string = \"main\", order?: number): LayoutBuilder {\n   388→        return this.addSlot(sectionListSlot(), region, order);\n   389→    }\n   390→\n   391→    addPlayground(playgroundId: string, title?: string, region: string = \"main\", order?: number): LayoutBuilder {\n   392→        return this.addSlot(playgroundSlot(`playground-${this.placements.length}`, playgroundId, title), region, order);\n   393→    }\n   394→\n   395→    addText(title: string, content: string, region: string = \"main\", order?: number): LayoutBuilder {\n   396→        return