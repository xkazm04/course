             initial={{ opacity: shouldAnimate ? 0 : 1 }}\n                        animate={{ opacity: 1 }}\n                        transition={{ delay: shouldAnimate ? 0.4 * durationMultiplier : 0 }}\n                        className=\"flex flex-col sm:flex-row gap-4 justify-center lg:justify-start\"\n                    >\n                        <Link href=\"/overview\">\n                            <button\n                                onMouseEnter={() => handlePrefetch(\"/overview\")}\n                                className={cn(\n                                    \"group relative px-8 py-4 rounded-2xl shadow-xl overflow-hidden hover:scale-[1.02] transition-transform\",\n                                    theme.buttons.primary\n                                )}\n                                data-testid=\"landing-enter-platform-btn\"\n                            >\n                                <div className={cn(\n                                    \"absolute inset-0 opacity-0 group-hover:opacity-100 transition-opacity duration-300\",\n                                    theme.buttons.primaryHoverOverlay\n                                )} />\n                                <span className={cn(\n                                    \"relative z-10 font-bold flex items-center gap-2\",\n                                    theme.buttons.primaryTextHover\n                                )}>\n                                    Enter Platform <BoxSelect size={ICON_SIZES.md} />\n                                </span>\n                            </button>\n                        </Link>\n                        <Link href=\"/goal-path\">\n                            <button\n                                onMouseEnter={() => handlePrefetch(\"/goal-path\")}\n                                className={cn(\n                                    \"px-8 py-4 backdrop-blur-md rounded-2xl font-bold transition-colors\",\n                                    