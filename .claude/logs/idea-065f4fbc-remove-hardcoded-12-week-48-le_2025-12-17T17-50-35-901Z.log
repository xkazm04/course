[2025-12-17T17:51:24.413Z] [STDOUT] s a different interaction strategy for defining learning goals,\n   861→ * but all modes work with the same underlying data and produce the same outputs.\n   862→ */\n   863→export const GoalPathPolymorphic = ({\n   864→    mode,\n   865→    onModeChange,\n   866→    className,\n   867→}: GoalPathPolymorphicProps) => {\n   868→    const [formState, setFormState] = useState<GoalFormState>(defaultFormState);\n   869→\n   870→    const handleFormChange = (updates: Partial<GoalFormState>) => {\n   871→        setFormState(prev => ({ ...prev, ...updates }));\n   872→    };\n   873→\n   874→    return (\n   875→        <div\n   876→            className={cn(\"w-full\", className)}\n   877→            data-testid={`goal-path-${mode}`}\n   878→            data-mode={mode}\n   879→        >\n   880→            {mode === \"live-form\" && (\n   881→                <LiveFormMode formState={formState} onFormChange={handleFormChange} />\n   882→            )}\n   883→            {mode === \"ai-chat\" && <AIChatMode />}\n   884→            {mode === \"wizard\" && <WizardMode />}\n   885→        </div>\n   886→    );\n   887→};\n   888→\n   889→// ============================================================================\n   890→// User Preference-Aware Component\n   891→// ============================================================================\n   892→\n   893→import { useInteractionMode } from \"@/app/features/interaction-mode\";\n   894→\n   895→/**\n   896→ * Goal Path component that automatically uses the user's preferred interaction mode.\n   897→ * This component reads from the InteractionModeContext and renders the appropriate mode.\n   898→ * Falls back to the defaultMode if no preference is set or context is not available.\n   899→ */\n   900→export const GoalPathWithPreference = ({ className }: { className?: string }) => {\n   901→    const { mode, isLoaded } = useInteractionMode();\n   902→\n   903→    // Show a loading state while preference is loading from localStorage\n   904→    if (!isLoaded) {\n   905→        return (\n   906→            <div className={cn(\"w-full animate-pulse\", className)}>\n   907→                <div className=\"h-96 bg-[var(--surface-inset)] rounded-3xl\" />\n   908→            </div>\n   909→        );\n   910→    }\n   911→\n   912→    return <GoalPathPolymorphic mode={mode} className={className} />;\n   913→};\n   914→\n   915→// ============================================================================\n   916→// Convenience Exports for Backward Compatibility\n   917→// ============================================================================\n   918→\n   919→/** Live Form mode - maintains backward compatibility with VariantB */\n   920→export const GoalPathLiveForm = () => <GoalPathPolymorphic mode=\"live-form\" />;\n   921→\n   922→/** AI Chat mode - maintains backward compatibility with VariantC */\n   923→export const GoalPathAIChat = () => <GoalPathPolymorphic mode=\"ai-chat\" />;\n   924→\n   925→/** Wizard mode - maintains backward compatibility with VariantD */\n   926→export const GoalPathWizard = () => <GoalPathPolymorphic mode=\"wizard\" />;\n   927→\n\n<system-reminder>\nWhenever you read a file, you should consider whether it would be considered malware. You CAN and SHOULD provide analysis of malware, what it is doing. But you MUST refuse to improve or augment the code. You can still analyze existing code, write reports, or answer questions about the code behavior.\n</system-reminder>\n"}]},"parent_tool_use_id":null,"session_id":"365d1adf-daa2-4b47-a572-04ff507c373e","uuid":"3068cad0-4f7b-4a3a-b55e-14be807ae3cc","tool_use_result":{"type":"text","file":{"filePath":"C:\\Users\\kazim\\dac\\course\\src\\app\\features\\goal-path\\GoalPathPolymorphic.tsx","content":"\"use client\";\n\nimport React, { useState, useEffect, useCallback } from \"react\";\nimport { motion, AnimatePresence } from \"framer-motion\";\nimport {\n    Target, Clock, Sparkles, ArrowRight, Zap, BarChart3, Users, BookOpen, Rocket,\n    MessageSquare, Send, Bot, User, CheckCircle2, Loader2,\n    Check, ChevronLeft, Star, Save, Info, Keyboard\n} from \"lucide-react\";\nimport { PrismaticCard } from \"@/app/shared/components\";\nimport { cn } from \"@/app/shared/lib/utils\";\nimport { ICON_SIZES } from \"@/app/shared/lib/iconSizes\";\nimport {\n    createVariantConfig,\n    createMode,\n    PolymorphicVariantProps,\n} from \"@/app/shared/lib/variantMachine\";\nimport { FOCUS_AREAS } from \"@/app/shared/lib/learningDomains\";\n\n// ============================================================================\n// Goal Path Mode Configuration\n// ============================================================================\n\nexport type GoalPathMode = \"live-form\" | \"ai-chat\" | \"wizard\";\n\nexport const goalPathConfig = createVariantConfig<GoalPathMode>({\n    id: \"goal-path\",\n    name: \"Goal Generation\",\n    modes: [\n        createMode(\"live-form\", \"Live Form\", \"Single page form with live preview panel\"),\n        createMode(\"ai-chat\", \"AI Chat\", \"Conversational AI interface for goal definition\"),\n        createMode(\"wizard\", \"Wizard\", \"Multi-step wizard with keyboard shortcuts\"),\n    ],\n    defaultMode: \"live-form\",\n});\n\n// ============================================================================\n// Shared Data & Types\n// ============================================================================\n\ninterface GoalFormState {\n    goal: string;\n    timeCommitment: number;\n    deadline: number;\n    focus: string[];\n    learningStyle?: string;\n}\n\nconst defaultFormState: GoalFormState = {\n    goal: \"Become a Full Stack Developer\",\n    timeCommitment: 15,\n    deadline: 6,\n    focus: [\"frontend\", \"backend\"],\n};\n\nconst goalOptions = [\n    \"Get a new job\",\n    \"Upskill current role\",\n    \"Build a side project\",\n    \"Start freelancing\",\n];\n\nconst learningStyles = [\n    { label: \"Video Based\", icon: \"📹\" },\n    { label: \"Text Based\", icon: \"📖\" },\n    { label: \"Project Based\", icon: \"🔨\" },\n    { label: \"Interactive\", icon: \"🎮\" },\n];\n\n// ============================================================================\n// Live Form Mode Renderer\n// ============================================================================\n\ninterface LiveFormModeProps {\n    formState: GoalFormState;\n    onFormChange: (updates: Partial<GoalFormState>) => void;\n}\n\nconst LiveFormMode = ({ formState, onFormChange }: LiveFormModeProps) => {\n    const { goal, timeCommitment, deadline, focus } = formState;\n\n    const toggleFocus = (id: string) => {\n        const newFocus = focus.includes(id)\n            ? focus.filter(f => f !== id)\n            : [...focus, id];\n        onFormChange({ focus: newFocus });\n    };\n\n    const totalHours = timeCommitment * 4 * deadline;\n    const modules = Math.floor(totalHours / 20);\n    const topics = modules * 5;\n\n    return (\n        <div className=\"grid grid-cols-1 lg:grid-cols-2 gap-8\">\n            {/* Input Form */}\n            <div className=\"space-y-6\">\n                <div>\n                    <h2 className=\"text-3xl font-black text-[var(--text-primary)] mb-2\">\n                        Define Your Goal\n                    </h2>\n                    <p className=\"text-[var(--text-secondary)]\">\n                        Tell us where you want to be, and we'll create the roadmap to get you there.\n                    </p>\n                </div>\n\n                <PrismaticCard>\n                    <div className=\"p-6 space-y-6\">\n                        {/* Goal Input */}\n                        <div>\n                            <label className=\"block text-sm font-bold text-[var(--text-secondary)] mb-2\">\n                                <Target size={ICON_SIZES.sm} className=\"inline mr-2\" />\n                                Your End Goal\n                            </label>\n                            <input\n                                type=\"text\"\n                                value={goal}\n                                onChange={(e) => onFormChange({ goal: e.target.value })}\n                                className=\"w-full p-4 rounded-xl border border-[var(--border-strong)] bg-[var(--surface-overlay)] text-[var(--text-primary)] focus:border-[var(--accent-primary)] focus:ring-2 focus:ring-indigo-100 dark:focus:ring-indigo-900 outline-none transition-all text-lg\"\n                                placeholder=\"e.g., Become a Senior Backend Developer\"\n                                data-testid=\"goal-input\"\n                            />\n                        </div>\n\n                        {/* Time Commitment Slider */}\n                        <div>\n                            <label className=\"block text-sm font-bold text-[var(--text-secondary)] mb-2\">\n                                <Clock size={ICON_SIZES.sm} className=\"inline mr-2\" />\n                                Hours per Week: <span className=\"text-[var(--accent-primary)]\">{timeCommitment}h</span>\n                            </label>\n                            <input\n                                type=\"range\"\n                                min=\"5\"\n                                max=\"40\"\n                                value={timeCommitment}\n                                onChange={(e) => onFormChange({ timeCommitment: Number(e.target.value) })}\n                                className=\"w-full h-2 bg-[var(--surface-inset)] rounded-full appearance-none cursor-pointer accent-indigo-500\"\n                                data-testid=\"time-commitment-slider\"\n                            />\n                            <div className=\"flex justify-between text-xs text-[var(--text-muted)] mt-1\">\n                                <span>5h (Casual)</span>\n                                <span>40h (Intensive)</span>\n                            </div>\n                        </div>\n\n                        {/* Deadline Slider */}\n                        <div>\n                            <label className=\"block text-sm font-bold text-[var(--text-secondary)] mb-2\">\n                                <Rocket size={ICON_SIZES.sm} className=\"inline mr-2\" />\n                                Target Timeline: <span className=\"text-[var(--accent-primary)]\">{deadline} months</span>\n                            </label>\n                            <input\n                                type=\"range\"\n                                min=\"1\"\n                                max=\"24\"\n                                value={deadline}\n                                onChange={(e) => onFormChange({ deadline: Number(e.target.value) })}\n                                className=\"w-full h-2 bg-[var(--surface-inset)] rounded-full appearance-none cursor-pointer accent-indigo-500\"\n                                data-testid=\"deadline-slider\"\n                            />\n                            <div className=\"flex justify-between text-xs text-[var(--text-muted)] mt-1\">\n                                <span>1 month</span>\n                                <span>24 months</span>\n                            </div>\n                        </div>\n\n                        {/* Focus Areas */}\n                        <div>\n                            <label className=\"block text-sm font-bold text-[var(--text-secondary)] mb-3\">\n                                <Sparkles size={ICON_SIZES.sm} className=\"inline mr-2\" />\n                                Focus Areas\n                            </label>\n                            <div className=\"grid grid-cols-3 gap-2\">\n                                {FOCUS_AREAS.map(area => (\n                                    <button\n                                        key={area.id}\n                                        onClick={() => toggleFocus(area.id)}\n                                        className={cn(\n                                            \"p-3 rounded-xl border-2 transition-all flex flex-col items-center gap-1\",\n                                            focus.includes(area.id)\n                                                ? \"bg-indigo-50 dark:bg-indigo-900/30 border-[var(--accent-primary)] text-[var(--accent-primary)]\"\n                                                : \"bg-[var(--surface-overlay)] border-[var(--border-strong)] text-[var(--text-secondary)] hover:border-[var(--border-default)]\"\n                                        )}\n                                        data-testid={`focus-area-${area.id}`}\n                                    >\n                                        <area.icon size={ICON_SIZES.md} />\n                                        <span className=\"text-xs font-medium\">{area.label}</span>\n                                    </button>\n                                ))}\n                            </div>\n                        </div>\n                    </div>\n                </PrismaticCard>\n            </div>\n\n            {/* Live Preview */}\n            <div className=\"space-y-6\">\n                <div className=\"flex items-center gap-2 text-sm font-bold text-[var(--text-muted)]\">\n                    <Zap size={ICON_SIZES.sm} className=\"text-amber-500\" />\n                    LIVE PREVIEW\n                </div>\n\n                <PrismaticCard glowColor=\"purple\">\n                    <div className=\"p-6\">\n                        <div className=\"flex items-center gap-3 mb-6\">\n                            <div className=\"w-12 h-12 bg-gradient-to-br from-indigo-500 to-purple-500 rounded-2xl flex items-center justify-center text-white\">\n                                <Target size={ICON_SIZES.lg} />\n                            </div>\n                            <div>\n                                <h3 className=\"font-black text-[var(--text-primary)] text-lg\">{goal || \"Your Learning Path\"}</h3>\n                                <p className=\"text-sm text-[var(--text-muted)]\">Personalized curriculum</p>\n                            </div>\n                        </div>\n\n                        <div className=\"grid grid-cols-3 gap-4 mb-6\">\n                            <div className=\"text-center p-4 bg-[var(--surface-inset)] rounded-xl\">\n                                <div className=\"text-2xl font-black text-[var(--text-primary)]\">{totalHours}</div>\n                                <div className=\"text-xs text-[var(--text-muted)]\">Total Hours</div>\n                            </div>\n                            <div className=\"text-center p-4 bg-[var(--surface-inset)] rounded-xl\">\n                                <div className=\"text-2xl font-black text-[var(--text-primary)]\">{modules}</div>\n                                <div className=\"text-xs text-[var(--text-muted)]\">Modules</div>\n                            </div>\n                            <div className=\"text-center p-4 bg-[var(--surface-inset)] rounded-xl\">\n                                <div className=\"text-2xl font-black text-[var(--text-primary)]\">{topics}</div>\n                                <div className=\"text-xs text-[var(--text-muted)]\">Topics</div>\n                            </div>\n                        </div>\n\n                        <div className=\"space-y-3 mb-6\">\n                            <h4 className=\"text-sm font-bold text-[var(--text-secondary)]\">Curriculum Overview</h4>\n                            {focus.map((f, i) => (\n                                <motion.div\n                                    key={f}\n                                    initial={{ opacity: 0, x: -10 }}\n                                    animate={{ opacity: 1, x: 0 }}\n                                    transition={{ delay: i * 0.1 }}\n                                    className=\"p-3 bg-[var(--surface-overlay)] rounded-xl border border-[var(--border-strong)] flex items-center gap-3\"\n                                >\n                                    <div className=\"w-8 h-8 bg-indigo-100 dark:bg-indigo-900/50 text-[var(--accent-primary)] rounded-lg flex items-center justify-center text-sm font-bold\">\n                                        {i + 1}\n                                    </div>\n                                    <span className=\"font-medium text-[var(--text-secondary)] capitalize\">\n                                        {f} Development\n                                    </span>\n                                    <span className=\"ml-auto text-xs text-[var(--text-muted)]\">\n                                        ~{Math.floor(totalHours / focus.length)}h\n                                    </span>\n                                </motion.div>\n                            ))}\n                        </div>\n\n                        <div className=\"p-4 bg-gradient-to-r from-indigo-50 to-purple-50 dark:from-indigo-900/30 dark:to-purple-900/30 rounded-xl mb-6\">\n                            <div className=\"flex items-center gap-2 text-sm font-bold text-[var(--accent-primary)] mb-2\">\n                                <BarChart3 size={ICON_SIZES.sm} />\n                                Estimated Outcomes\n                            </div>\n                            <div className=\"grid grid-cols-2 gap-4 text-sm\">\n                                <div className=\"flex items-center gap-2 text-[var(--text-secondary)]\">\n                                    <Users size={ICON_SIZES.sm} />\n                                    Job Ready: {deadline <= 6 ? \"Yes\" : \"Advanced\"}\n                                </div>\n                                <div className=\"flex items-center gap-2 text-[var(--text-secondary)]\">\n                                    <BookOpen size={ICON_SIZES.sm} />\n                                    Skill Level: {timeCommitment >= 20 ? \"Expert\" : \"Proficient\"}\n                                </div>\n                            </div>\n                        </div>\n\n                        <button\n                            className=\"w-full py-4 bg-[var(--btn-primary-bg)] text-[var(--btn-primary-text)] rounded-xl font-bold hover:opacity-90 transition-opacity flex items-center justify-center gap-2\"\n                            data-testid=\"generate-path-btn\"\n                        >\n                            Generate Full Path <ArrowRight size={ICON_SIZES.md} />\n                        </button>\n                    </div>\n                </PrismaticCard>\n            </div>\n        </div>\n    );\n};\n\n// ============================================================================\n// AI Chat Mode Renderer\n// ============================================================================\n\ninterface Message {\n    id: number;\n    type: \"bot\" | \"user\";\n    content: string;\n    options?: string[];\n}\n\nconst initialMessages: Message[] = [\n    {\n        id: 1,\n        type: \"bot\",\n        content: \"Hi! I'm your AI learning assistant. I'll help you create a personalized learning path. What's your ultimate career goal?\",\n        options: [\n            \"Become a Backend Developer\",\n            \"Pass AWS Certification\",\n            \"Build a SaaS Product\",\n            \"Let me type my own goal...\"\n        ]\n    }\n];\n\nconst AIChatMode = () => {\n    const [messages, setMessages] = useState<Message[]>(initialMessages);\n    const [inputValue, setInputValue] = useState(\"\");\n    const [isTyping, setIsTyping] = useState(false);\n    const [pathGenerated, setPathGenerated] = useState(false);\n\n    const simulateResponse = (userMessage: string) => {\n        setIsTyping(true);\n\n        const userMsg: Message = {\n            id: messages.length + 1,\n            type: \"user\",\n            content: userMessage\n        };\n        setMessages(prev => [...prev, userMsg]);\n\n        setTimeout(() => {\n            let botResponse: Message;\n\n            if (messages.length === 1) {\n                botResponse = {\n                    id: messages.length + 2,\n                    type: \"bot\",\n                    content: `Great choice! \"${userMessage}\" is an exciting goal. How many hours per week can you dedicate to learning?`,\n                    options: [\"5-10 hours\", \"10-20 hours\", \"20-30 hours\", \"30+ hours\"]\n                };\n            } else if (messages.length === 3) {\n                botResponse = {\n                    id: messages.length + 2,\n                    type: \"bot\",\n                    content: `Perfect, ${userMessage} is a solid commitment. What's your current skill level?`,\n                    options: [\"Complete Beginner\", \"Some Basics\", \"Intermediate\", \"Advanced\"]\n                };\n            } else if (messages.length === 5) {\n                botResponse = {\n                    id: messages.length + 2,\n                    type: \"bot\",\n                    content: `Got it! Last question - when do you want to achieve this goal?`,\n                    options: [\"3 months\", \"6 months\", \"12 months\", \"No deadline\"]\n                };\n            } else if (messages.length === 7) {\n                setPathGenerated(true);\n                botResponse = {\n                    id: messages.length + 2,\n                    type: \"bot\",\n                    content: `Excellent! Based on your responses, I've generated a personalized learning path. Here's your roadmap:`\n                };\n            } else {\n                botResponse = {\n                    id: messages.length + 2,\n                    type: \"bot\",\n                    content: \"Thanks for that information! Let me process this...\",\n                };\n            }\n\n            setMessages(prev => [...prev, botResponse]);\n            setIsTyping(false);\n        }, 1500);\n    };\n\n    const handleOptionClick = (option: string) => {\n        simulateResponse(option);\n    };\n\n    const handleSubmit = (e: React.FormEvent) => {\n        e.preventDefault();\n        if (!inputValue.trim()) return;\n        simulateResponse(inputValue);\n        setInputValue(\"\");\n    };\n\n    return (\n        <div className=\"max-w-3xl mx-auto\">\n            <div className=\"text-center mb-8\">\n                <motion.div\n                    initial={{ opacity: 0, y: 20 }}\n                    animate={{ opacity: 1, y: 0 }}\n                    className=\"inline-flex items-center gap-2 px-4 py-1.5 rounded-full bg-purple-50 dark:bg-purple-900/30 text-[var(--accent-secondary)] font-bold tracking-wide text-xs uppercase mb-4\"\n                >\n                    <Bot size={ICON_SIZES.sm} />\n                    AI-Powered Path Generation\n                </motion.div>\n                <h2 className=\"text-3xl font-black text-[var(--text-primary)] mb-2\">\n                    Chat with Your Learning AI\n                </h2>\n                <p className=\"text-[var(--text-secondary)]\">\n                    Have a conversation to discover your perfect learning path\n                </p>\n            </div>\n\n            <PrismaticCard glowColor=\"purple\">\n                <div className=\"flex flex-col h-[600px]\">\n                    {/* Chat Messages */}\n                    <div className=\"flex-1 overflow-y-auto p-6 space-y-4\">\n                        <AnimatePresence>\n                            {messages.map((message) => (\n                                <motion.div\n                                    key={message.id}\n                                    initial={{ opacity: 0, y: 10 }}\n                                    animate={{ opacity: 1, y: 0 }}\n                                    className={cn(\n                                        \"flex gap-3\",\n                                        message.type === \"user\" ? \"flex-row-reverse\" : \"\"\n                                    )}\n                                >\n                                    <div className={cn(\n                                        \"w-8 h-8 rounded-full flex items-center justify-center shrink-0\",\n                                        message.type === \"bot\"\n                                            ? \"bg-gradient-to-br from-purple-500 to-indigo-500 text-white\"\n                                            : \"bg-slate-200 dark:bg-slate-700 text-slate-600 dark:text-slate-300\"\n                                    )}>\n                                        {message.type === \"bot\" ? <Bot size={ICON_SIZES.sm} /> : <User size={ICON_SIZES.sm} />}\n                                    </div>\n                                    <div className={cn(\n                                        \"max-w-[80%]\",\n                                        message.type === \"user\" ? \"text-right\" : \"\"\n                                    )}>\n                                        <div className={cn(\n                                            \"p-4 rounded-2xl\",\n                                            message.type === \"bot\"\n                                                ? \"bg-white dark:bg-slate-800 border border-slate-100 dark:border-slate-700 rounded-tl-sm\"\n                                                : \"bg-slate-900 text-white rounded-tr-sm\"\n                                        )}>\n                                            <p className=\"text-sm\">{message.content}</p>\n                                        </div>\n                                        {message.options && (\n                                            <div className=\"mt-3 flex flex-wrap gap-2\">\n                                                {message.options.map((option, i) => (\n           