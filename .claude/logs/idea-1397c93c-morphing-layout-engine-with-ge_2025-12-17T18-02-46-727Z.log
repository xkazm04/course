ective[] = [\"split\", \"map\", \"roadmap\", \"skills\"];\n\n/**\n * Get the next view in the cycle\n */\nexport function getNextView(current: ViewPerspective): ViewPerspective {\n    const currentIndex = VIEW_ORDER.indexOf(current);\n    const nextIndex = (currentIndex + 1) % VIEW_ORDER.length;\n    return VIEW_ORDER[nextIndex];\n}\n\n/**\n * Get the previous view in the cycle\n */\nexport function getPreviousView(current: ViewPerspective): ViewPerspective {\n    const currentIndex = VIEW_ORDER.indexOf(current);\n    const prevIndex = (currentIndex - 1 + VIEW_ORDER.length) % VIEW_ORDER.length;\n    return VIEW_ORDER[prevIndex];\n}\n\n/**\n * Get view by keyboard shortcut\n */\nexport function getViewByShortcut(shortcut: string): ViewPerspective | null {\n    const entry = Object.entries(VIEW_CONFIGS).find(([, config]) => config.shortcut === shortcut);\n    return entry ? (entry[0] as ViewPerspective) : null;\n}\n\n/**\n * Interpolate between two rotations\n */\nexport function interpolateRotation(\n    from: Rotation3D,\n    to: Rotation3D,\n    progress: number\n): Rotation3D {\n    return {\n        rotateX: from.rotateX + (to.rotateX - from.rotateX) * progress,\n        rotateY: from.rotateY + (to.rotateY - from.rotateY) * progress,\n        rotateZ: from.rotateZ + (to.rotateZ - from.rotateZ) * progress,\n    };\n}\n\n/**\n * Interpolate between two positions\n */\nexport function interpolatePosition(\n    from: Position3D,\n    to: Position3D,\n    progress: number\n): Position3D {\n    return {\n        x: from.x + (to.x - from.x) * progress,\n        y: from.y + (to.y - from.y) * progress,\n        z: from.z + (to.z - from.z) * progress,\n    };\n}\n\n/**\n * Calculate eased progress value\n */\nexport function easeProgress(t: number, easing: \"spring\" | \"ease-out\" | \"ease-in-out\"): number {\n    switch (easing) {\n        case \"ease-out\":\n            return 1 - Math.pow(1 - t, 3);\n        case \"ease-in-out\":\n            return t < 0.5 ? 4 * t * t