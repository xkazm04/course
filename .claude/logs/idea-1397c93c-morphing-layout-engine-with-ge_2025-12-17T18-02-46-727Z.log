 ring form\n    };\n    // Animation state\n    isAnimating: boolean;\n    // Selection state\n    isSelected: boolean;\n    isHovered: boolean;\n}\n\n/**\n * Connection between nodes in the data space\n */\nexport interface MorphableConnection {\n    id: string;\n    fromId: string;\n    toId: string;\n    type: \"prerequisite\" | \"related\" | \"optional\";\n    // Visibility per view\n    visibleIn: ViewPerspective[];\n}\n\n/**\n * View configuration for each perspective\n */\nexport interface ViewConfig {\n    id: ViewPerspective;\n    name: string;\n    shortcut: string;\n    rotation: Rotation3D;\n    scale: number;\n    // Camera position for this view\n    cameraPosition: Position3D;\n    // Node arrangement function name\n    arrangement: \"grid\" | \"spatial\" | \"linear\" | \"radial\";\n}\n\n/**\n * Gesture event data\n */\nexport interface GestureEvent {\n    type: GestureType;\n    source: GestureSource;\n    velocity: number;\n    delta: { x: number; y: number };\n    timestamp: number;\n}\n\n/**\n * Morphing engine state\n */\nexport interface MorphingEngineState {\n    // Current view perspective\n    currentView: ViewPerspective;\n    // Target view during transition\n    targetView: ViewPerspective | null;\n    // Transition progress (0-1)\n    transitionProgress: number;\n    // Is currently transitioning\n    isTransitioning: boolean;\n    // All morphable nodes\n    nodes: MorphableNode[];\n    // Connections between nodes\n    connections: MorphableConnection[];\n    // Current gesture state\n    gestureState: {\n        isGesturing: boolean;\n        startPosition: { x: number; y: number } | null;\n        currentPosition: { x: number; y: number } | null;\n        source: GestureSource | null;\n    };\n    // Selected node ID\n    selectedNodeId: string | null;\n    // Hovered node ID\n    hoveredNodeId: string | null;\n    // Zoom level\n    zoom: number;\n    // Pan offset\n    pan: { x: number; y: number };\n}\n\n/**\n * View transition configuration\n */\nexport interface TransitionConfig {\n    duration: number;\n    easing: \"spring\" | \"ease-out\" | \"ease-in-out\";\n    stagger: number; // Delay between node transitions\n}\n\n/**\n * Node positions for each view perspective\n */\nexport interface NodePositions {\n    split: Position3D;\n    map: Position3D;\n    roadmap: Position3D;\n    skills: Position3D;\n}\n"}}],"stop_reason":"tool_use","stop_sequence":null,"usage":{"input_tokens":1,"cache_creation_input_tokens":440,"cache_read_input_tokens":59197,"cache_creation":{"ephemeral_5m_input_tokens":440,"ephemeral_1h_input_tokens":0},"output_tokens":1296,"service_tier":"standard"},"context_management":{"applied_edits":[]}},"parent_tool_use_id":null,"session_id":"75a62dd3-c8ca-4318-9290-db1227413dc0","uuid":"13b93d69-6a9a-4c3a-b136-047afffaae92"}
