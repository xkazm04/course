[2025-12-22T17:43:03.273Z] [STDOUT] = createVariantConfig<LandingMode>({\n    48→    id: \"landing\",\n    49→    name: \"Landing Page\",\n    50→    modes: [\n    51→        createMode(\"spatial\", \"Spatial\", \"Light theme with 3D stacked cards\"),\n    52→        createMode(\"dark\", \"Dark\", \"Dark mode mirror of spatial variant\"),\n    53→    ],\n    54→    defaultMode: \"spatial\",\n    55→});\n    56→\n    57→/**\n    58→ * Resolve theme from mode - supports both legacy modes and ThemeCompiler presets.\n    59→ * This demonstrates the UI-as-compilation pattern where themes are not hand-crafted\n    60→ * but programmatically generated from semantic intent.\n    61→ */\n    62→function resolveThemeFromMode(mode: ExtendedLandingMode): ExtendedThemeScheme {\n    63→    // Legacy mode support\n    64→    if (mode === \"spatial\" || mode === \"dark\" || mode === \"light\") {\n    65→        return getExtendedTheme(mode);\n    66→    }\n    67→\n    68→    // For ThemeCompiler presets, compile the theme\n    69→    try {\n    70→        return themeCompiler.compilePreset(mode as ThemePreset);\n    71→    } catch {\n    72→        // Fallback to light theme on invalid preset\n    73→        return getExtendedTheme(\"light\");\n    74→    }\n    75→}\n    76→\n    77→// ============================================================================\n    78→// Shared Data & Constants\n    79→// ============================================================================\n    80→\n    81→const floatingBadges = [\n    82→    { icon: Cpu, text: \"AI Powered\", delay: 0.2, x: \"10%\", y: \"20%\" },\n    83→    { icon: Globe, text: \"Global Access\", delay: 0.6, x: \"80%\", y: \"15%\" },\n    84→    { icon: Hexagon, text: \"Career Ready\", delay: 0.9, x: \"75%\", y: \"80%\" },\n    85→    { icon: Sparkles, text: \"Dynamic\", delay: 1.2, x: \"5%\", y: \"75%\" },\n    86→];\n    87→\n    88→\n    89→// ============================================================================\n    90→// Polymorphic Landing Component\n    91→// ============================================================================\n    92→\n    93→export type LandingPolymorphicProps = Omit<PolymorphicVariantProps<LandingMode>, 'data'> & {\n    94→    /**\n    95→     * Extended mode support - can use either legacy modes (spatial/dark)\n    96→     * or ThemeCompiler presets (midnight, ocean, highContrast, etc.)\n    97→     */\n    98→    extendedMode?: ExtendedLandingMode;\n    99→};\n   100→\n   101→/**\n   102→ * Polymorphic Landing Component\n   103→ *\n   104→ * A single component that can render in multiple modes (spatial/dark).\n   105→ * This replaces the need for separate VariantC and VariantD components.\n   106→ * The mode prop transforms the rendering strategy while maintaining\n   107→ * identical structure, data, and interactions.\n   108→ *\n   109→ * Uses ThemeScheme injection pattern to eliminate isDark ternaries.\n   110→ * Adding new themes (high-contrast, sepia) requires only adding a new\n   111→ * ExtendedThemeScheme definition - no structural changes needed.\n   112→ *\n   113→ * Now supports ThemeCompiler presets for programmatic theme generation:\n   114→ * - highContrast / highContrastDark for accessibility\n   115→ * - protanopia / deuteranopia / tritanopia for colorblind users\n   116→ * - midnight / forest / ocean / sunset for aesthetic themes\n   117→ *\n   118→ * @example\n   119→ * ```tsx\n   120→ * // Legacy mode\n   121→ * <LandingPolymorphic mode=\"dark\" />\n   122→ *\n   123→ * // ThemeCompiler preset\n   124→ * <LandingPolymorphic mode=\"spatial\" extendedMode=\"midnight\" />\n   125→ * ```\n   126→ */\n   127→export const LandingPolymorphic = ({\n   128→    mode,\n   129→    extendedMode,\n   130→    // eslint-disable-next-line @typescript-eslint/no-unused-vars\n   131→    onModeChange,\n   132→    className,\n   133→}: LandingPolymorphicProps) => {\n   134→    // Theme injection - supports both legacy modes and ThemeCompiler presets\n   135→    // When extendedMode is provided, use it; otherwise use legacy mode\n   136→    const effectiveMode = extendedMode ?? mode;\n   137→    const theme = resolveThemeFromMode(effectiveMode);\n   138→    const router = useRouter();\n   139→\n   140→    // Use velocity context for adaptive UI behavior\n   141→    const { adaptations, velocity } = useUserVelocity();\n   142→    const { shouldAnimate, intensity, showDecorations, durationMultiplier } = useVelocityAnimation();\n   143→    const { shouldPrefetchOnHover, shouldPrefetchAll } = useVelocityPrefetch();\n   144→\n   145→    // Detect visibility for lazy loading animations\n   146→    // Pauses gradient animations when page is scrolled away or tab is hidden\n   147→    const [gradientContainerRef, isGradientVisible] = useVisibility<HTMLDivElement>({ threshold: 0.1 });\n   148→\n   149→    // Check for personalized assessment profile to customize floating badges\n   150→    const [personalizedBadges, setPersonalizedBadges] = React.useState(floatingBadges);\n   151→\n   152→    React.useEffect(() => {\n   153→        const profile = assessmentStorage.getProfile();\n   154→        if (profile?.result?.personalizedTags) {\n   155→            const tags = profile.result.personalizedTags;\n   156→            const personalizedIcons = [Target, Award, Rocket, Zap];\n   157→            const newBadges = tags.slice(0, 4).map((tag, index) => ({\n   158→                icon: personalizedIcons[index] || Sparkles,\n   159→                text: tag,\n   160→                delay: 0.2 + index * 0.3,\n   161→                x: floatingBadges[index]?.x || `${10 + index * 20}%`,\n   162→                y: floatingBadges[index]?.y || `${20 + index * 15}%`,\n   163→            }));\n   164→            setPersonalizedBadges(newBadges);\n   165→        }\n   166→    }, []);\n   167→\n   168→    // Prefetch all routes immediately if in exploring mode\n   169→    React.useEffect(() => {\n   170→        if (shouldPrefet