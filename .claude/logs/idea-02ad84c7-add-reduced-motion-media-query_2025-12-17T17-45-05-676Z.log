t: \"Dynamic\", delay: 1.2, x: \"5%\", y: \"75%\" },\n];\n\n// ============================================================================\n// Mode-Aware Sub-Components (Theme Injected)\n// ============================================================================\n\ninterface FloatingBadgeProps {\n    icon: typeof Cpu;\n    text: string;\n    delay: number;\n    x: string;\n    y: string;\n    theme: ExtendedThemeScheme;\n    reducedMotion?: boolean;\n}\n\nconst FloatingBadge = ({ icon: Icon, text, delay, x, y, theme, reducedMotion = false }: FloatingBadgeProps) => {\n    const floatAnimation = useFloatLoop({ delay, xRange: 10, yRange: 10, reducedMotion });\n\n    return (\n        <motion.div\n            {...floatAnimation}\n            className={cn(\n                \"absolute px-4 py-2 backdrop-blur-md rounded-full shadow-lg flex items-center gap-2 text-xs font-semibold z-20 pointer-events-none\",\n                theme.badge.container,\n                theme.badge.text\n            )}\n            style={{ left: x, top: y }}\n            data-testid={`floating-badge-${text.toLowerCase().replace(/\\s+/g, '-')}`}\n        >\n            <Icon size={ICON_SIZES.sm} className={theme.badge.icon} />\n            {text}\n        </motion.div>\n    );\n};\n\n// Main 3D Card - theme injected component\ninterface MainCardProps {\n    theme: ExtendedThemeScheme;\n    reducedMotion?: boolean;\n}\n\nconst MainCard = ({ theme, reducedMotion = false }: MainCardProps) => {\n    const {\n        ref,\n        rotateX,\n        rotateY,\n        contentX,\n        contentY,\n        handleMouseMove,\n        handleMouseLeave,\n    } = use3DParallax({ rotationRange: reducedMotion ? 0 : 15, translationRange: reducedMotion ? 0 : 10 });\n\n    const [hoveredModule, setHoveredModule] = useState<string | null>(null);\n    const router = useRouter();\n\n    const handleModuleHover = (moduleId: string, href: string) => {\n        setHoveredModule(moduleId);\n        router.prefetch(