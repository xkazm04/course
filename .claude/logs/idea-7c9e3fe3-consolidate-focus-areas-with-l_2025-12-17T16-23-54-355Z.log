ll\"\n                                data-testid=\"chat-input\"\n                            />\n                            <button\n                                type=\"submit\"\n                                disabled={!inputValue.trim() || isTyping}\n                                className=\"px-6 bg-slate-900 dark:bg-white text-white dark:text-slate-900 rounded-xl font-bold hover:bg-slate-800 dark:hover:bg-slate-100 transition-colors disabled:opacity-50 disabled:cursor-not-allowed\"\n                                data-testid=\"chat-send-btn\"\n                            >\n                                <Send size={ICON_SIZES.md} />\n                            </button>\n                        </div>\n                    </form>\n                </div>\n            </PrismaticCard>\n        </div>\n    );\n};\n\n// ============================================================================\n// Wizard Mode Renderer\n// ============================================================================\n\ninterface Selection {\n    [key: number]: string;\n}\n\nconst contextHelp: Record<number, string> = {\n    1: \"Define the north star of your learning journey. Be specific!\",\n    2: \"We'll adapt the content format to match how you learn best.\",\n    3: \"This helps us estimate a realistic timeline for you.\",\n};\n\ninterface OptionButtonProps {\n    label: string;\n    onClick: () => void;\n    centered?: boolean;\n    icon?: string;\n    isFirst?: boolean;\n}\n\nconst OptionButton = ({ label, onClick, centered, icon, isFirst }: OptionButtonProps) => {\n    return (\n        <motion.button\n            onClick={onClick}\n            whileHover={{ scale: 1.02 }}\n            whileTap={{ scale: 0.98 }}\n            className={cn(\n                \"group relative p-4 rounded-xl bg-white dark:bg-slate-800 border-2 border-slate-100 dark:border-slate-700 hover:border-indigo-400 transition-all hover:shadow-lg hover:shadow-indigo-100 dark:hover:shadow-indigo-