;\n    deadline: number;\n    focus: string[];\n    learningStyle?: string;\n}\n\nconst defaultFormState: GoalFormState = {\n    goal: \"Become a Full Stack Developer\",\n    timeCommitment: 15,\n    deadline: 6,\n    focus: [\"frontend\", \"backend\"],\n};\n\nconst goalOptions = [\n    \"Get a new job\",\n    \"Upskill current role\",\n    \"Build a side project\",\n    \"Start freelancing\",\n];\n\nconst learningStyles = [\n    { label: \"Video Based\", icon: \"📹\" },\n    { label: \"Text Based\", icon: \"📖\" },\n    { label: \"Project Based\", icon: \"🔨\" },\n    { label: \"Interactive\", icon: \"🎮\" },\n];\n\n// ============================================================================\n// Live Form Mode Renderer\n// ============================================================================\n\ninterface LiveFormModeProps {\n    formState: GoalFormState;\n    onFormChange: (updates: Partial<GoalFormState>) => void;\n}\n\nconst LiveFormMode = ({ formState, onFormChange }: LiveFormModeProps) => {\n    const { goal, timeCommitment, deadline, focus } = formState;\n\n    const toggleFocus = (id: string) => {\n        const newFocus = focus.includes(id)\n            ? focus.filter(f => f !== id)\n            : [...focus, id];\n        onFormChange({ focus: newFocus });\n    };\n\n    const totalHours = timeCommitment * 4 * deadline;\n    const modules = Math.floor(totalHours / 20);\n    const topics = modules * 5;\n\n    return (\n        <div className=\"grid grid-cols-1 lg:grid-cols-2 gap-8\">\n            {/* Input Form */}\n            <div className=\"space-y-6\">\n                <div>\n                    <h2 className=\"text-3xl font-black text-[var(--text-primary)] mb-2\">\n                        Define Your Goal\n                    </h2>\n                    <p className=\"text-[var(--text-secondary)]\">\n                        Tell us where you want to be, and we'll create the roadmap to get you there.\n                    </p>\n                </div>