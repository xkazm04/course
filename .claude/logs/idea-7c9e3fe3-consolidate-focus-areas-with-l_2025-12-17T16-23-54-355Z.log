      {/* Time Commitment Slider */}\n                        <div>\n                            <label className=\"block text-sm font-bold text-[var(--text-secondary)] mb-2\">\n                                <Clock size={ICON_SIZES.sm} className=\"inline mr-2\" />\n                                Hours per Week: <span className=\"text-[var(--accent-primary)]\">{timeCommitment}h</span>\n                            </label>\n                            <input\n                                type=\"range\"\n                                min=\"5\"\n                                max=\"40\"\n                                value={timeCommitment}\n                                onChange={(e) => onFormChange({ timeCommitment: Number(e.target.value) })}\n                                className=\"w-full h-2 bg-[var(--surface-inset)] rounded-full appearance-none cursor-pointer accent-indigo-500\"\n                                data-testid=\"time-commitment-slider\"\n                            />\n                            <div className=\"flex justify-between text-xs text-[var(--text-muted)] mt-1\">\n                                <span>5h (Casual)</span>\n                                <span>40h (Intensive)</span>\n                            </div>\n                        </div>\n\n                        {/* Deadline Slider */}\n                        <div>\n                            <label className=\"block text-sm font-bold text-[var(--text-secondary)] mb-2\">\n                                <Rocket size={ICON_SIZES.sm} className=\"inline mr-2\" />\n                                Target Timeline: <span className=\"text-[var(--accent-primary)]\">{deadline} months</span>\n                            </label>\n                            <input\n                                type=\"range\"\n                                min=\"1\"\n                                max=\"24\"\n                                value={deadline}\n                                onChange={(e) => onFormChange({ deadline: Number(e.target.value) })}\n                                className=\"w-full h-2 bg-[var(--surface-inset)] rounded-full appearance-none cursor-pointer accent-indigo-500\"\n                                data-testid=\"deadline-slider\"\n                            />\n                            <div className=\"flex justify-between text-xs text-[var(--text-muted)] mt-1\">\n                                <span>1 month</span>\n                                <span>24 months</span>\n                            </div>\n                        </div>\n\n                        {/* Focus Areas */}\n                        <div>\n                            <label className=\"block text-sm font-bold text-[var(--text-secondary)] mb-3\">\n                                <Sparkles size={ICON_SIZES.sm} className=\"inline mr-2\" />\n                                Focus Areas\n                            </label>\n                            <div className=\"grid grid-cols-3 gap-2\">\n                                {focusAreas.map(area => (\n                                    <button\n                                        key={area.id}\n                                        onClick={() => toggleFocus(area.id)}\n                                        className={cn(\n                                            \"p-3 rounded-xl border-2 transition-all flex flex-col items-center gap-1\",\n                                            focus.includes(area.id)\n                                                ? \"bg-indigo-50 dark:bg-indigo-900/30 border-[var(--accent-primary)] text-[var(--accent-primary)]\"\n                                                : \"bg-[var(--surface-overlay)] border-[var(--border-strong)] text-[var(--text-secondary)] hover:border-[var(--border-default)]\"\n                                        )}\n                                        data-testid={`focus-area-${area.id}`}\n                       