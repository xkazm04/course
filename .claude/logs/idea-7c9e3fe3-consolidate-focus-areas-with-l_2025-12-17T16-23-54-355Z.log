Response = (userMessage: string) => {\n        setIsTyping(true);\n\n        const userMsg: Message = {\n            id: messages.length + 1,\n            type: \"user\",\n            content: userMessage\n        };\n        setMessages(prev => [...prev, userMsg]);\n\n        setTimeout(() => {\n            let botResponse: Message;\n\n            if (messages.length === 1) {\n                botResponse = {\n                    id: messages.length + 2,\n                    type: \"bot\",\n                    content: `Great choice! \"${userMessage}\" is an exciting goal. How many hours per week can you dedicate to learning?`,\n                    options: [\"5-10 hours\", \"10-20 hours\", \"20-30 hours\", \"30+ hours\"]\n                };\n            } else if (messages.length === 3) {\n                botResponse = {\n                    id: messages.length + 2,\n                    type: \"bot\",\n                    content: `Perfect, ${userMessage} is a solid commitment. What's your current skill level?`,\n                    options: [\"Complete Beginner\", \"Some Basics\", \"Intermediate\", \"Advanced\"]\n                };\n            } else if (messages.length === 5) {\n                botResponse = {\n                    id: messages.length + 2,\n                    type: \"bot\",\n                    content: `Got it! Last question - when do you want to achieve this goal?`,\n                    options: [\"3 months\", \"6 months\", \"12 months\", \"No deadline\"]\n                };\n            } else if (messages.length === 7) {\n                setPathGenerated(true);\n                botResponse = {\n                    id: messages.length + 2,\n                    type: \"bot\",\n                    content: `Excellent! Based on your responses, I've generated a personalized learning path. Here's your roadmap:`\n                };\n            } else {\n                botResponse = {\n                    id: messages.length + 2,\n                    type: \"bot\",\n                    content: \"Thanks for that information! Let me process this...\",\n                };\n            }\n\n            setMessages(prev => [...prev, botResponse]);\n            setIsTyping(false);\n        }, 1500);\n    };\n\n    const handleOptionClick = (option: string) => {\n        simulateResponse(option);\n    };\n\n    const handleSubmit = (e: React.FormEvent) => {\n        e.preventDefault();\n        if (!inputValue.trim()) return;\n        simulateResponse(inputValue);\n        setInputValue(\"\");\n    };\n\n    return (\n        <div className=\"max-w-3xl mx-auto\">\n            <div className=\"text-center mb-8\">\n                <motion.div\n                    initial={{ opacity: 0, y: 20 }}\n                    animate={{ opacity: 1, y: 0 }}\n                    className=\"inline-flex items-center gap-2 px-4 py-1.5 rounded-full bg-purple-50 dark:bg-purple-900/30 text-[var(--accent-secondary)] font-bold tracking-wide text-xs uppercase mb-4\"\n                >\n                    <Bot size={ICON_SIZES.sm} />\n                    AI-Powered Path Generation\n                </motion.div>\n                <h2 className=\"text-3xl font-black text-[var(--text-primary)] mb-2\">\n                    Chat with Your Learning AI\n                </h2>\n                <p className=\"text-[var(--text-secondary)]\">\n                    Have a conversation to discover your perfect learning path\n                </p>\n            </div>\n\n            <PrismaticCard glowColor=\"purple\">\n                <div className=\"flex flex-col h-[600px]\">\n                    {/* Chat Messages */}\n                    <div className=\"flex-1 overflow-y-auto p-6 space-y-4\">\n                        <AnimatePresence>\n                            {messages.map((message) => (\n                                <motion.div\n                                    key={message.id}\n                                    initial={{ opacity: 0, y: 10 }}\n                                    animate={{ opacity: 1, y: 0 }}\n                                    className={cn(\n                                        \"flex gap-3\",\n                                        message.type === \"user\" ? \"flex-row-reverse\" : \"\"\n                                    )}\n                                >\n                                    <div className={cn(\n                                        \"w-8 h-8 rounded-full flex items-center justify-center shrink-0\",\n                                        message.type === \"bot\"\n                                            ? \"bg-gradient-to-br from-purple-500 to-indigo-500 text-white\"\n                                            : \"bg-slate-200 dark:bg-slate-700 text-slate-600 dark:text-slate-300\"\n                                    )}>\n                                        {message.type === \"bot\" ? <Bot size={ICON_SIZES.sm} /> : <User size={ICON_SIZES.sm} />}\n                                    </div>\n                                    <div className={cn(\n                                        \"max-w-[80%]\",\n                                        message.type === \"user\" ? \"text-right\" : \"\"\n                                    )}>\n                                        <div className={cn(\n                                            \"p-4 rounded-2xl\",\n                                            message.type === \"bot\"\n                                                ? \"bg-white dark:bg-slate-800 border border-slate-100 dark:border-slate-700 rounded-tl-sm\"\n                                                : \"bg-slate-900 text-white rounded-tr-sm\"\n                                        )}>\n                                            <p className=\"text-sm\">{message.content}</p>\n                                        </div>\n                                        {message.options && (\n                                            <div className=\"mt-3 flex flex-wrap gap-2\">\n                                                {message.options.map((option, i) => (\n                                                    <button\n                                                        key={i}\n                                                        onClick={() => handleOptionClick(option)}\n                                                        className=\"px-4 py-2 bg-indigo-50 dark:bg-indigo-900/30 hover:bg-indigo-100 dark:hover:bg-indigo-900/50 text-indigo-700 dark:text-indigo-400 rounded-full text-sm font-medium transition-colors\"\n                                                        data-testid={`chat-option-${i}`}\n                                                    >\n                                                        {option}\n                                                    </button>\n                                                ))}\n                                            </div>\n                                        )}\n                                    </div>\n                                </motion.div>\n                            ))}\n                        </AnimatePresence>\n\n                        {isTyping && (\n                            <motion.div\n                                initial={{ opacity: 0 }}\n                                animate={{ opacity: 1 }}\n                                className=\"flex gap-3\"\n                            >\n                                <div className=\"w-8 h-8 rounded-full bg-gradient-to-br from-purple-500 to-indigo-500 text-white flex items-center justify-center\">\n                                    <Bot size={ICON_SIZES.sm} />\n                                </div>\n                                <div className=\"p-4 bg-white dark:bg-slate-800 border border-slate-100 dark:border-slate-700 rounded-2xl rounded-tl-sm\">\n                                    <div className=\"flex gap-1\">\n           