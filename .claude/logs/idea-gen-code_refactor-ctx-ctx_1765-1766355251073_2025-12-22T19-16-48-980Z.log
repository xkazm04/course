deProps {\n   303→    formState: GoalFormState;\n   304→    onFormChange: (updates: Partial<GoalFormState>) => void;\n   305→}\n   306→\n   307→const AIChatMode = ({ formState, onFormChange }: AIChatModeProps) => {\n   308→    // Use the decision tree engine with our goal definition tree\n   309→    const {\n   310→        messages,\n   311→        formState: treeFormState,\n   312→        isComplete,\n   313→        isTyping,\n   314→        selectOption,\n   315→        submitInput,\n   316→        goBack,\n   317→        state,\n   318→    } = useDecisionTree({\n   319→        tree: goalDefinitionTree,\n   320→        onComplete: (finalFormState) => {\n   321→            // Sync the final form state back to the parent\n   322→            onFormChange(finalFormState);\n   323→        },\n   324→        onEvent: (event) => {\n   325→            // Optional: Track events for analytics\n   326→            if (event.type === \"BRANCHING_OCCURRED\") {\n   327→                console.log(`[DecisionTree] Branch: ${event.fromNodeId} -> ${event.toNodeId} (${event.reason})`);\n   328→            }\n   329→        },\n   330→        typingDelay: 1200, // Simulated typing delay for natural feel\n   331→    });\n   332→\n   333→    // Sync form state updates as user progresses\n   334→    useEffect(() => {\n   335→        onFormChange(treeFormState);\n   336→    }, [treeFormState, onFormChange]);\n   337→\n   338→    return (\n   339→        <ChatTreeRenderer\n   340→            messages={messages}\n   341→            isTyping={isTyping}\n   342→            isComplete={isComplete}\n   343→            formState={treeFormState}\n   344→            onSelectOption={selectOption}\n   345→            onSubmitInput={submitInput}\n   346→            onBack={goBack}\n   347→            canGoBack={state.history.length > 0}\n   348→            showResultCard={true}\n   349→        />\n   350→    );\n   351→};\n   352→\n   353→// ============================================================================\n   354→// Wizard Mode Renderer\n   355→// ============================================================================\n   356→\n   357→interface Selection {\n   358→    [key: number]: string;\n   359→}\n   360→\n   361→const contextHelp: Record<number, string> = {\n   362→    1: \"Define the north star of your learning journey. Be specific!\",\n   363→    2: \"We'll adapt the content format to match how you learn best.\",\n   364→    3: \"This helps us estimate a realistic timeline for you.\",\n   365→};\n   366→\n   367→interface OptionButtonProps {\n   368→    label: string;\n   369→    onClick: () => void;\n   370→    centered?: boolean;\n   371→    icon?: string;\n   372→    isFirst?: boolean;\n   373→    prefersReducedMotion?: boolean | null;\n   374→}\n   375→\n   376→const OptionButton = ({ label, onClick, centered, icon, isFirst, prefersReducedMotion }: OptionButtonProps) => {\n   377→    return (\n   378→        <motion.button\n   379→            onClick={onClick}\n   380→            whileHover={prefersReducedMotion ? undefined : { scale: 1.02 }}\n   381→            whileTap={prefersReducedMotion ? undefined : { scale: 0.98 }}\n   382→            className={cn(\n   383→                \"group relative p-4 rounded-xl bg-white dark:bg-slate-800 border-2 border-slate-100 dark:border-slate-700 hover:border-indigo-400 transition-all hover:shadow-lg hover:shadow-indigo-100 dark:hover:shadow-indigo-900/20\",\n   384→                centered ? \"flex flex-col items-center justify-center text-center h-28\" : \"text-left flex items-center justify-between\"\n   385→            )}\n   386→            data-testid={`wizard-option-${label.toLowerCase().replace(/\\s+/g, '-')}`}\n   387→        >\n   388→            {icon && <span className=\"text-3xl mb-2\">{icon}</span>}\n   389→            <span className=\"font-bold text-slate-900 dark:text-white group-hover:text-indigo-600 dark:group-hover:text-indigo-400 transition-colors\">\n   390→                {label}\n   391→            </span>\n   392→            {!centered && (\n   393→                <ArrowRight size={ICON_SIZES.sm} className=\"opacity-0 -translate-x-2 group-hover:opacity-100 group-hover:translate-x-0 transition-all text-indigo-500\" />\n   394→            )}\n   395→            {isFirst && (\n   396→                <span className=\"absolute -top-2 -right-2 px-2 py-0.5 bg-indigo-500 text-white text-[10px] font-bold rounded-full\">\n   397→                    Recommended\n   398→                </span>\n   399→            )}\n   400→        </motion.button>\n   401→    );\n   402→};\n   403→\n   404→/** Helper to determine initial wizard step from form state */\n   405→const getInitialWizardStep = (formState: GoalFormState): number => {\n   406→    // Check if goal was already selected (not default)\n   407→    if (formState.goal && formState.goal !== defaultFormState.goal) {\n   408→        // Check if learning style was selected\n   409→        if (formState.learningStyle) {\n   410→            // Check if time commitment was changed from default\n   411→            if (formState.timeCommitment !== defaultFormState.timeCommitment) {\n   412→                return 4; // All done\n   413→            }\n   414→            return 3; // Time commitment pending\n   415→        }\n   416→        return 2; // Learning style pending\n   417→    }\n   418→    return 1; // Starting from goal\n   419→};\n   420→\n   421→/** Helper to parse time commitment from wizard slider selection */\n   422→const parseWizardTimeCommitment = (selection: string): number => {\n   423→    const match = selection.match(/(\\d+)/);\n   424→    return match ? parseInt(match[1], 10) : 10;\n   425→};\n   426→\n   427→interface WizardModeProps {\n   428→    formState: GoalFormState;\n   429→    onFormChange: (updates: Partial<GoalFormState>) => void;\n   430→}\n   431→\n   432→const WizardMode = ({ formState, onFormChange }: WizardModeProps) => {\n   433→    // Initialize step based 