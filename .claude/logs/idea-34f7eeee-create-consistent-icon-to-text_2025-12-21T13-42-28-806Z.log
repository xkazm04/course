ar List Component - unique visual renderer for VariantB\ninterface SidebarListProps {\n    paths: LearningPath[];\n    selectedPath: LearningPath;\n    onSelectPath: (path: LearningPath) => void;\n    staggerDelay: number;\n}\n\nconst SidebarList = ({ paths, selectedPath, onSelectPath, staggerDelay }: SidebarListProps) => (\n    <div className=\"lg:col-span-4 space-y-2\">\n        <div className=\"mb-6\">\n            <h2 className=\"text-2xl font-bold text-[var(--text-primary)] mb-2\">Select Your Path</h2>\n            <p className=\"text-sm text-[var(--text-secondary)]\">Sorted by learning progression</p>\n        </div>\n\n        {paths.map((path, index) => {\n            const node = getGraphNode(path.id as LearningDomainId);\n            return (\n                <PathCard\n                    key={path.id}\n                    path={path}\n                    variant=\"sidebar\"\n                    isSelected={selectedPath.id === path.id}\n                    onClick={() => onSelectPath(path)}\n                    animationDelay={index * staggerDelay}\n                    badge={node ? HIERARCHY_LABELS[node.hierarchyLevel] : undefined}\n                />\n            );\n        })}\n    </div>\n);\n\n// Main VariantB Component - uses VariantShell for state management\nexport const VariantB = () => {\n    return (\n        <VariantShell<LearningPath>\n            items={sortedPaths}\n            initialSelected={sortedPaths[0]}\n            getItemId={(path) => path.id}\n            animationPreset=\"stagger-fast\"\n        >\n            {(context: VariantShellContext<LearningPath>) => {\n                const { selection, animation } = context;\n                const selectedPath = selection.selected ?? sortedPaths[0];\n\n                return (\n                    <div className=\"grid grid-cols-1 lg:grid-cols-12 gap-6 min-h-[600px]\" data-testid=\"variant-b-container\">\n                        <SidebarList\n                            paths={sortedPaths}\n                            selectedPath={selectedPath}\n                            onSelectPath={selection.setSelected}\n                            staggerDelay={animation.staggerDelay}\n                        />\n                        <div className=\"lg:col-span-8\">\n                            <PreviewPanel\n                                selectedPath={selectedPath}\n                                onSelectPath={selection.setSelected}\n                            />\n                        </div>\n                    </div>\n                );\n            }}\n        </VariantShell>\n    );\n};\n","structuredPatch":[{"oldStart":237,"oldLines":9,"newStart":237,"newLines":10,"lines":["                         <div className=\"mt-auto flex gap-4\">","                             <button","                                 data-testid=\"start-path-btn\"","-                                className=\"flex-1 py-4 bg-[var(--btn-primary-bg)] text-[var(--btn-primary-text)] rounded-2xl font-bold hover:opacity-90 transition-opacity flex items-center justify-center gap-2\"","+                                className=\"flex-1 py-4 bg-[var(--btn-primary-bg)] text-[var(--btn-primary-text)] rounded-2xl font-bold hover:opacity-90 transition-opacity icon-text-align justify-center\"","                             >","-                                Start Path <ArrowRight size={ICON_SIZES.md} />","+                                <span>Start Path</span>","+                                <ArrowRight size={ICON_SIZES.md} data-icon />","                             </button>","                             <button","                                 data-testid=\"preview-path-btn\""]}],"userModified":false,"replaceAll":false}}
