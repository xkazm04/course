rm\": \"Live Form\",\n    \"ai-chat\": \"AI Chat\",\n};\n\nconst MODE_DESCRIPTIONS: Record<GoalPathMode, string> = {\n    \"wizard\": \"Guided step-by-step experience\",\n    \"live-form\": \"All options with live preview\",\n    \"ai-chat\": \"Conversational AI guidance\",\n};\n\n// ============================================================================\n// Types\n// ============================================================================\n\nexport interface GoalPathAdaptiveProps {\n    /** Initial mode (defaults to wizard for progressive disclosure) */\n    initialMode?: GoalPathMode;\n    /** Whether to enable friction detection and escalation suggestions */\n    enableAdaptiveUX?: boolean;\n    /** Whether to show the mode selector in header */\n    showModeSelector?: boolean;\n    /** Callback when mode changes */\n    onModeChange?: (mode: GoalPathMode, reason: \"user\" | \"escalation\") => void;\n    /** Custom className */\n    className?: string;\n}\n\n// ============================================================================\n// Mode Selector Component\n// ============================================================================\n\ninterface ModeSelectorProps {\n    currentMode: GoalPathMode;\n    onModeChange: (mode: GoalPathMode) => void;\n    isOpen: boolean;\n    onToggle: () => void;\n}\n\nconst ModeSelector = ({\n    currentMode,\n    onModeChange,\n    isOpen,\n    onToggle,\n}: ModeSelectorProps) => {\n    const prefersReducedMotion = useReducedMotion();\n\n    return (\n        <div className=\"relative\" data-testid=\"adaptive-mode-selector\">\n            <button\n                onClick={onToggle}\n                className=\"flex items-center gap-2 px-4 py-2.5 min-h-[44px] rounded-lg bg-[var(--surface-inset)] hover:bg-[var(--surface-overlay)] border border-[var(--border-default)] transition-colors text-sm\"\n                data-testid=\"mode-selector-trigger\"\n            >\n                <Settings size={ICON_SIZES.sm} cla