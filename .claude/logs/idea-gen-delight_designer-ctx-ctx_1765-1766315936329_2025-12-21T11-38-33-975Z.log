elers = getTravelersOnPath(selectedPath.id);\n    const pathSquads = getSquadsForPath(selectedPath.id as SquadDomainId);\n    const pathStudyRooms = getStudyRoomsForPath(selectedPath.id as SquadDomainId);\n    const onlineTravelers = pathTravelers.filter(t => t.isOnline);\n\n    const previewAnimation = useVariantAnimation({\n        preset: \"smooth\",\n        direction: \"right\",\n    });\n\n    return (\n        <AnimatePresence mode=\"wait\">\n            <motion.div\n                key={selectedPath.id}\n                initial={previewAnimation.initial}\n                animate={previewAnimation.animate}\n                exit={previewAnimation.exit}\n                transition={previewAnimation.transition}\n            >\n                <PrismaticCard className=\"h-full\" glowColor={getGlowColor(selectedColor)}>\n                    <div className=\"p-8 h-full flex flex-col\">\n                        <PathHeader path={selectedPath} pathColor={selectedColor} />\n\n                        <h2 className=\"text-3xl font-black text-[var(--text-primary)] mb-2\">\n                            {selectedPath.name}\n                        </h2>\n                        <p className=\"text-lg text-[var(--text-secondary)] mb-6\">\n                            {selectedPath.description}\n                        </p>\n\n                        {/* Travelers on this path */}\n                        {onlineTravelers.length > 0 && (\n                            <div className=\"mb-6 p-4 bg-gradient-to-r from-indigo-500/10 to-purple-500/10 rounded-xl\">\n                                <div className=\"flex items-center justify-between\">\n                                    <div className=\"flex items-center gap-3\">\n                                        <TravelerAvatarGroup\n                                            travelers={onlineTravelers}\n                                            maxDisplay={5}\n                                            size=\"sm\"\n                                        />\n                                        <span className=\"text-sm text-[var(--text-secondary)]\">\n                                            <span className=\"font-semibold text-[var(--text-primary)]\">{onlineTravelers.length}</span> travelers online\n                                        </span>\n                                    </div>\n                                    <button\n                                        className=\"px-3 py-1.5 bg-[var(--btn-secondary-bg)] border border-[var(--btn-secondary-border)] text-[var(--btn-secondary-text)] rounded-lg text-sm font-medium hover:bg-[var(--surface-overlay)] transition-colors flex items-center gap-1\"\n                                        data-testid=\"find-squad-btn\"\n                                    >\n                                        <UserPlus size={ICON_SIZES.xs} /> Find Squad\n                                    </button>\n                                </div>\n                            </div>\n                        )}\n\n                        <div className=\"grid grid-cols-3 gap-4 mb-6\">\n                            <div className=\"p-4 bg-[var(--surface-inset)] rounded-xl\" data-testid=\"path-duration-stat\">\n                                <Clock className=\"text-[var(--text-muted)] mb-2\" size={ICON_SIZES.md} />\n                                <div className=\"text-2xl font-bold text-[var(--text-primary)]\">{selectedPath.hours}h</div>\n                                <div className=\"text-xs text-[var(--text-muted)]\">Total Duration</div>\n                            </div>\n                            <div className=\"p-4 bg-[var(--surface-inset)] rounded-xl\" data-testid=\"path-courses-stat\">\n                                <Users className=\"text-[var(--text-muted)] mb-2\" size={ICON_SIZES.md} />\n                                <div className=\"text-2xl font-bold text-[var(--text-primary)]\">{selectedPath.courses}</div>\n                                <div className=\"text-xs text-[var(--text-muted)]\">Courses</div>\n                            </div>\n                            <div className=\"p-4 bg-[var(--surface-inset)] rounded-xl\" data-testid=\"total-courses-stat\">\n                                <TrendingUp className=\"text-[var(--text-muted)] mb-2\" size={ICON_SIZES.md} />\n                                <div className=\"text-2xl font-bold text-[var(--text-primary)]\">{totalCourses}</div>\n                                <div className=\"text-xs text-[var(--text-muted)]\">Total Courses</div>\n                            </div>\n                        </div>\n\n                        <PathStats path={selectedPath} pathColor={selectedColor} />\n\n                        {/* Learning Path Relationships - powered by shared graph data */}\n                        {(prerequisites.length > 0 || leadsTo.length > 0) && (\n                            <div className=\"mt-6 mb-6 p-4 bg-[var(--surface-inset)] rounded-xl\">\n                                <div className=\"flex items-center gap-2 mb-3\">\n                                    <GitBranch size={ICON_SIZES.sm} className=\"text-[var(--text-muted)]\" />\n                                    <span className=\"text-sm font-semibold text-[var(--text-secondary)]\">Learning Path Connections</span>\n                                </div>\n                                <div className=\"grid grid-cols-2 gap-4\">\n                                    {prerequisites.length > 0 && (\n                                        <div>\n                                            <div className=\"text-xs text-[var(--text-muted)] mb-2\">Builds on:</div>\n                                            <div className=\"space-y-1\">\n                                                {prerequisites.map(p => (\n                                                    <button\n                                                        key={p.id}\n                  