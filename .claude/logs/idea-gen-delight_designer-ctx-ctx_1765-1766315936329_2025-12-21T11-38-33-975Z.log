                <div className=\"p-4 text-center\">\n                    <Target size={ICON_SIZES.md} className=\"mx-auto text-cyan-500 mb-2\" />\n                    <div className=\"text-2xl font-bold text-[var(--text-primary)]\">\n                        {squads.reduce((acc, s) => acc + s.goals.filter(g => g.isCompleted).length, 0)}\n                    </div>\n                    <div className=\"text-xs text-[var(--text-muted)]\">Goals Completed</div>\n                </div>\n            </PrismaticCard>\n        </div>\n    );\n};\n\n// Overview Content - Shows travelers and featured squads\nconst OverviewContent = ({ onViewSquad }: { onViewSquad: (squad: Squad) => void }) => {\n    const onlineTravelers = getOnlineTravelers();\n    const featuredSquads = squads.filter(s => s.status === \"active\").slice(0, 2);\n\n    return (\n        <div className=\"grid lg:grid-cols-3 gap-6\">\n            {/* Left Column - Online Travelers */}\n            <div className=\"lg:col-span-1\">\n                <PrismaticCard glowColor=\"indigo\">\n                    <div className=\"p-6\">\n                        <h3 className=\"font-bold text-[var(--text-primary)] mb-4 flex items-center gap-2\">\n                            <Users size={ICON_SIZES.sm} className=\"text-indigo-500\" />\n                            Travelers Online\n                        </h3>\n                        <div className=\"space-y-4\">\n                            {onlineTravelers.map((traveler) => (\n                                <motion.div\n                                    key={traveler.id}\n                                    initial={{ opacity: 0, x: -20 }}\n                                    animate={{ opacity: 1, x: 0 }}\n                                    className=\"flex items-center justify-between p-3 bg-[var(--surface-inset)] rounded-xl\"\n                                    data-testid={`online-traveler-${traveler.id}`}\n                                >\n                    