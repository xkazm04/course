ad Detail View\nconst SquadDetailView = ({ squad, onBack }: { squad: Squad; onBack: () => void }) => (\n    <motion.div\n        initial={{ opacity: 0, x: 50 }}\n        animate={{ opacity: 1, x: 0 }}\n        exit={{ opacity: 0, x: -50 }}\n    >\n        <button\n            onClick={onBack}\n            className=\"mb-4 text-sm text-[var(--text-secondary)] hover:text-[var(--text-primary)] flex items-center gap-1\"\n            data-testid=\"back-to-squads-btn\"\n        >\n            <ChevronRight size={ICON_SIZES.xs} className=\"rotate-180\" /> Back to squads\n        </button>\n        <div className=\"grid lg:grid-cols-2 gap-6\">\n            <SquadCard squad={squad} variant=\"full\" />\n            <SquadProgress squad={squad} />\n        </div>\n    </motion.div>\n);\n\n// Main VariantSquad Component\nexport const VariantSquad = () => {\n    const [viewMode, setViewMode] = useState<ViewMode>(\"overview\");\n    const [selectedSquad, setSelectedSquad] = useState<Squad | null>(null);\n    const [selectedPathId, setSelectedPathId] = useState<string>(\"frontend\");\n\n    const handleViewSquad = (squad: Squad) => {\n        setSelectedSquad(squad);\n        setViewMode(\"squads\");\n    };\n\n    return (\n        <div className=\"space-y-6\" data-testid=\"variant-squad-container\">\n            {/* Header */}\n            <div className=\"flex flex-col sm:flex-row sm:items-center justify-between gap-4\">\n                <div>\n                    <h2 className=\"text-2xl font-bold text-[var(--text-primary)]\">Learning Squad Hub</h2>\n                    <p className=\"text-sm text-[var(--text-secondary)]\">\n                        Connect with fellow learners, join squads, and study together\n                    </p>\n                </div>\n                <ViewTabs active={viewMode} onChange={(mode) => {\n                    setViewMode(mode);\n                    setSelectedSquad(null);\n                }} />\n            </div>\n\n            {/* Stats - 