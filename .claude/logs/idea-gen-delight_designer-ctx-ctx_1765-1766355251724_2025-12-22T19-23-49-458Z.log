otalXp || 0;\n\n    return (\n        <div className=\"space-y-6\" data-testid=\"career-dna-variant\">\n            {/* Header with Career DNA Branding */}\n            <PrismaticCard glowColor=\"indigo\">\n                <div className=\"p-6\">\n                    <div className=\"flex flex-col md:flex-row md:items-center md:justify-between gap-4\">\n                        <div className=\"flex items-center gap-4\">\n                            <motion.div\n                                className=\"w-16 h-16 rounded-2xl flex items-center justify-center\"\n                                style={{\n                                    background: \"linear-gradient(to bottom right, var(--color-indigo), var(--color-purple))\",\n                                }}\n                                whileHover={{ scale: 1.05, rotate: 3 }}\n                            >\n                                <Fingerprint size={32} className=\"text-white\" />\n                            </motion.div>\n                            <div>\n                                <h1 className=\"text-2xl font-bold text-[var(--text-primary)]\">\n                                    Career DNA\n                                </h1>\n                                <p className=\"text-[var(--text-muted)]\">\n                                    Your verified skill passport\n                                </p>\n                            </div>\n                        </div>\n\n                        {/* Quick Stats */}\n                        <div className=\"flex gap-4\">\n                            <div className=\"flex items-center gap-2 px-4 py-2 rounded-xl bg-[var(--surface-inset)]\">\n                                <Zap size={ICON_SIZES.md} style={{ color: \"var(--color-amber)\" }} />\n                                <div>\n                                    <div className=\"text-lg font-bold text-[var(--text-primary)]\">\n                                        {xp.toLocaleString