[2025-12-17T21:26:18.186Z] [STDOUT] mport { ICON_SIZES } from \"@/app/shared/lib/iconSizes\";\n\n// Gamified Career Mapping - adapted from gemi/app career-mapping with Spatial theming\ninterface Achievement {\n    id: number;\n    title: string;\n    progress: number;\n    locked: boolean;\n    reward: string;\n    xp: number;\n    description: string;\n}\n\nconst achievements: Achievement[] = [\n    {\n        id: 1,\n        title: \"Internship Ready\",\n        progress: 100,\n        locked: false,\n        reward: \"Unlock Job Board\",\n        xp: 500,\n        description: \"Complete foundational skills to be internship-ready\"\n    },\n    {\n        id: 2,\n        title: \"Junior Developer\",\n        progress: 65,\n        locked: true,\n        reward: \"Unlock Salary Data\",\n        xp: 1200,\n        description: \"Master core programming concepts and frameworks\"\n    },\n    {\n        id: 3,\n        title: \"Mid-Level Engineer\",\n        progress: 30,\n        locked: true,\n        reward: \"Unlock Client List\",\n        xp: 2000,\n        description: \"Build production-ready applications independently\"\n    },\n    {\n        id: 4,\n        title: \"Senior Developer\",\n        progress: 10,\n        locked: true,\n        reward: \"Unlock Leadership Track\",\n        xp: 3500,\n        description: \"Lead projects and mentor junior developers\"\n    },\n];\n\nconst levelThresholds = [\n    { level: 1, title: \"Novice Explorer\", minXp: 0 },\n    { level: 2, title: \"Code Apprentice\", minXp: 500 },\n    { level: 3, title: \"Digital Craftsman\", minXp: 1500 },\n    { level: 4, title: \"Career Architect\", minXp: 3000 },\n    { level: 5, title: \"Tech Visionary\", minXp: 5000 },\n];\n\nexport const VariantD = () => {\n    const [xp, setXp] = useState(650);\n    const [hoveredId, setHoveredId] = useState<number | null>(null);\n    const [showShareTooltip, setShowShareTooltip] = useState(false);\n    const [isHydrated, setIsHydrated] = useState(false);\n\n    // Handle hydration to show skeleton during initial load\n    useEffect(() => {\n        // Small delay to ensure smooth transition from skeleton to animated bar\n        const timer = setTimeout(() => setIsHydrated(true), 100);\n        return () => clearTimeout(timer);\n    }, []);\n\n    const currentLevel = levelThresholds.reduce((acc, threshold) =>\n        xp >= threshold.minXp ? threshold : acc\n    , levelThresholds[0]);\n\n    const nextLevel = levelThresholds.find(t => t.minXp > xp) || levelThresholds[levelThresholds.length - 1];\n    const progressToNextLevel = nextLevel.minXp > currentLevel.minXp\n        ? ((xp - currentLevel.minXp) / (nextLevel.minXp - currentLevel.minXp)) * 100\n        : 100;\n\n    return (\n        <div className=\"space-y-8\">\n            {/* XP Header Card */}\n            <PrismaticCard glowColor=\"orange\">\n                <div className=\"p-6\">\n                    <div className=\"flex items-center justify-between mb-6\">\n                        <div className=\"flex items-center gap-4\">\n                            <motion.div\n                                className=\"w-16 h-16 rounded-2xl bg-gradient-to-br from-amber-400 to-orange-500 flex items-center justify-center font-black text-2xl text-white shadow-lg shadow-orange-200\"\n                                whileHover={{ scale: 1.05, rotate: 5 }}\n                            >\n                                {currentLevel.level}\n                            </motion.div>\n                            <div>\n                                <h2 className=\"text-xl font-black text-[var(--text-primary)]\">{currentLevel.title}</h2>\n                                <div className=\"flex items-center gap-2 text-sm text-[var(--text-muted)]\">\n                                    <Zap size={ICON_SIZES.sm} className=\"text-amber-500\" />\n                                    <span className=\"font-bold\">{xp.toLocaleString()}</span> / {nextLevel.minXp.toLocaleString()} XP\n                                </div>\n                            </div>\n                        </div>\n\n                        <div className=\"flex items-center gap-3\">\n                            <div className=\"relative\">\n                                <button\n                                    className=\"p-3 bg-[var(--surface-inset)] rounded-xl hover:bg-[var(--surface-overlay)] transition-colors\"\n                                    onMouseEnter={() => setShowShareTooltip(true)}\n                                    onMouseLeave={() => setShowShareTooltip(false)}\n                                    data-testid=\"share-progress-btn\"\n                                >\n                                    <Share2 size={ICON_SIZES.md} className=\"text-[var(--text-secondary)]\" />\n                                </button>\n                                <AnimatePresence>\n                                    {showShareTooltip && (\n                                        <motion.span\n                                            initial={{ opacity: 0, y: 5 }}\n                                            animate={{ opacity: 1, y: 0 }}\n                                            exit={{ opacity: 0 }}\n                                            className=\"absolute top-full right-0 mt-2 text-xs bg-slate-900 text-white px-3 py-1.5 rounded-lg whitespace-nowrap\"\n                                        >\n                                            Share Progress\n                                        </motion.span>\n                                    )}\n                                </AnimatePresence>\n                            </div>\n                            <button\n                                className=\"px-4 py-3 bg-gradient-to-r from-amber-500 to-orange-500 text-white rounded-xl font-bold hover:opacity-90 transition-opacity flex items-center gap-2\"\n                                data-testid=\"leaderboard-btn\"\n                            >\n                                <Trophy size={ICON_SIZES.sm} />\n                                Leaderboard\n                            </button>\n                        </div>\n                    </div>\n\n                    {/* XP Progress Bar */}\n                    <div className=\"space-y-2\">\n                        <div className=\"h-4 bg-[var(--surface-inset)] rounded-full overflow-hidden\" data-testid=\"xp-progress-bar\">\n                            {!isHydrated ? (\n                                /* Shimmer skeleton during hydration */\n                                <div className=\"h-full w-full bg-[var(--surface-inset)] relative overflow-hidden\">\n                                    <div\n                                        className=\"absolute inset-0 bg-gradient-to-r from-transparent via-amber-200/40 dark:via-amber-400/20 to-transparent animate-shimmer\"\n                                        data-testid=\"xp-progress-skeleton\"\n                                    />\n                                </div>\n                            ) : (\n                                <motion.div\n                                    initial={{ width: 0 }}\n                                    animate={{ width: `${progressToNextLevel}%` }}\n                                    transition={{ duration: 1, ease: \"easeOut\" }}\n                                    className=\"h-full bg-gradient-to-r from-amber-400 to-orange-500 relative\"\n                                    data-testid=\"xp-progress-fill\"\n                                >\n                                    <div className=\"absolute inset-0 bg-white/30 animate-pulse\" />\n                                </motion.div>\n                            )}\n                        </div>\n                        <div className=\"flex justify-between text-xs text-[var(--text-muted)]\">\n                            <span>Level {currentLevel.level}</span>\n     