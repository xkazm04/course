em.locked\n                                                        ? \"bg-[var(--surface-inset)] text-[var(--text-muted)]\"\n                                                        : \"bg-gradient-to-br from-emerald-400 to-teal-500 text-white\"\n                                                )}\n                                                whileHover={{ rotate: item.locked ? 0 : 5 }}\n                                            >\n                                                {item.locked\n                                                    ? <Lock size={ICON_SIZES.xl} />\n                                                    : <Unlock size={ICON_SIZES.xl} />\n                                                }\n                                            </motion.div>\n\n                                            {/* Content */}\n                                            <div className=\"flex-1 space-y-2\">\n                                                <div className=\"flex items-center gap-3\">\n                                                    <h3 className=\"text-xl font-bold text-[var(--text-primary)]\">{item.title}</h3>\n                                                    {!item.locked && (\n                                                        <span className=\"px-2 py-0.5 bg-emerald-100 dark:bg-emerald-900/30 text-emerald-700 dark:text-emerald-400 text-xs font-bold rounded-full\">\n                                                            Achieved\n                                                        </span>\n                                                    )}\n                                                </div>\n                                                <p className=\"text-sm text-[var(--text-muted)]\">{item.description}</p>\n                                                <div className=\"flex items-center gap-4 text-sm\">\n                                                    <span className=\"flex items-center gap-1 text-amber-600\">\n                                                        <Star size={ICON_SIZES.sm} /> +{item.xp} XP\n                                                    </span>\n                                                    <span className=\"flex items-center gap-1 text-emerald-600\">\n                                                        <Award size={ICON_SIZES.sm} /> {item.reward}\n                                                    </span>\n                                                </div>\n\n                                                {/* Requirements for locked items */}\n                                                {item.locked && (\n                                                    <div className=\"flex items-center gap-2 text-xs text-[var(--text-muted)] mt-2\">\n                                                        <Shield size={ICON_SIZES.xs} />\n                                                        Requires Level {Math.min(i + 1, 4)} and previous milestone\n                                                    </div>\n                                                )}\n                                            </div>\n\n                                            {/* Progress Section */}\n                                            <div className=\"w-full md:w-48 space-y-2\">\n                                                <div className=\"flex justify-between text-xs font-bold uppercase tracking-wider text-[var(--text-muted)]\">\n                                                    <span>Progress</span>\n                                                    <span>{item.progress}%</span>\n                                                </div>\n                                                <div className=\"h-3 bg-[var(--surface-inset)] rounded-full overflow-hidden\" data-testid={`achievement-progress-bar-${item.id}`}>\n                                                    {!isHydrated ? (\n                                                        /* Shimmer skeleton during hydration */\n                                                        <div className=\"h-full w-full bg-[var(--surface-inset)] relative overflow-hidden\">\n                                                            <div\n                                                                className={cn(\n                                                                    \"absolute inset-0 bg-gradient-to-r from-transparent to-transparent animate-shimmer\",\n                                                                    item.locked\n                                                                        ? \"via-slate-200/40 dark:via-slate-400/20\"\n                                                                        : \"via-emerald-200/40 dark:via-emerald-400/20\"\n                                                                )}\n                                                                data-testid={`achievement-progress-skeleton-${item.id}`}\n                                                            />\n                                                        </div>\n                                                    ) : (\n                                                        <motion.div\n                                                            initial={{ width: 0 }}\n                                                            animate={{ width: `${item.progress}%` }}\n                                                            transition={{ duration: 1.5, ease: \"easeOut\", delay: i * 0.1 }}\n                                                            className={cn(\n                                                                \"h-full rounded-full relative overflow-hidden\",\n                                                                item.locked\n                                                                    ? \"bg-slate-300\"\n                                                                    : \"bg-gradient-to-r from-emerald-400 to-teal-500\"\n                                                            )}\n                                                            data-testid={`achievement-progress-fill-${item.id}`}\n                                                        >\n                                                            {!item.locked && (\n                                                                <motion.div\n                                                                    className=\"absolute inset-0 bg-white/30\"\n                                                                    animate={{ x: [\"-100%\", \"100%\"] }}\n                                                                    transition={{ duration: 2, repeat: Infinity, ease: \"linear\" }}\n                                                                />\n                                                            )}\n                                                        </motion.div>\n                                                    )}\n                                                </div>\n                                            </div>\n                                        </div>\n\n                                        {/* View Certificate Button */}\n                                        {!item.locked && (\n                                            <motion.button\n                                                className=\"absolute bottom-4 right-4 text-xs bg-emerald-50 hover:bg-emerald-100 px-4 py-2 rounded-xl text-emerald-700 font-medium transition-colors flex items-center gap-1\"\n                                                whileHover={{ scale: 1.05 }}\n                                            >\n                                                <Sparkles size={ICON_SIZES.xs} />\n                                                View Certificate\n                                            </motion.button>\n                                    