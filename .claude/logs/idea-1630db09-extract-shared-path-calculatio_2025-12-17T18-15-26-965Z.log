">\n                                {FOCUS_AREAS.map(area => (\n                                    <button\n                                        key={area.id}\n                                        onClick={() => toggleFocus(area.id)}\n                                        className={cn(\n                                            \"p-3 rounded-xl border-2 transition-all flex flex-col items-center gap-1\",\n                                            focus.includes(area.id)\n                                                ? \"bg-indigo-50 dark:bg-indigo-900/30 border-[var(--accent-primary)] text-[var(--accent-primary)]\"\n                                                : \"bg-[var(--surface-overlay)] border-[var(--border-strong)] text-[var(--text-secondary)] hover:border-[var(--border-default)]\"\n                                        )}\n                                        data-testid={`focus-area-${area.id}`}\n                                    >\n                                        <area.icon size={ICON_SIZES.md} />\n                                        <span className=\"text-xs font-medium\">{area.label}</span>\n                                    </button>\n                                ))}\n                            </div>\n                        </div>\n                    </div>\n                </PrismaticCard>\n            </div>\n\n            {/* Live Preview */}\n            <div className=\"space-y-6\">\n                <div className=\"flex items-center gap-2 text-sm font-bold text-[var(--text-muted)]\">\n                    <Zap size={ICON_SIZES.sm} className=\"text-amber-500\" />\n                    LIVE PREVIEW\n                </div>\n\n                <PrismaticCard glowColor=\"purple\">\n                    <div className=\"p-6\">\n                        <div className=\"flex items-center gap-3 mb-6\">\n                            <div className=\"w-12 h-12 bg-gradient-to-br from-indigo-500 to-purple-500 rounded-2xl