700\">\n                        <div className=\"flex gap-3\">\n                            <input\n                                type=\"text\"\n                                value={inputValue}\n                                onChange={(e) => setInputValue(e.target.value)}\n                                placeholder=\"Type your message...\"\n                                className=\"flex-1 p-4 bg-slate-50 dark:bg-slate-800 rounded-xl border border-slate-200 dark:border-slate-700 focus:border-indigo-500 focus:ring-2 focus:ring-indigo-100 dark:focus:ring-indigo-900 outline-none transition-all\"\n                                data-testid=\"chat-input\"\n                            />\n                            <button\n                                type=\"submit\"\n                                disabled={!inputValue.trim() || isTyping}\n                                className=\"px-6 bg-slate-900 dark:bg-white text-white dark:text-slate-900 rounded-xl font-bold hover:bg-slate-800 dark:hover:bg-slate-100 transition-colors disabled:opacity-50 disabled:cursor-not-allowed\"\n                                data-testid=\"chat-send-btn\"\n                            >\n                                <Send size={ICON_SIZES.md} />\n                            </button>\n                        </div>\n                    </form>\n                </div>\n            </PrismaticCard>\n        </div>\n    );\n};\n\n// ============================================================================\n// Wizard Mode Renderer\n// ============================================================================\n\ninterface Selection {\n    [key: number]: string;\n}\n\nconst contextHelp: Record<number, string> = {\n    1: \"Define the north star of your learning journey. Be specific!\",\n    2: \"We'll adapt the content format to match how you learn best.\",\n    3: \"This helps us estimate a realistic timeline for you.\",\n};\n\ninterface OptionButtonProps {\n    label: string;\n    onClick: () => void;\n    centered?: boolean;\n    icon?: string;\n    isFirst?: boolean;\n}\n\nconst OptionButton = ({ label, onClick, centered, icon, isFirst }: OptionButtonProps) => {\n    return (\n        <motion.button\n            onClick={onClick}\n            whileHover={{ scale: 1.02 }}\n            whileTap={{ scale: 0.98 }}\n            className={cn(\n                \"group relative p-4 rounded-xl bg-white dark:bg-slate-800 border-2 border-slate-100 dark:border-slate-700 hover:border-indigo-400 transition-all hover:shadow-lg hover:shadow-indigo-100 dark:hover:shadow-indigo-900/20\",\n                centered ? \"flex flex-col items-center justify-center text-center h-28\" : \"text-left flex items-center justify-between\"\n            )}\n            data-testid={`wizard-option-${label.toLowerCase().replace(/\\s+/g, '-')}`}\n        >\n            {icon && <span className=\"text-3xl mb-2\">{icon}</span>}\n            <span className=\"font-bold text-slate-900 dark:text-white group-hover:text-indigo-600 dark:group-hover:text-indigo-400 transition-colors\">\n                {label}\n            </span>\n            {!centered && (\n                <ArrowRight size={ICON_SIZES.sm} className=\"opacity-0 -translate-x-2 group-hover:opacity-100 group-hover:translate-x-0 transition-all text-indigo-500\" />\n            )}\n            {isFirst && (\n                <span className=\"absolute -top-2 -right-2 px-2 py-0.5 bg-indigo-500 text-white text-[10px] font-bold rounded-full\">\n                    Recommended\n                </span>\n            )}\n        </motion.button>\n    );\n};\n\nconst WizardMode = () => {\n    const [step, setStep] = useState(1);\n    const [history, setHistory] = useState<number[]>([]);\n    const [selections, setSelections] = useState<Selection>({});\n    const [timeCommitment, setTimeCommitment] = useState(10);\n    const [showConfetti, setShowConfetti] = useState(false);\n\n    // Use React Context for friction tracking instead of window global\n    const { trackBackNavigation } = useFrictionTracking();\n\n    // Calculate estimated results based on timeCommitment (same logic as LiveFormMode)\n    // Assuming a 6-month default timeline (24 weeks)\n    const defaultWeeks = 24;\n    const totalHours = timeCommitment * defaultWeeks;\n    const estimatedWeeks = Math.max(4, Math.floor(totalHours / timeCommitment)); // weeks = totalHours / weekly hours\n    const modules = Math.max(1, Math.floor(totalHours / 20));\n    const lessons = modules * 4; // ~4 lessons per module\n    const projects = Math.max(1, Math.floor(modules / 2)); // 1 project per 2 modules\n\n    const nextStep = useCallback((selection: string) => {\n        setSelections(prev => ({ ...prev, [step]: selection }));\n        setHistory(prev => [...prev, step]);\n        setStep(prev => prev + 1);\n    }, [step]);\n\n    const prevStep = useCallback(() => {\n        const last = history[history.length - 1];\n        if (last) {\n            setStep(last);\n            setHistory(prev => prev.slice(0, -1));\n            // Track back navigation for friction detection via React Context\n            trackBackNavigation();\n        }\n    }, [history, trackBackNavigation]);\n\n    const handleFinish = useCallback(() => {\n        setShowConfetti(true);\n    }, []);\n\n    // Keyboard navigation\n    useEffect(() => {\n        const handleKeyDown = (e: KeyboardEvent) => {\n            if (e.key === \"Enter\" && step < 4) {\n                if (step === 1) nextStep(goalOptions[0]);\n                if (step === 2) nextStep(learningStyles[0].label);\n                if (step === 3) nextStep(`${timeCommitment} hours/week`);\n            }\n            if (e.key === \"Escape\" && step > 1) {\n                prevStep();\n            }\n        };\n\n        window.addEventListener(\"keydown\", handleKeyDown);\n        return () => window.removeEventListener(\"keydown\", handleKeyDown);\n    }, [step, timeCommitment, nextStep, prevStep]);\n\n    return (\n        <div className=\"max-w-2xl mx-auto\">\n            {/* Animated Progress Bar */}\n            <div className=\"mb-8\">\n                <div className=\"flex justify-between text-xs text-slate-500 mb-2 uppercase tracking-widest font-bold\">\n                    <span className={cn(step >= 1 && \"text-indigo-600 dark:text-indigo-400\")}>Goal</span>\n                    <span className={cn(step >= 2 && \"text-indigo-600 dark:text-indigo-400\")}>Style</span>\n                    <span className={cn(step >= 3 && \"text-indigo-600 dark:text-indigo-400\")}>Time</span>\n                    <span className={cn(step >= 4 && \"text-indigo-600 dark:text-indigo-400\")}>Done</span>\n                </div>\n                <div className=\"h-2 bg-slate-100 dark:bg-slate-800 rounded-full overflow-hidden\">\n                    <motion.div\n                        className=\"h-full bg-gradient-to-r from-indigo-500 via-purple-500 to-cyan-500 rounded-full\"\n                        initial={{ width: 0 }}\n                        animate={{ width: `${(step / 4) * 100}%` }}\n                        transition={{ duration: 0.5, ease: \"easeInOut\" }}\n                    />\n                </div>\n            </div>\n\n            <PrismaticCard className=\"min-h-[450px] relative overflow-visible\">\n                <div className=\"p-8 relative\">\n                    {/* Auto-save indicator */}\n                    <div className=\"absolute top-6 right-6 flex items-center gap-2 text-xs text-slate-400\">\n                        <Save size={ICON_SIZES.xs} /> Auto-saved\n                    </div>\n\n                    {/* Back Button */}\n                    {step > 1 && step < 4 && (\n                        <button\n                            onClick={prevStep}\n                            className=\"absolute top-6 left-6 text-slate-400 hover:text-slate-700 dark:hover:text-slate-300 flex items-center gap-1 text-sm font-medium transition-colors\"\n                            data-testid=\"wizard-back-btn\"\n                        >\n                            <ChevronLeft size={ICON_SIZES.sm} /> Back\n                        </button>\n                    )}\n\n                    <AnimatePresence mode=\"wait\">\n                        <motion.div\n                            key={step}\n                            initial={{ opacity: 0, x: 20 }}\n                            animate={{ opacity: 1, x: 0 }}\n                            exit={{ opacity: 0, x: -20 }}\n                            transition={{ duration: 0.3 }}\n                            className=\"h-full flex flex-col justify-center pt-8\"\n                        >\n                            {step === 1 && (\n                                <>\n                                    <div className=\"flex items-center gap-2 mb-2\">\n                                        <Target size={ICON_SIZES.md} className=\"text-[var(--accent-primary)]\" />\n                                        <h2 className=\"text-2xl font-black text-[var(--text-primary)]\">What's your primary goal?</h2>\n                                    </div>\n                                    <p className=\"text-[var(--text-muted)] mb-6 flex items-center gap-2\">\n                                        <Info size={ICON_SIZES.sm} />\n                                        {contextHelp[1]}\n                                    </p>\n                                    <div className=\"grid gap-3\">\n                                        {goalOptions.map((opt, i) => (\n                                            <OptionButton\n                                                key={opt}\n                                                label={opt}\n                                                onClick={() => nextStep(opt)}\n                                                isFirst={i === 0}\n                                            />\n                                        ))}\n                                    </div>\n                                </>\n                            )}\n\n                            {step === 2 && (\n                                <>\n                                    <div className=\"flex items-center gap-2 mb-2\">\n                                        <BookOpen size={ICON_SIZES.md} className=\"text-purple-500\" />\n                                        <h2 className=\"text-2xl font-black text-slate-900 dark:text-white\">How do you learn best?</h2>\n                                    </div>\n                                    <p className=\"text-slate-500 mb-6 flex items-center gap-2\">\n                                        <Info size={ICON_SIZES.sm} />\n                                        {contextHelp[2]}\n                                    </p>\n                                    <div className=\"grid grid-cols-2 gap-4\">\n                                        {learningStyles.map((style, i) => (\n                                            <OptionButton\n                                                key={style.label}\n                                                label={style.label}\n                                                onClick={() => nextStep(style.label)}\n                                                centered\n                                                icon={style.icon}\n                                                isFirst={i === 0}\n                                            />\n                                        ))}\n                                    </div>\n                                </>\n                            )}\n\n                            {step === 3 && (\n                                <>\n                                    <div className=\"flex items-center gap-2 mb-2\">\n                                        <Clock size={ICON_SIZES.md} className=\"text-cyan-500\" />\n                                        <h2 c