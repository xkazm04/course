e=\"px-6 bg-slate-900 dark:bg-white text-white dark:text-slate-900 rounded-xl font-bold hover:bg-slate-800 dark:hover:bg-slate-100 transition-colors disabled:opacity-50 disabled:cursor-not-allowed\"\n   521→                                data-testid=\"chat-send-btn\"\n   522→                            >\n   523→                                <Send size={ICON_SIZES.md} />\n   524→                            </button>\n   525→                        </div>\n   526→                    </form>\n   527→                </div>\n   528→            </PrismaticCard>\n   529→        </div>\n   530→    );\n   531→};\n   532→\n   533→// ============================================================================\n   534→// Wizard Mode Renderer\n   535→// ============================================================================\n   536→\n   537→interface Selection {\n   538→    [key: number]: string;\n   539→}\n   540→\n   541→const contextHelp: Record<number, string> = {\n   542→    1: \"Define the north star of your learning journey. Be specific!\",\n   543→    2: \"We'll adapt the content format to match how you learn best.\",\n   544→    3: \"This helps us estimate a realistic timeline for you.\",\n   545→};\n   546→\n   547→interface OptionButtonProps {\n   548→    label: string;\n   549→    onClick: () => void;\n   550→    centered?: boolean;\n   551→    icon?: string;\n   552→    isFirst?: boolean;\n   553→}\n   554→\n   555→const OptionButton = ({ label, onClick, centered, icon, isFirst }: OptionButtonProps) => {\n   556→    return (\n   557→        <motion.button\n   558→            onClick={onClick}\n   559→            whileHover={{ scale: 1.02 }}\n   560→            whileTap={{ scale: 0.98 }}\n   561→            className={cn(\n   562→                \"group relative p-4 rounded-xl bg-white dark:bg-slate-800 border-2 border-slate-100 dark:border-slate-700 hover:border-indigo-400 transition-all hover:shadow-lg hover:shadow-indigo-100 dark:hover:shadow-indigo-900/2