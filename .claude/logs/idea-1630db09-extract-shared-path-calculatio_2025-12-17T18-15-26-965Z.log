                          initial={{ opacity: 0, x: -10 }}\n                                    animate={{ opacity: 1, x: 0 }}\n                                    transition={{ delay: i * 0.1 }}\n                                    className=\"p-3 bg-[var(--surface-overlay)] rounded-xl border border-[var(--border-strong)] flex items-center gap-3\"\n                                >\n                                    <div className=\"w-8 h-8 bg-indigo-100 dark:bg-indigo-900/50 text-[var(--accent-primary)] rounded-lg flex items-center justify-center text-sm font-bold\">\n                                        {i + 1}\n                                    </div>\n                                    <span className=\"font-medium text-[var(--text-secondary)] capitalize\">\n                                        {f} Development\n                                    </span>\n                                    <span className=\"ml-auto text-xs text-[var(--text-muted)]\">\n                                        ~{hoursPerFocusArea}h\n                                    </span>\n                                </motion.div>\n                            ))}\n                        </div>\n\n                        <div className=\"p-4 bg-gradient-to-r from-indigo-50 to-purple-50 dark:from-indigo-900/30 dark:to-purple-900/30 rounded-xl mb-6\">\n                            <div className=\"flex items-center gap-2 text-sm font-bold text-[var(--accent-primary)] mb-2\">\n                                <BarChart3 size={ICON_SIZES.sm} />\n                                Estimated Outcomes\n                            </div>\n                            <div className=\"grid grid-cols-2 gap-4 text-sm\">\n                                <div className=\"flex items-center gap-2 text-[var(--text-secondary)]\">\n                                    <Users size={ICON_SIZES.sm} />\n                                    Job Ready: {isJobReady ? \"Yes\" : \"Advanced\"}\n                                </div>\n                                <div className=\"flex items-center gap-2 text-[var(--text-secondary)]\">\n                                    <BookOpen size={ICON_SIZES.sm} />\n                                    Skill Level: {skillLevel}\n                                </div>\n                            </div>\n                        </div>\n\n                        <button\n                            className=\"w-full py-4 bg-[var(--btn-primary-bg)] text-[var(--btn-primary-text)] rounded-xl font-bold hover:opacity-90 transition-opacity flex items-center justify-center gap-2\"\n                            data-testid=\"generate-path-btn\"\n                        >\n                            Generate Full Path <ArrowRight size={ICON_SIZES.md} />\n                        </button>\n                    </div>\n                </PrismaticCard>\n            </div>\n        </div>\n    );\n};\n\n// ============================================================================\n// AI Chat Mode Renderer\n// ============================================================================\n\ninterface Message {\n    id: number;\n    type: \"bot\" | \"user\";\n    content: string;\n    options?: string[];\n}\n\nconst initialMessages: Message[] = [\n    {\n        id: 1,\n        type: \"bot\",\n        content: \"Hi! I'm your AI learning assistant. I'll help you create a personalized learning path. What's your ultimate career goal?\",\n        options: [\n            \"Become a Backend Developer\",\n            \"Pass AWS Certification\",\n            \"Build a SaaS Product\",\n            \"Let me type my own goal...\"\n        ]\n    }\n];\n\nconst AIChatMode = () => {\n    const [messages, setMessages] = useState<Message[]>(initialMessages);\n    const [inputValue, setInputValue] = useState(\"\");\n    const [isTyping, setIsTyping] = useState(false);\n    const [pathGenerated, setPathGenerated] = useState(false);\n\n    const simulateResponse = (userMessage: string) => {\n        setIsTyping(true);\n\n        const userMsg: Message = {\n            id: messages.length + 1,\n            type: \"user\",\n            content: userMessage\n        };\n        setMessages(prev => [...prev, userMsg]);\n\n        setTimeout(() => {\n            let botResponse: Message;\n\n            if (messages.length === 1) {\n                botResponse = {\n                    id: messages.length + 2,\n                    type: \"bot\",\n                    content: `Great choice! \"${userMessage}\" is an exciting goal. How many hours per week can you dedicate to learning?`,\n                    options: [\"5-10 hours\", \"10-20 hours\", \"20-30 hours\", \"30+ hours\"]\n                };\n            } else if (messages.length === 3) {\n                botResponse = {\n                    id: messages.length + 2,\n                    type: \"bot\",\n                    content: `Perfect, ${userMessage} is a solid commitment. What's your current skill level?`,\n                    options: [\"Complete Beginner\", \"Some Basics\", \"Intermediate\", \"Advanced\"]\n                };\n            } else if (messages.length === 5) {\n                botResponse = {\n                    id: messages.length + 2,\n                    type: \"bot\",\n                    content: `Got it! Last question - when do you want to achieve this goal?`,\n                    options: [\"3 months\", \"6 months\", \"12 months\", \"No deadline\"]\n                };\n            } else if (messages.length === 7) {\n                setPathGenerated(true);\n                botResponse = {\n                    id: messages.length + 2,\n                    type: \"bot\",\n                    content: `Excellent! Based on your responses, I've generated a personalized learning path. Here's your roadmap:`\n                };\n            } else {\n                botResponse = {\n                    id: messages.length + 2,\n                    type: \"bot\",\n                    content: \"Thanks for that information! Let me process this...\",\n                };\n            }\n\n            setMessages(prev => [...prev, botResponse]);\n            setIsTyping(false);\n        }, 1500);\n    };\n\n    const handleOptionClick = (option: string) => {\n        simulateResponse(option);\n    };\n\n    const handleSubmit = (e: React.FormEvent) => {\n        e.preventDefault();\n        if (!inputValue.trim()) return;\n        simulateResponse(inputValue);\n        setInputValue(\"\");\n    };\n\n    return (\n        <div className=\"max-w-3xl mx-auto\">\n            <div className=\"text-center mb-8\">\n                <motion.div\n                    initial={{ opacity: 0, y: 20 }}\n                    animate={{ opacity: 1, y: 0 }}\n                    className=\"inline-flex items-center gap-2 px-4 py-1.5 rounded-full bg-purple-50 dark:bg-purple-900/30 text-[var(--accent-secondary)] font-bold tracking-wide text-xs uppercase mb-4\"\n                >\n                    <Bot size={ICON_SIZES.sm} />\n                    AI-Powered Path Generation\n                </motion.div>\n                <h2 className=\"text-3xl font-black text-[var(--text-primary)] mb-2\">\n                    Chat with Your Learning AI\n                </h2>\n                <p className=\"text-[var(--text-secondary)]\">\n                    Have a conversation to discover your perfect learning path\n                </p>\n            </div>\n\n            <PrismaticCard glowColor=\"purple\">\n                <div className=\"flex flex-col h-[600px]\">\n                    {/* Chat Messages */}\n                    <div className=\"flex-1 overflow-y-auto p-6 space-y-4\">\n                        <AnimatePresence>\n                            {messages.map((message) => (\n                                <motion.div\n                                    key={message.id}\n                                    initial={{ opacity: 0, y: 10 }}\n                                    animate={{ opacity: 1, y: 0 }}\n                                    className={cn(\n                                        \"flex gap-3\",\n                                        message.type === \"user\" ? \"flex-row-reverse\" : \"\"\n                                    )}\n                                >\n                                    <div className={cn(\n                                        \"w-8 h-8 rounded-full flex items-center justify-center shrink-0\",\n                                        message.type === \"bot\"\n                                            ? \"bg-gradient-to-br from-purple-500 to-indigo-500 text-white\"\n                                            : \"bg-slate-200 dark:bg-slate-700 text-slate-600 dark:text-slate-300\"\n                                    )}>\n                                        {message.type === \"bot\" ? <Bot size={ICON_SIZES.sm} /> : <User size={ICON_SIZES.sm} />}\n                                    </div>\n                                    <div className={cn(\n                                        \"max-w-[80%]\",\n                                        message.type === \"user\" ? \"text-right\" : \"\"\n                                    )}>\n                                        <div className={cn(\n                                            \"p-4 rounded-2xl\",\n                                            message.type === \"bot\"\n                                                ? \"bg-white dark:bg-slate-800 border border-slate-100 dark:border-slate-700 rounded-tl-sm\"\n                                                : \"bg-slate-900 text-white rounded-tr-sm\"\n                                        )}>\n                                            <p className=\"text-sm\">{message.content}</p>\n                                        </div>\n                                        {message.options && (\n                                            <div className=\"mt-3 flex flex-wrap gap-2\">\n                                                {message.options.map((option, i) => (\n                                                    <button\n                                                        key={i}\n                                                        onClick={() => handleOptionClick(option)}\n                                                        className=\"px-4 py-2 bg-indigo-50 dark:bg-indigo-900/30 hover:bg-indigo-100 dark:hover:bg-indigo-900/50 text-indigo-700 dark:text-indigo-400 rounded-full text-sm font-medium transition-colors\"\n                                                        data-testid={`chat-option-${i}`}\n                                                    >\n                                                        {option}\n                                                    </button>\n                                                ))}\n                                            </div>\n                                        )}\n                                    </div>\n                                </motion.div>\n                            ))}\n                        </AnimatePresence>\n\n                        {isTyping && (\n                            <motion.div\n                                initial={{ opacity: 0 }}\n                                animate={{ opacity: 1 }}\n                                className=\"flex gap-3\"\n                            >\n                                <div className=\"w-8 h-8 rounded-full bg-gradient-to-br from-purple-500 to-indigo-500 text-white flex items-center justify-center\">\n                                    <Bot size={ICON_SIZES.sm} />\n                                </div>\n                                <div className=\"p-4 bg-white dark:bg-slate-800 border border-slate-100 dark:border-slate-700 rounded-2xl rounded-tl-sm\">\n                                    <div className=\"flex gap-1\">\n                                        <span className=\"w-2 h-2 bg-slate-300 dark:bg-slate-600 rounded-full animate-bounce\" style={{ animationDelay: \"0ms\" }} />\n                                        <span className=\"w-2 h-2 bg-slate-300 dark:bg-slate-600 rounded-full animate-bounce\" style={{ animationDelay: \"150ms\" }} />\n                                        <span className=\"w-2 h-2 bg-slate-300 dark:bg-slate-600 rounded-full animate-bounce\" style={{ animationDelay: \"300ms\" }} />\n                                    </div>\n                                </div>\n                            </motion.div>\n                        )}\n\n                        {/* Generated Path Card */}\n                        {pathGenerated && (\n                            <motion.div\n                                initial={{ opacity: 0, scale: 0.95 }}\n                                animate={{ opacity: 1, scale: 1 }}\n                                className=\"mt-4 p-6 bg-gradient-to-br from-indigo-500 to-purple-600 rounded-2xl text-white\"\n                            >\n                                <div className=\"flex items-center gap-2 mb-4\">\n                                    <Sparkles size={ICON_SIZES.md} />\n                                    <h4 className=\"font-bold\">Your Personalized Path</h4>\n                                </div>\n                                <div className=\"space-y-3\">\n                                    {[\n                                        { title: \"Foundations\", duration: \"4 weeks\" },\n                                        { title: \"Core Skills\", duration: \"8 weeks\" },\n                                        { title: \"Advanced Concepts\", duration: \"6 weeks\" },\n                                        { title: \"Projects & Portfolio\", duration: \"4 weeks\" },\n                                    ].map((item, i) => (\n                                        <div key={i} className=\"flex items-center gap-3 p-3 bg-white/10 rounded-xl\">\n                                            <CheckCircle2 size={ICON_SIZES.md} />\n                                            <span className=\"flex-1 font-medium\">{item.title}</span>\n                                            <span className=\"text-sm text-white/70\">{item.duration}</span>\n                                        </div>\n                                    ))}\n                                </div>\n                                <button\n                                    className=\"w-full mt-4 py-3 bg-white text-indigo-600 rounded-xl font-bold hover:bg-white/90 transition-colors flex items-center justify-center gap-2\"\n                                    data-testid=\"start-journey-btn\"\n                                >\n                                    Start My Journey <ArrowRight size={ICON_SIZES.md} />\n                                </button>\n                            </motion.div>\n                        )}\n                    </div>\n\n                    {/* Input Form */}\n                    <form onSubmit={handleSubmit} className=\"p-4 border-t border-slate-100 dark:border-slate-700\">\n                        <div className=\"flex gap-3\">\n                            <input\n                                type=\"text\"\n                                value={inputValue}\n                                onChange={(e) => setInputValue(e.target.value)}\n                                placeholder=\"Type your message...\"\n                                className=\"flex-1 p-4 bg-slate-50 dark:bg-slate-800 rounded-xl border border-slate-200 dark:border-slate-700 focus:border-indigo-500 focus:ring-2 focus:ring-indigo-100 dark:focus:ring-indigo-900 outline-none transition-all\"\n                                data-testid=\"chat-input\"\n                            />\n                            <button\n                                type=\"submit\"\n                                disabled={!inputValue.trim() || isTyping}\n                                className=\"px-6 bg-slate-900 dark:bg-white text-white dark:text-slate-900 rounded-xl font-bold hover:bg-slate-800 dark:hover:bg-slate-100 transition-colors disabled:opacity-50 disabled:cursor-not-allowed\"\n                                data-testid=\"chat-send-btn\"\n                            >\n                                <Send size={ICON_SIZES.md} />\n                            </button>\n                        </div>\n                    </form>\n                </div>\n            </PrismaticCard>\n        </div>\n    );\n};\n\n// ============================================================================\n// Wizard Mode Renderer\n// ============================================================================\n\ninterface Selection {\n    [key: number]: string;\n}\n\nconst contextHelp: Record<number, string> = {\n    1: \"Define the north star of your learning journey. Be specific!\",\n    2: \"We'll adapt the content format to match how you learn best.\",\n    3: \"This helps us estimate a realistic timeline for you.\",\n};\n\ninterface OptionButtonProps {\n    label: string;\n    onClick: () => void;\n    centered?: boolean;\n    icon?: string;\n    isFirst?: boolean;\n}\n\nconst OptionButton = ({ label, onClick, centered, icon, isFirst }: OptionButtonProps) => {\n    return (\n        <motion.button\n            onClick={onClick}\n            whileHover={{ scale: 1.02 }}\n            whileTap={{ scale: 0.98 }}\n            className={cn(\n                \"group relative p-4 rounded-xl bg-white dark:bg-slate-800 border-2 border-slate-100 dark:border-slate-700 hover:border-indigo-400 transition-all hover:shadow-lg hover:shadow-indigo-100 dark:hover:shadow-indigo-900/20\",\n                centered ? \"flex flex-col items-center justify-center text-center h-28\" : \"text-left flex items-center justify-between\"\n            )}\n            data-testid={`wizard-option-${label.toLowerCase(