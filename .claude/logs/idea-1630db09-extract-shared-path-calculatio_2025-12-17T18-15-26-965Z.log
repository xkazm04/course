h.floor(totalHours / timeCommitment)); // weeks = totalHours / weekly hours\n   598→    const modules = Math.max(1, Math.floor(totalHours / 20));\n   599→    const lessons = modules * 4; // ~4 lessons per module\n   600→    const projects = Math.max(1, Math.floor(modules / 2)); // 1 project per 2 modules\n   601→\n   602→    const nextStep = useCallback((selection: string) => {\n   603→        setSelections(prev => ({ ...prev, [step]: selection }));\n   604→        setHistory(prev => [...prev, step]);\n   605→        setStep(prev => prev + 1);\n   606→    }, [step]);\n   607→\n   608→    const prevStep = useCallback(() => {\n   609→        const last = history[history.length - 1];\n   610→        if (last) {\n   611→            setStep(last);\n   612→            setHistory(prev => prev.slice(0, -1));\n   613→            // Track back navigation for friction detection via React Context\n   614→            trackBackNavigation();\n   615→        }\n   616→    }, [history, trackBackNavigation]);\n   617→\n   618→    const handleFinish = useCallback(() => {\n   619→        setShowConfetti(true);\n   620→    }, []);\n   621→\n   622→    // Keyboard navigation\n   623→    useEffect(() => {\n   624→        const handleKeyDown = (e: KeyboardEvent) => {\n   625→            if (e.key === \"Enter\" && step < 4) {\n   626→                if (step === 1) nextStep(goalOptions[0]);\n   627→                if (step === 2) nextStep(learningStyles[0].label);\n   628→                if (step === 3) nextStep(`${timeCommitment} hours/week`);\n   629→            }\n   630→            if (e.key === \"Escape\" && step > 1) {\n   631→                prevStep();\n   632→            }\n   633→        };\n   634→\n   635→        window.addEventListener(\"keydown\", handleKeyDown);\n   636→        return () => window.removeEventListener(\"keydown\", handleKeyDown);\n   637→    }, [step, timeCommitment, nextStep, prevStep]);\n   638→\n   639→    return (\n   640→        <div className=\"max-w-2xl