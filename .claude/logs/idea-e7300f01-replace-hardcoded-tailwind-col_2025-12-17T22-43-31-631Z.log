sName=\"flex items-center gap-4 text-sm\">\n                                                    <span className=\"flex items-center gap-1 text-amber-600\">\n                                                        <Star size={ICON_SIZES.sm} /> +{item.xp} XP\n                                                    </span>\n                                                    <span className=\"flex items-center gap-1 text-emerald-600\">\n                                                        <Award size={ICON_SIZES.sm} /> {item.reward}\n                                                    </span>\n                                                </div>\n\n                                                {/* Requirements for locked items */}\n                                                {item.locked && (\n                                                    <div className=\"flex items-center gap-2 text-xs text-[var(--text-muted)] mt-2\">\n                                                        <Shield size={ICON_SIZES.xs} />\n                                                        Requires Level {Math.min(i + 1, 4)} and previous milestone\n                                                    </div>\n                                                )}\n                                            </div>\n\n                                            {/* Progress Section */}\n                                            <div className=\"w-full md:w-48 space-y-2\">\n                                                <div className=\"flex justify-between text-xs font-bold uppercase tracking-wider text-[var(--text-muted)]\">\n                                                    <span>Progress</span>\n                                                    <span>{item.progress}%</span>\n                                                </div>\n                                                <div className=\"h-3 bg-[var(--surface-inset)] rounded-full overflow-hidden\" data-testid={`achievement-p