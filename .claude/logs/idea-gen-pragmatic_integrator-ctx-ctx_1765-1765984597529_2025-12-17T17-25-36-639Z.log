ale: isDimmed ? 0.9 : 1 }}\n                                    transition={{ delay: index * 0.1 }}\n                                    whileHover={{ scale: isDimmed ? 0.9 : 1.15, zIndex: 50 }}\n                                    onMouseEnter={() => setHoveredPath(path.id)}\n                                    onMouseLeave={() => setHoveredPath(null)}\n                                >\n                                    {/* Node */}\n                                    <div className=\"relative\">\n                                        {/* Orbit Ring */}\n                                        <motion.div\n                                            animate={{ rotate: 360 }}\n                                            transition={{ duration: 20, repeat: Infinity, ease: \"linear\" }}\n                                            className=\"absolute -inset-2 border border-dashed border-[var(--border-strong)] rounded-full\"\n                                        />\n\n                                        {/* Main Circle */}\n                                        <div className={cn(\n                                            \"w-24 h-24 rounded-full flex flex-col items-center justify-center text-center transition-all shadow-xl\",\n                                            \"bg-[var(--surface-overlay)] backdrop-blur-md border-2\",\n                                            isHovered\n                                                ? \"border-[var(--accent-primary)] shadow-[var(--shadow-glow)]\"\n                                                : \"border-[var(--border-default)]\"\n                                        )}>\n                                            <div className={cn(\n                                                \"w-10 h-10 rounded-xl flex items-center justify-center mb-1\",\n                                                `bg-${path.color}-100 dark:bg-${path.color}-900/30`\n                                            )}>\n    