 object recreation\n    const codeBlockProps = useMemo(() => ({\n        code: data.code,\n        language: data.language ?? \"typescript\",\n        filename: data.filename,\n        showLineNumbers: data.showLineNumbers ?? true,\n        showCopy: data.showCopy ?? true,\n        showHeader: data.showHeader,\n    }), [data.code, data.language, data.filename, data.showLineNumbers, data.showCopy, data.showHeader]);\n\n    // If no previous code or no changes, show normal code block\n    if (!hasActualChanges) {\n        return (\n            <CodeBlock\n                {...codeBlockProps}\n                className={className}\n                data-testid={`code-slot-${slot.id}`}\n            />\n        );\n    }\n\n    // Show code with diff toggle\n    return (\n        <div className={cn(\"space-y-0\", className)} data-testid={`code-slot-${slot.id}`}>\n            {/* Toggle Header */}\n            <div className=\"flex items-center justify-between px-4 py-2 bg-slate-800 dark:bg-slate-800 rounded-t-xl border-b border-slate-700/50\">\n                <div className=\"flex items-center gap-2\">\n                    {showDiff ? (\n                        <GitCompare size={ICON_SIZES.sm} className=\"text-amber-400\" />\n                    ) : (\n                        <Code2 size={ICON_SIZES.sm} className=\"text-slate-400\" />\n                    )}\n                    {data.filename && (\n                        <span className=\"text-xs font-medium text-slate-400\">\n                            {data.filename}\n                        </span>\n                    )}\n                    {!data.filename && data.language && (\n                        <span className=\"text-xs font-medium text-slate-400 uppercase\">\n                            {data.language}\n                        </span>\n                    )}\n                </div>\n\n                {/* Show Changes Toggle */}\n                <button\n                    onClick={toggleDiffView}\n      