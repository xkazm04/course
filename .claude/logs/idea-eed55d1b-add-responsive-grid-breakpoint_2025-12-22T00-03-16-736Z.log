ate?: LayoutTemplate;\n}\n\nexport const ResponsiveSlotLayout: React.FC<ResponsiveSlotLayoutProps> = ({\n    template,\n    mobileTemplate,\n    state,\n    className,\n}) => {\n    // For now, just use the main template\n    // Mobile template support can be added later with useMediaQuery\n    return (\n        <SlotBasedLayout\n            template={template}\n            state={state}\n            className={className}\n        />\n    );\n};\n\nexport default SlotBasedLayout;\n","numLines":177,"startLine":1,"totalLines":177}}}
