   )}\n        </motion.button>\n    );\n};\n\n/** Helper to determine initial wizard step from form state */\nconst getInitialWizardStep = (formState: GoalFormState): number => {\n    // Check if goal was already selected (not default)\n    if (formState.goal && formState.goal !== defaultFormState.goal) {\n        // Check if learning style was selected\n        if (formState.learningStyle) {\n            // Check if time commitment was changed from default\n            if (formState.timeCommitment !== defaultFormState.timeCommitment) {\n                return 4; // All done\n            }\n            return 3; // Time commitment pending\n        }\n        return 2; // Learning style pending\n    }\n    return 1; // Starting from goal\n};\n\n/** Helper to parse time commitment from wizard slider selection */\nconst parseWizardTimeCommitment = (selection: string): number => {\n    const match = selection.match(/(\\d+)/);\n    return match ? parseInt(match[1], 10) : 10;\n};\n\ninterface WizardModeProps {\n    formState: GoalFormState;\n    onFormChange: (updates: Partial<GoalFormState>) => void;\n}\n\nconst WizardMode = ({ formState, onFormChange }: WizardModeProps) => {\n    // Initialize step based on existing form state (for mode switching continuity)\n    const initialStep = getInitialWizardStep(formState);\n    const [step, setStep] = useState(initialStep);\n    const [history, setHistory] = useState<number[]>([]);\n    const prefersReducedMotion = useReducedMotion();\n    const [selections, setSelections] = useState<Selection>(() => {\n        // Pre-populate selections from form state for display purposes\n        const initial: Selection = {};\n        if (formState.goal && formState.goal !== defaultFormState.goal) {\n            initial[1] = formState.goal;\n        }\n        if (formState.learningStyle) {\n            initial[2] = formState.learningStyle;\n        }\n        if (formState.timeCommitment !== defaultFormState.timeCommitment) {\n            initial[3] = `${formState.timeCommitment} hours/week`;\n        }\n        return initial;\n    });\n    const [timeCommitment, setTimeCommitment] = useState(formState.timeCommitment || 10);\n    const [showConfetti, setShowConfetti] = useState(initialStep >= 4);\n\n    // Use React Context for friction tracking instead of window global\n    const { trackBackNavigation } = useFrictionTracking();\n\n    // Use shared path calculator hook for consistent metrics - now uses actual shared formState\n    const { estimatedWeeks, lessons, projects } = usePathCalculator(formState);\n\n    const nextStep = useCallback((selection: string) => {\n        // Update local selections for display\n        setSelections(prev => ({ ...prev, [step]: selection }));\n        setHistory(prev => [...prev, step]);\n\n        // Update shared form state based on which step we're completing\n        if (step === 1) {\n            // Goal selection\n            onFormChange({ goal: selection });\n        } else if (step === 2) {\n            // Learning style selection\n            onFormChange({ learningStyle: selection });\n        } else if (step === 3) {\n            // Time commitment selection\n            const time = parseWizardTimeCommitment(selection);\n            onFormChange({ timeCommitment: time });\n        }\n\n        setStep(prev => prev + 1);\n    }, [step, onFormChange]);\n\n    const prevStep = useCallback(() => {\n        const last = history[history.length - 1];\n        if (last) {\n            setStep(last);\n            setHistory(prev => prev.slice(0, -1));\n            // Track back navigation for friction detection via React Context\n            trackBackNavigation();\n        }\n    }, [history, trackBackNavigation]);\n\n    const handleFinish = useCallback(() => {\n        setShowConfetti(true);\n    }, []);\n\n    // Keep local time commitment in sync with form state\n    const handleTimeCommitmentChange = useCallback((value: number) => {\n        setTimeCommitment(value);\n    }, []);\n\n    // Keyboard navigation\n    useEffect(() => {\n        const handleKeyDown = (e: KeyboardEvent) => {\n            if (e.key === \"Enter\" && step < 4) {\n                if (step === 1) nextStep(goalOptions[0]);\n                if (step === 2) nextStep(learningStyles[0].label);\n                if (step === 3) nextStep(`${timeCommitment} hours/week`);\n            }\n            if (e.key === \"Escape\" && step > 1) {\n                prevStep();\n            }\n        };\n\n        window.addEventListener(\"keydown\", handleKeyDown);\n        return () => window.removeEventListener(\"keydown\", handleKeyDown);\n    }, [step, timeCommitment, nextStep, prevStep]);\n\n    return (\n        <div className=\"max-w-2xl mx-auto\">\n            {/* Animated Progress Bar */}\n            <div className=\"mb-8\">\n                <div className=\"flex justify-between text-xs text-slate-500 mb-2 uppercase tracking-widest font-bold\">\n                    <span className={cn(step >= 1 && \"text-indigo-600 dark:text-indigo-400\")}>Goal</span>\n                    <span className={cn(step >= 2 && \"text-indigo-600 dark:text-indigo-400\")}>Style</span>\n                    <span className={cn(step >= 3 && \"text-indigo-600 dark:text-indigo-400\")}>Time</span>\n                    <span className={cn(step >= 4 && \"text-indigo-600 dark:text-indigo-400\")}>Done</span>\n                </div>\n                <div className=\"h-2 bg-slate-100 dark:bg-slate-800 rounded-full overflow-hidden\">\n                    <motion.div\n                        className=\"h-full bg-gradient-to-r from-indigo-500 via-purple-500 to-cyan-500 rounded-full\"\n                        initial={prefersReducedMotion ? false : { width: 0 }}\n                        animate={{ width: `${(step / 4) * 100}%` }}\n                        transition={prefersReducedMotion ? { duration: 0 } : { duration: 0.5, ease: \"easeInOut\" }}\n                    />\n                </div