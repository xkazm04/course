ype=\"range\"\n                                                min=\"2\"\n                                                max=\"20\"\n                                                value={timeCommitment}\n                                                onChange={(e) => setTimeCommitment(Number(e.target.value))}\n                                                className=\"w-full h-3 bg-slate-100 dark:bg-slate-700 rounded-full appearance-none cursor-pointer accent-indigo-500\"\n                                                data-testid=\"wizard-time-slider\"\n                                            />\n                                            <div className=\"absolute -top-8 left-1/2 -translate-x-1/2 px-3 py-1 bg-slate-900 dark:bg-white text-white dark:text-slate-900 rounded-lg text-sm font-bold\">\n                                                {timeCommitment}h/week\n                                            </div>\n                                        </div>\n                                        <div className=\"pt-4\">\n                                            <button\n                                                onClick={() => nextStep(`${timeCommitment} hours/week`)}\n                                                className=\"w-full py-4 bg-gradient-to-r from-indigo-600 to-purple-600 text-white rounded-xl font-bold hover:opacity-90 transition-opacity flex items-center justify-center gap-2\"\n                                                data-testid=\"wizard-generate-btn\"\n                                            >\n                                                <Sparkles size={ICON_SIZES.sm} />\n                                                Generate My Path\n                                            </button>\n                                        </div>\n                                    </div>\n                                </>\n                            )}\n\n                            {step === 4 && (\n           