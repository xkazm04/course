          : [...focus, id];\n        onFormChange({ focus: newFocus });\n    };\n\n    // Use shared path calculator hook for consistent metrics\n    const { totalHours, modules, topics, hoursPerFocusArea, isJobReady, skillLevel } = usePathCalculator(formState);\n\n    return (\n        <div className=\"grid grid-cols-1 lg:grid-cols-2 gap-8\">\n            {/* Input Form */}\n            <div className=\"space-y-6\">\n                <div>\n                    <h2 className=\"text-3xl font-black text-[var(--text-primary)] mb-2\">\n                        Define Your Goal\n                    </h2>\n                    <p className=\"text-[var(--text-secondary)]\">\n                        Tell us where you want to be, and we'll create the roadmap to get you there.\n                    </p>\n                </div>\n\n                <PrismaticCard>\n                    <div className=\"p-6 space-y-6\">\n                        {/* Goal Input */}\n                        <div>\n                            <label className=\"block text-sm font-bold text-[var(--text-secondary)] mb-2\">\n                                <Target size={ICON_SIZES.sm} className=\"inline mr-2\" />\n                                Your End Goal\n                            </label>\n                            <input\n                                type=\"text\"\n                                value={goal}\n                                onChange={(e) => onFormChange({ goal: e.target.value })}\n                                className=\"w-full p-4 rounded-xl border border-[var(--border-strong)] bg-[var(--surface-overlay)] text-[var(--text-primary)] focus:border-[var(--accent-primary)] focus:ring-2 focus:ring-indigo-100 dark:focus:ring-indigo-900 outline-none transition-all text-lg\"\n                                placeholder=\"e.g., Become a Senior Backend Developer\"\n                                data-testid=\"goal-input\"\n                            />\n                        </div>\n\n                        {/* Time Commitment Slider */}\n                        <div>\n                            <label className=\"block text-sm font-bold text-[var(--text-secondary)] mb-2\">\n                                <Clock size={ICON_SIZES.sm} className=\"inline mr-2\" />\n                                Hours per Week\n                            </label>\n                            <SemanticSlider\n                                value={timeCommitment}\n                                min={5}\n                                max={40}\n                                onChange={(value) => onFormChange({ timeCommitment: value })}\n                                ranges={timeCommitmentRanges}\n                                unit=\"h\"\n                                minLabel=\"5h (Casual)\"\n                                maxLabel=\"40h (Intensive)\"\n                                data-testid=\"time-commitment-slider\"\n                            />\n                        </div>\n\n                        {/* Deadline Slider */}\n                        <div>\n                            <label className=\"block text-sm font-bold text-[var(--text-secondary)] mb-2\">\n                                <Rocket size={ICON_SIZES.sm} className=\"inline mr-2\" />\n                                Target Timeline\n                            </label>\n                            <SemanticSlider\n                                value={deadline}\n                                min={1}\n                                max={24}\n                                onChange={(value) => onFormChange({ deadline: value })}\n                                ranges={deadlineRanges}\n                                unit=\" months\"\n                                minLabel=\"1 month (Sprint)\"\n                                maxLabel=\"24 months (Relaxed)\"\n                                data-testid=\"deadline-slider\"\n                            />\n           