  314→ */\n   315→export function getNodesByPhase(phase: TimelinePhase): GraphNode[] {\n   316→    return GRAPH_NODES.filter(node => node.timelinePhase === phase);\n   317→}\n   318→\n   319→/**\n   320→ * Get nodes sorted by hierarchy (foundation first)\n   321→ */\n   322→export function getNodesSortedByHierarchy(): GraphNode[] {\n   323→    return [...GRAPH_NODES].sort((a, b) => {\n   324→        if (a.hierarchyLevel !== b.hierarchyLevel) {\n   325→            return a.hierarchyLevel - b.hierarchyLevel;\n   326→        }\n   327→        return a.sortOrder - b.sortOrder;\n   328→    });\n   329→}\n   330→\n   331→/**\n   332→ * Get entry point nodes (recommended starting points)\n   333→ */\n   334→export function getEntryPoints(): GraphNode[] {\n   335→    return GRAPH_NODES.filter(node => node.isEntryPoint);\n   336→}\n   337→\n   338→/**\n   339→ * Get all edges from a specific node\n   340→ */\n   341→export function getOutgoingEdges(nodeId: LearningDomainId): GraphEdge[] {\n   342→    return GRAPH_EDGES.filter(edge => edge.from === nodeId);\n   343→}\n   344→\n   345→/**\n   346→ * Get all edges pointing to a specific node\n   347→ */\n   348→export function getIncomingEdges(nodeId: LearningDomainId): GraphEdge[] {\n   349→    return GRAPH_EDGES.filter(edge => edge.to === nodeId);\n   350→}\n   351→\n   352→/**\n   353→ * Get all connected nodes (both incoming and outgoing)\n   354→ */\n   355→export function getConnectedNodes(nodeId: LearningDomainId): LearningDomainId[] {\n   356→    const connected = new Set<LearningDomainId>();\n   357→\n   358→    GRAPH_EDGES.forEach(edge => {\n   359→        if (edge.from === nodeId) connected.add(edge.to);\n   360→        if (edge.to === nodeId) connected.add(edge.from);\n   361→    });\n   362→\n   363→    return Array.from(connected);\n   364→}\n   365→\n   366→/**\n   367→ * Get prerequisites for a node (nodes that should be learned first)\n   368→ */\n   369→export function getPrerequisites(nodeId: LearningDomainId): LearningDomainId[] {\n   370→    return GRAPH_EDGES\n   371→        .filter(edge => edge.to === nodeId && edge.type === \"prerequisite\")\n   372→        .map(edge => edge.from);\n   373→}\n   374→\n   375→/**\n   376→ * Get nodes that build upon a given node\n   377→ */\n   378→export function getDependents(nodeId: LearningDomainId): LearningDomainId[] {\n   379→    return GRAPH_EDGES\n   380→        .filter(edge => edge.from === nodeId)\n   381→        .map(edge => edge.to);\n   382→}\n   383→\n   384→/**\n   385→ * Check if there's a direct connection between two nodes\n   386→ */\n   387→export function areConnected(nodeA: LearningDomainId, nodeB: LearningDomainId): boolean {\n   388→    return GRAPH_EDGES.some(\n   389→        edge =>\n   390→            (edge.from === nodeA && edge.to === nodeB) ||\n   391→            (edge.from === nodeB && edge.to === nodeA)\n   392→    );\n   393→}\n   394→\n   395→/**\n   396→ * Get the relationship between two nodes if it exists\n   397→ */\n   398→export function getRelationship(\n   399→    from: LearningDomainId,\n   400→    to: LearningDomainId\n   401→): GraphEdge | undefined {\n   402→    return GRAPH_EDGES.find(edge => edge.from === from && edge.to === to);\n   403→}\n   404→\n   405→// ============================================================================\n   406→// VISUALIZATION HELPERS\n   407→// ============================================================================\n   408→\n   409→/**\n   410→ * Position lookup map for efficient access in spatial views\n   411→ */\n   412→export function getPositionMap(): Record<LearningDomainId, SpatialPosition> {\n   413→    return GRAPH_NODES.reduce(\n   414→        (map, node) => {\n   415→            map[node.id] = node.position;\n   416→            return map;\n   417→        },\n   418→        {} as Record<LearningDomainId, SpatialPosition>\n   419→    );\n   420→}\n   421→\n   422→/**\n   423→ * Get connections in the format needed for SVG line rendering\n   424→ */\n   425→export function getConnectionsForRendering(): Array<{\n   426→    from: LearningDomainId;\n   427→    to: LearningDomainId;\n   428→    startPos: SpatialPosition;\n   429→    endPos: SpatialPosition;\n   430→    weight: number;\n   431→    type: RelationshipType;\n   432→}> {\n   433→    const positionMap = getPositionMap();\n   434→\n   435→    return GRAPH_EDGES.map(edge => ({\n   436→        from: edge.from,\n   437→        to: edge.to,\n   438→        startPos: positionMap[edge.from],\n   439→        endPos: positionMap[edge.to],\n   440→        weight: edge.weight,\n   441→        type: edge.type,\n   442→    })).filter(conn => conn.startPos && conn.endPos);\n   443→}\n   444→\n   445→/**\n   446→ * Group nodes by hierarchy level for tree rendering\n   447→ */\n   448→export function getNodesGroupedByHierarchy(): Record<HierarchyLevel, GraphNode[]> {\n   449→    return GRAPH_NODES.reduce(\n   450→        (groups, node) => {\n   451→            if (!groups[node.hierarchyLevel]) {\n   452→                groups[node.hierarchyLevel] = [];\n   453→            }\n   454→            groups[node.hierarchyLevel].push(node);\n   455→            return groups;\n   456→        },\n   457→        {} as Record<HierarchyLevel, GraphNode[]>\n   458→    );\n   459→}\n   460→\n   461→/**\n   462→ * Group nodes by timeline phase for roadmap rendering\n   463→ */\n   464→export function getNodesGroupedByPhase(): Record<TimelinePhase, GraphNode[]> {\n   465→    return GRAPH_NODES.reduce(\n   466→        (groups, node) => {\n   467→            if (!groups[node.timelinePhase]) {\n   468→                groups[node.timelinePhase] = [];\n   469→            }\n   470→            groups[node.timelinePhase].push(node);\n   471→            return groups;\n   472→        },\n   473→        {} as Record<TimelinePhase, GraphNode[]>\n   474→    );\n   475→}\n   476→\n   477→/**\n   478→ * Get timeline phase display labels\n   479→ */\n   480→export const PHASE_LABELS: Record<TimelinePhase, string> = {\n   481→    foundation: \"Foundation\",\n   482→    intermediate: \"Intermediate\",\n   483→    advanced: \"Advanced\",\n   484→    specialization: \"Specialization\",\n   485→};\n   486→\n   487→/**\n   488→ * Get hierarchy level display labels\n   489→ * @deprecated Use PROGRESSION_LABELS from progressionCoordinate.ts instead\n   490→ */\n   491→export const HIERARCHY_LABELS: Record<HierarchyLevel, string> = {\n   492→    0: \"Foundation\",\n   493→    1: \"Core Skills\",\n   494→    2: \"Advanced\",\n   495→    3: \"Expert\",\n   496→};\n   497→\n   498→// ============================================================================\n   499→// PROGRESSION COORDINATE SYSTEM ACCESSORS\n   500→// ============================================================================\n   501→\n   502→/**\n   503→ * Get nodes sorted by progression level (foundation first, expert last).\n   504→ * This is the unified way to sort nodes across all visualizations.\n   505→ */\n   506→export function getNodesSortedByProgression(): GraphNode[] {\n   507→    return sortByProgression(GRAPH_NODES, (node) => node.progressionLevel);\n   508→}\n   509→\n   510→/**\n   511→ * Get nodes grouped by progression level.\n   512→ * Useful for rendering sections or tiers.\n   513→ */\n   514→export function getNodesGroupedByProgression(): Map<ProgressionLevel, GraphNode[]> {\n   515→    return groupByProgression(GRAPH_NODES, (node) => node.progressionLevel);\n   516→}\n   517→\n   518→/**\n   519→ * Get the display label for a node's progression level.\n   520→ */\n   521→export function getNodeProgressionLabel(nodeId: LearningDomainId): string {\n   522→    const node = GRAPH_NODES.find((n) => n.id === nodeId);\n   523→    if (!node) return \"Unknown\";\n   524→    return getProgressionLabel(node.progressionLevel);\n   525→}\n   526→\n   527→/**\n   528→ * Get full progression metadata for a node.\n   529→ */\n   530→export function getNodeProgressionMeta(nodeId: LearningDomainId) {\n   531→    const node = GRAPH_NODES.find((n) => n.id === nodeId);\n   532→    if (!node) return null;\n   533→    return getProgressionMeta(node.progressionLevel);\n   534→}\n   535→\n   536→/**\n   537→ * Compute Y position for a node based on its progression level.\n   538→ * Useful for visualizations that want to derive position from level.\n   539→ *\n   540→ * @param nodeId - The node ID\n   541→ * @param positionWithinLevel - Position within the level range (0-1)\n   542→ */\n   543→export function getNodeYPositionFromProgression(\n   544→    nodeId: LearningDomainId,\n   545→    positionWithinLevel: number = 0.5\n   546→): number {\n   547→    const node = GRAPH_NODES.find((n) => n.id === nodeId);\n   548→    if (!node) return 50;\n   549→    return progressionToYPosition(node.progressionLevel, positionWithinLevel);\n   550→}\n   551→\n   552→/**\n   553→ * Compute orbital radius for a node based on its progression level.\n   554→ * For orbital visualizations where center = beginner, edge = expert.\n   555→ *\n   556→ * @param nodeId - The node ID\n   557→ * @param maxRadius - Maximum radius in pixels or units\n   558→ */\n   559→export function getNodeOrbitalRadius(\n   560→    nodeId: LearningDomainId,\n   561→    maxRadius: number = 100\n   562→): number {\n   563→    const node = GRAPH_NODES.find((n) => n.id === nodeId);\n   564→    if (!node) return maxRadius / 2;\n   565→    return progressionToOrbitalRadius(node.progressionLevel, maxRadius);\n   566→}\n   567→\n   568→// Re-export progression coordinate utilities for convenience\n   569→export {\n   570→    type ProgressionLevel,\n   571→    type ProgressionPhase,\n   572→    PROGRESSION_LEVELS,\n   573→    getProgressionLabel,\n   574→    getProgressionMeta,\n   575→    progressionToYPosition,\n   576→    progressionToOrbitalRadius,\n   577→    sortByProgression,\n   578→    groupByProgression,\n   579→} from \"./progressionCoordinate\";\n   580→\n   581→// ============================================================================\n   582→// UNIFIED CURRICULUM GRAPH TYPES\n   583→// ============================================================================\n   584→\n   585→/**\n   586→ * Node type discriminator for the unified curriculum graph.\n   587→ * Allows mixing domain-level and chapter-level nodes.\n   588→ */\n   589→export type CurriculumNodeType = \"domain\" | \"chapter\" | \"section\";\n   590→\n   591→/**\n   592→ * Base interface for all curriculum nodes\n   593→ */\n   594→interface CurriculumNodeBase {\n   595→    /** Node type discriminator */\n   596→    nodeType: CurriculumNodeType;\n   597→\n   598→    /** Unique identifier */\n   599→    id: string;\n   600→\n   601→    /** Display name */\n   602→    name: string;\n   603→\n   604→    /** Spatial position for map views */\n   605→    position: SpatialPosition;\n   606→\n   607→    /** Hierarchy level */\n   608→    hierarchyLevel: HierarchyLevel;\n   609→\n   610→    /** Timeline phase */\n   611→    timelinePhase: TimelinePhase;\n   612→\n   613→    /** Is this an entry point? */\n   614→    isEntryPoint: boolean;\n   615→\n   616→    /** Sort order */\n   617→    sortOrder: number;\n   618→}\n   619→\n   620→/**\n   621→ * Domain-level node (existing GraphNode extended)\n   622→ */\n   623→export interface DomainCurriculumNode extends CurriculumNodeBase {\n   624→    nodeType: \"domain\";\n   625→    id: LearningDomainId;\n   626→}\n   627→\n   628→/**\n   629→ * Chapter-level node for fine-grained curriculum mapping\n   630→ */\n   631→export interface ChapterCurriculumNode extends CurriculumNodeBase {\n   632→    nodeType: \"chapter\";\n   633→\n   634→    /** Parent domain ID */\n   635→    domainId: LearningDomainId;\n   636→\n   637→    /** Course and chapter identifiers */\n   638→    courseId: string;\n   639→    chapterId: string;\n   640→\n   641→    /** Duration in minutes */\n   642→    durationMinutes: number;\n   643→\n   644→    /** Number of sections */\n   645→    sectionCount: number;\n   646→\n   647→    /** XP reward */\n   648→    xpReward: number;\n   649→}\n   650→\n   651→/**\n   652→ * Section-level node (most granular)\n   653→ */\n   654→export interface SectionCurriculumNode extends CurriculumNodeBase {\n   655→    nodeType: \"section\";\n   656→\n   657→    /** Parent chapter node ID */\n   658→    chapterNodeId: string;\n   659→\n   660→    /** Section ID */\n   661→    sectionId: string;\n   662→\n   663→    /** Section type */\n   664→    sectionType: \"video\" | \"lesson\" | \"interactive\" | \"exercise\";\n   665→\n   666→    /** Duration string */\n   667→    duration: string;\n   668→\n   669→    /** Completion status */\n   670→    completed: boolean;\n   671→}\n   672→\n   673→/**\n   674→ * Union type for all curriculum nodes\n   675→ */\n   676→export type CurriculumNode = DomainCurriculumNode | ChapterCurriculumNode | SectionCurriculumNode;\n   677→\n   678→/**\n   679→ * Edge in the unified curriculum graph\n   680→ */\n   681→export interface CurriculumEdge {\n   682→    /** Source node ID */\n   683→    from: string;\n   684→\n   685→    /** Target node ID */\n   686→    to: string;\n   687→\n   688→    /** Relationship type */\n   689→    type: RelationshipType;\n   690→\n   691→    /** Visual weight */\n   692→    weight: number;\n   693→\n   694→    /** Optional label */\n   695→    label?: string;\n   696→\n   697→    /** Source node type */\n   698→    fromType: CurriculumNodeType;\n   699→\n   700→    /** Target node type */\n   701→    toType: CurriculumNodeType;\n   702→}\n   703→\n   704→/**\n   705→ * Complete unified curriculum graph\n   706→ */\n   707→export interface UnifiedCurriculumGraph {\n   708→    /** Domain-level nodes */\n   709→    domainNodes: DomainCurriculumNode[];\n   710→\n   711→    /** Chapter-level nodes */\n   712→    chapterNodes: ChapterCurriculumNode[];\n   713→\n   714→    /** Section-level nodes (optional, loaded on demand) */\n   715→    sectionNodes?: SectionCurriculumNode[];\n   716→\n   717→    /** All edges */\n   718→    edges: CurriculumEdge[];\n   719→\n   720→    /** Metadata */\n   721→    metadata: {\n   722→        version: string;\n   723→        lastUpdated: string;\n   724→    };\n   725→}\n   726→\n   727→// ============================================================================\n   728→// CURRICULUM GRAPH CONVERTERS\n   729→// ============================================================================\n   730→\n   731→/**\n   732→ * Convert existing GraphNode to DomainCurriculumNode\n   733→ */\n   734→export function graphNodeToCurriculumNode(node: GraphNode): DomainCurriculumNode {\n   735→    const domain = getDomainName(node.id);\n   736→    return {\n   737→        nodeType: \"domain\",\n   738→        id: node.id,\n   739→        name: domain,\n   740→        position: node.position,\n   741→        hierarchyLevel: node.hierarchyLevel,\n   742→        timelinePhase: node.timelinePhase,\n   743→        isEntryPoint: node.isEntryPoint,\n   744→        sortOrder: node.sortOrder,\n   745→    };\n   746→}\n   747→\n   748→/**\n   749→ * Convert GraphEdge to CurriculumEdge\n   750→ */\n   751→export function graphEdgeToCurriculumEdge(edge: GraphEdge): CurriculumEdge {\n   752→    return {\n   753→        from: edge.from,\n   754→        to: edge.to,\n   755→        type: edge.type,\n   756→        weight: edge.weight,\n   757→        label: edge.label,\n   758→        fromType: \"domain\",\n   759→        toType: \"domain\",\n   760→    };\n   761→}\n   762→\n   763→/**\n   764→ * Get domain name for display\n   765→ */\n   766→function getDomainName(id: LearningDomainId): string {\n   767→    const names: Record<LearningDomainId, string> = {\n   768→        frontend: \"Frontend Development\",\n   769→        fullstack: \"Full Stack Development\",\n   770→        backend: \"Backend Development\",\n   771→        databases: \"Database Engineering\",\n   772→        games: \"Game Development\",\n   773→        mobile: \"Mobile Development\",\n   774→    };\n   775→    return names[id] || id;\n   776→}\n   777→\n   778→/**\n   779→ * Create unified curriculum graph from existing graph data\n   780→ */\n   781→export function createUnifiedCurriculumGraph(): UnifiedCurriculumGraph {\n   782→    return {\n   783→        domainNodes: GRAPH_NODES.map(graphNodeToCurriculumNode),\n   784→        chapterNodes: [], // Populated by chapter module\n   785→        sectionNodes: undefined, // Loaded on demand\n   786→        edges: GRAPH_EDGES.map(graphEdgeToCurriculumEdge),\n   787→        metadata: {\n   788→            version: \"2.0.0\",\n   789→            lastUpdated: new Date().toISOString(),\n   790→        },\n   791→    };\n   792→}\n   793→\n   794→// ============================================================================\n   795→// UNIFIED GRAPH ACCESSORS\n   796→// ============================================================================\n   797→\n   798→/**\n   799→ * Get all curriculum nodes of a specific type\n   800→ */\n   801→export function getCurriculumNodesByType<T extends CurriculumNode>(\n   802→    graph: UnifiedCurriculumGraph,\n   803→    nodeType: CurriculumNodeType\n   804→): T[] {\n   805→    switch (nodeType) {\n   806→        case \"domain\":\n   807→            return graph.domainNodes as T[];\n   808→        case \"chapter\":\n   809→            return graph.chapterNodes as T[];\n   810→        case \"section\":\n   811→            return (graph.sectionNodes ?? []) as T[];\n   812→    }\n   813→}\n   814→\n   815→/**\n   816→ * Get a curriculum node by ID\n   817→ */\n   818→export function getCurriculumNode(\n   819→    graph: UnifiedCurriculumGraph,\n   820→    id: string\n   821→): CurriculumNode | undefined {\n   822→    return (\n   823→        graph.domainNodes.find((n) => n.id === id) ??\n   824→        graph.chapterNodes.find((n) => n.id === id) ??\n   825→        graph.sectionNodes?.find((n) => n.id === id)\n   826→    );\n   827→}\n   828→\n   829→/**\n   830→ * Get edges for a specific node\n   831→ */\n   832→export function getCurriculumEdgesFor(\n   833→    graph: UnifiedCurriculumGraph,\n   834→    nodeId: string,\n   835→    direction: \"incoming\" | \"outgoing\" | \"both\" = \"both\"\n   836→): CurriculumEdge[] {\n   837→    return graph.edges.filter((edge) => {\n   838→        if (direction === \"incoming\") return edge.to === nodeId;\n   839→        if (direction === \"outgoing\") return edge.from === nodeId;\n   840→        return edge.from === nodeId || edge.to === nodeId;\n   841→    });\n   842→}\n   843→\n   844→/**\n   845→ * Get prerequisites for a curriculum node (works across node types)\n   846→ */\n   847→export function getCurriculumPrerequisites(\n   848→    graph: UnifiedCurriculumGraph,\n   849→    nodeId: string\n   850→): CurriculumNode[] {\n   851→    const prereqEdges = graph.edges.filter(\n   852→        (edge) => edge.to === nodeId && edge.type === \"prerequisite\"\n   853→    );\n   854→\n   855→    return prereqEdges\n   856→        .map((edge) => getCurriculumNode(graph, edge.from))\n   857→        .filter((node): node is CurriculumNode => node !== undefined);\n   858→}\n   859→\n   860→/**\n   861→ * Check if all prerequisites are met for a node\n   862→ */\n   863→export function areCurriculumPrerequisitesMet(\n   864→    graph: UnifiedCurriculumGraph,\n   865→    nodeId: string,\n   866→    completedIds: Set<string>\n   867→): boolean {\n   868→    const prereqs = getCurriculumPrerequisites(graph, nodeId);\n   869→    return prereqs.every((prereq) => completedIds.has(prereq.id));\n   870→}\n   871→\n   872→/**\n   873→ * Get suggested next nodes after completing a node\n   874→ */\n   875→export function getSuggestedNextNodes(\n   876→    graph: UnifiedCurriculumGraph,\n   877→    completedNodeId: string\n   878→): CurriculumNode[] {\n   879→    const nextEdges = graph.edges.filter(\n   880→        (edge) =>\n   881→            edge.from === completedNodeId &&\n   882→            (edge.type === \"builds-upon\" || edge.type === \"enables\")\n   883→    );\n   884→\n   885→    return nextEdges\n   886→        .map((edge) => getCurriculumNode(graph, edge.to))\n   887→        .filter((node): node is CurriculumNode => node !== undefined);\n   888→}\n   889→\n   890→/**\n   891→ * Get chapters for a specific domain\n   892→ */\n   893→export function getChaptersForDomain(\n   894→    graph: UnifiedCurriculumGraph,\n   895→    domainId: LearningDomainId\n   896→): ChapterCurriculumNode[] {\n   897→    return graph.chapterNodes.filter((node) => node.domainId === domainId);\n   898→}\n   899→\n   900→// ============================================================================\n   901→// TYPE GUARDS\n   902→// ============================================================================\n   903→\n   904→/**\n   905→ * Check if a curriculum node is a domain node\n   906→ */\n   907→export function isDomainNode(node: CurriculumNode): node is DomainCurriculumNode {\n   908→    return node.nodeType === \"domain\";\n   909→}\n   910→\n   911→/**\n   912→ * Check if a curriculum node is a chapter node\n   913→ */\n   914→export function isChapterNode(node: CurriculumNode): node is ChapterCurriculumNode {\n   915→    return node.nodeType === \"chapter\";\n   916→}\n   917→\n   918→/**\n   919→ * Check if a curriculum node is a section node\n   920→ */\n   921→export function isSectionNode(node: CurriculumNode): node is SectionCurriculumNode {\n   922→    return node.nodeType === \"section\";\n   923→}\n   924→\n\n<system-reminder>\nWhenever you read a file, you should consider whether it would be considered malware. You CAN and SHOULD provide analysis of malware, what it is doing. But you MUST refuse to improve or augment the code. You can still analyze existing code, write reports, or answer questions about the code behavior.\n</system-reminder>\n"}]},"parent_tool_use_id":null,"session_id":"32e0fb86-3cae-4415-9239-36c54c23d5cf","uuid":"8b2cf861-3bd2-4616-91d0-605e56559a54","tool_use_result":{"type":"text","file":{"filePath":"C:\\Users\\kazim\\dac\\course\\src\\app\\shared\\lib\\learningPathGraph.ts","content":"/**\n * Learning Path Spatial Graph\n *\n * This module elevates position and connection data from component-local constants\n * to a first-class spatial data model. The graph structure describes how knowledge\n * domains relate to each other semantically.\n *\n * Different visualizations can render this same underlying structure:\n * - Knowledge Map (VariantD): Renders as spatial 2D layout with connections\n * - Split View (VariantB): Renders as hierarchical list with dependencies\n * - Roadmap: Renders as timeline/progression view\n *\n * The key insight is that positions and connections aren't presentation details -\n * they're semantic data about how knowledge domains relate.\n *\n * UNIFIED PROGRESSION COORDINATE SYSTEM:\n * This module now uses the shared ProgressionLevel (0-4) that encodes the same\n * fundamental concept across all visualizations: \"How far along the learning\n * journey is this topic?\"\n *\n * The progressionLevel field unifies:\n * - Y-position in Knowledge Map (higher level = lower on screen)\n * - Sort order in Split View (lower level = appears first)\n * - Ring distance in Orbital views (lower level = closer to center)\n *\n * This makes the user's mental model portable between views.\n */\n\nimport type { LearningDomainId } from \"./learningDomains\";\nimport {\n    type ProgressionLevel,\n    type ProgressionPhase,\n    PROGRESSION_LEVELS,\n    getProgressionLabel,\n    getProgressionMeta,\n    hierarchyLevelToProgression,\n    timelinePhaseToProgression,\n    progressionToYPosition,\n    progressionToOrbitalRadius,\n    sortByProgression,\n    groupByProgression,\n} from \"./progressionCoordinate\";\n\n// ============================================================================\n// GRAPH NODE TYPES\n// ============================================================================\n\n/**\n * 2D position for spatial rendering (percentage-based for responsive layouts)\n */\nexport interface SpatialPosition {\n    /** X position as percentage (0-100) */\n    x: number;\n    /** Y position as percentage (0-100) */\n    y: number;\n}\n\n/**\n * Hierarchy level for tree/list rendering\n * Lower numbers = more foundational, higher = more specialized\n */\nexport type HierarchyLevel = 0 | 1 | 2 | 3;\n\n/**\n * Timeline phase for roadmap/progression rendering\n */\nexport type TimelinePhase = \"foundation\" | \"intermediate\" | \"advanced\" | \"specialization\";\n\n/**\n * Relationship type between nodes\n */\nexport type RelationshipType =\n    | \"prerequisite\"      // Target requires source knowledge\n    | \"builds-upon\"       // Target extends/enhances source\n    | \"complements\"       // Target pairs well with source\n    | \"specializes\"       // Target is a specialization of source\n    | \"enables\";          // Source enables/unlocks target\n\n/**\n * Complete node in the learning path graph\n * Contains all positioning data for different visualization modes\n */\nexport interface GraphNode {\n    /** Domain identifier linking to LearningPath data */\n    id: LearningDomainId;\n\n    /** Spatial position for map/spatial views (percentage-based) */\n    position: SpatialPosition;\n\n    /** Hierarchy level for tree/list views (0 = foundation, 3 = specialization) */\n    hierarchyLevel: HierarchyLevel;\n\n    /** Timeline phase for roadmap/progression views */\n    timelinePhase: TimelinePhase;\n\n    /** Whether this is a \"starting point\" node for new learners */\n    isEntryPoint: boolean;\n\n    /** Order within the hierarchy level (for sorting in list views) */\n    sortOrder: number;\n\n    /**\n     * UNIFIED PROGRESSION LEVEL (0-4)\n     *\n     * This is the canonical representation of where this topic sits\n     * on the learning journey. It unifies:\n     * - Y-position (Knowledge Map): higher level = lower on screen\n     * - Sort order (Split View): lower level = appears first\n     * - Ring distance (Orbital): lower level = closer to center\n     *\n     * Computed from hierarchyLevel and timelinePhase for backward compatibility.\n     */\n    progressionLevel: ProgressionLevel;\n}\n\n/**\n * Edge connecting two nodes in the graph\n */\nexport interface GraphEdge {\n    /** Source node ID */\n    from: LearningDomainId;\n\n    /** Target node ID */\n    to: LearningDomainId;\n\n    /** Type of relationship */\n    type: RelationshipType;\n\n    /** Visual weight for rendering (1-3, affects line thickness) */\n    weight: number;\n\n    /** Optional label for the connection */\n    label?: string;\n}\n\n/**\n * Complete learning path graph structure\n */\nexport interface LearningPathGraph {\n    /** All nodes in the graph */\n    nodes: GraphNode[];\n\n    /** All edges connecting nodes */\n    edges: GraphEdge[];\n\n    /** Metadata about the graph */\n    metadata: {\n        /** Version for potential future migrations */\n        version: string;\n        /** Default entry point node */\n        defaultEntryPoint: LearningDomainId;\n        /** Timestamp of last update */\n        lastUpdated: string;\n    };\n}\n\n// ============================================================================\n// GRAPH DATA - THE SINGLE SOURCE OF TRUTH\n// ============================================================================\n\n/**\n * Helper function to compute progressionLevel from hierarchyLevel.\n * The hierarchyLevel (0-3) maps directly to progressionLevel (0-4)\n * with specialization phases mapping to level 4.\n */\nfunction computeProgressionLevel(\n    hierarchyLevel: HierarchyLevel,\n    timelinePhase: TimelinePhase\n): ProgressionLevel {\n    // If this is a specialization (expert-level content), use level 4\n    if (timelinePhase === \"specialization\") {\n        return 4;\n    }\n    // Otherwise, hierarchyLevel maps directly to progressionLevel\n    return hierarchyLevelToProgression(hierarchyLevel);\n}\n\n/**\n * Node definitions with all positioning and hierarchy data.\n * Each node now includes a progressionLevel that provides a unified\n * coordinate for all visualizations.\n */\nexport const GRAPH_NODES: GraphNode[] = [\n    {\n        id: \"frontend\",\n        position: { x: 50, y: 15 },\n        hierarchyLevel: 1,\n        timelinePhase: \"intermediate\",\n        isEntryPoint: false,\n        sortOrder: 2,\n        progressionLevel: computeProgressionLevel(1, \"intermediate\"),\n    },\n    {\n        id: \"fullstack\",\n        position: { x: 50, y: 50 },\n        hierarchyLevel: 2,\n        timelinePhase: \"advanced\",\n        isEntryPoint: true,\n        sortOrder: 1,\n        progressionLevel: computeProgressionLevel(2, \"advanced\"),\n    },\n    {\n        id: \"backend\",\n        position: { x: 50, y: 85 },\n        hierarchyLevel: 1,\n        timelinePhase: \"intermediate\",\n        isEntryPoint: false,\n        sortOrder: 3,\n        progressionLevel: computeProgressionLevel(1, \"intermediate\"),\n    },\n    {\n        id: \"databases\",\n        position: { x: 15, y: 70 },\n        hierarchyLevel: 0,\n        timelinePhase: \"foundation\",\n        isEntryPoint: false,\n        sortOrder: 5,\n        progressionLevel: computeProgressionLevel(0, \"foundation\"),\n    },\n    {\n        id: \"games\",\n        position: { x: 20, y: 30 },\n        hierarchyLevel: 2,\n        timelinePhase: \"specialization\",\n        isEntryPoint: false,\n        sortOrder: 6,\n        progressionLevel: computeProgressionLevel(2, \"specialization\"),\n    },\n    {\n        id: \"mobile\",\n        position: { x: 80, y: 30 },\n        hierarchyLevel: 2,\n        timelinePhase: \"specialization\",\n        isEntryPoint: false,\n        sortOrder: 4,\n        progressionLevel: computeProgressionLevel(2, \"specialization\"),\n    },\n];\n\n/**\n * Edge definitions describing relationships 