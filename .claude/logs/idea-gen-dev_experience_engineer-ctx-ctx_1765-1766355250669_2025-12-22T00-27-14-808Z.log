ls-2 gap-3\">\n                        {predictions.industryTrends.slice(0, 2).map((trend) => (\n                            <IndustryTrendCard key={trend.sector} trend={trend} />\n                        ))}\n                    </div>\n                </div>\n            )}\n\n            {/* Action Buttons */}\n            <div className=\"flex items-center justify-center gap-4 pt-4\">\n                <button\n                    onClick={onViewPath}\n                    data-testid=\"oracle-view-path-btn\"\n                    className=\"px-6 py-3 bg-gradient-to-r from-indigo-600 to-purple-600 text-white rounded-xl font-bold hover:opacity-90 transition-opacity flex items-center gap-2\"\n                >\n                    <Target size={ICON_SIZES.sm} />\n                    View Learning Path\n                    <ChevronRight size={ICON_SIZES.sm} />\n                </button>\n                <button\n                    onClick={onViewJobs}\n                    data-testid=\"oracle-view-jobs-btn\"\n                    className=\"px-6 py-3 bg-white dark:bg-slate-800 text-slate-900 dark:text-slate-100 border border-slate-200 dark:border-slate-700 rounded-xl font-bold hover:bg-slate-50 dark:hover:bg-slate-700 transition-colors flex items-center gap-2\"\n                >\n                    <Briefcase size={ICON_SIZES.sm} />\n                    Browse Jobs\n                    <ChevronRight size={ICON_SIZES.sm} />\n                </button>\n            </div>\n        </motion.div>\n    );\n};\n\n// Path Step\ninterface PathStepProps {\n    path: any;\n    loading: boolean;\n    onBack: () => void;\n    onViewJobs: () => void;\n}\n\nconst PathStep = ({ path, loading, onBack, onViewJobs }: PathStepProps) => {\n    if (loading || !path) {\n        return (\n            <motion.div\n                key=\"path-loading\"\n                initial={{ opacity: 0 }}\n                animate={{ opacity: 1 }}\n                className=\"text-center py-16\"\n      