 15; // default\n   297→};\n   298→\n   299→/** Helper to parse deadline from chat option */\n   300→const parseDeadline = (option: string): number => {\n   301→    if (option.includes(\"3 months\")) return 3;\n   302→    if (option.includes(\"6 months\")) return 6;\n   303→    if (option.includes(\"12 months\")) return 12;\n   304→    if (option.includes(\"No deadline\")) return 24;\n   305→    return 6; // default\n   306→};\n   307→\n   308→/** Helper to determine which chat step a given formState represents */\n   309→const getInitialChatStep = (formState: GoalFormState): number => {\n   310→    // If goal is set and not default, user has answered question 1\n   311→    if (formState.goal && formState.goal !== defaultFormState.goal) {\n   312→        // If timeCommitment is not default, user has answered question 2\n   313→        if (formState.timeCommitment !== defaultFormState.timeCommitment) {\n   314→            // If learningStyle is set, user has answered question 3 (skill level maps to learningStyle)\n   315→            if (formState.learningStyle) {\n   316→                // If deadline is not default, user has answered question 4\n   317→                if (formState.deadline !== defaultFormState.deadline) {\n   318→                    return 4; // All questions answered\n   319→                }\n   320→                return 3; // Skill level answered, deadline pending\n   321→            }\n   322→            return 2; // Time commitment answered, skill level pending\n   323→        }\n   324→        return 1; // Goal answered, time commitment pending\n   325→    }\n   326→    return 0; // Starting fresh\n   327→};\n   328→\n   329→/** Create initial messages based on current form state */\n   330→const createInitialMessages = (formState: GoalFormState): Message[] => {\n   331→    const chatStep = getInitialChatStep(formState);\n   332→    const messages: Message[] = [];\n   333→    let msgId = 1;\n   334→\n   335→    // Always add the initial bot greeting\n   336→    messages.push({\n   337→        id: msgId++,\n   338→        type: \"bot\",\n   339→        content: \"Hi! I'm your AI learning assistant. I'll help you create a personalized learning path. What's your ultimate career goal?\",\n   340→        options: chatStep === 0 ? [\n   341→            \"Become a Backend Developer\",\n   342→            \"Pass AWS Certification\",\n   343→            \"Build a SaaS Product\",\n   344→            \"Let me type my own goal...\"\n   345→        ] : undefined,\n   346→        field: \"goal\"\n   347→    });\n   348→\n   349→    // If we have a goal, add it as user message and continue\n   350→    if (chatStep >= 1 && formState.goal) {\n   351→        messages.push({\n   352→            id: msgId++,\n   353→            type: \"user\",\n   354→            content: formState.goal\n   355→        });\n   356→        messages.push({\n   357→            id: msgId++,\n   358→            type: \"bot\",\n   359→            content: `Great choice! \"${formState.goal}\" is an exciting goal. How many hours per week can you dedicate to learning?`,\n   360→            options: chatStep === 1 ? [\"5-10 hours\", \"10-20 hours\", \"20-30 hours\", \"30+ hours\"] : undefined,\n   361→            field: \"timeCommitment\"\n   362→        });\n   363→    }\n   364→\n   365→    // If we have time commitment, add it and continue\n   366→    if (chatStep >= 2) {\n   367→        const timeStr = formState.timeCommitment <= 10 ? \"5-10 hours\" :\n   368→                       formState.timeCommitment <= 20 ? \"10-20 hours\" :\n   369→                       formState.timeCommitment <= 30 ? \"20-30 hours\" : \"30+ hours\";\n   370→        messages.push({\n   371→            id: msgId++,\n   372→            type: \"user\",\n   373→            content: timeStr\n   374→        });\n   375→        messages.push({\n   376→            id: msgId++,\n   377→            type: \"bot\",\n   378→            content: `Perfect, ${timeStr} is a solid commitment. What's your current skill level?`,\n   379→            options: chatStep === 2 ? [\"Complete Beginner\", \"Some Basics\", \"Intermediate\", \"Advanced\"] : undefined,\n   380→            field: \"learningStyle\"\n   381→        });\n   382→    }\n   383→\n   384→    // If we have learning style / skill level, add it and continue\n   385→    if (chatStep >= 3 && formState.learningStyle) {\n   386→        messages.push({\n   387→            id: msgId++,\n   388→            type: \"user\",\n   389→            content: formState.learningStyle\n   390→        });\n   391→        messages.push({\n   392→            id: msgId++,\n   393→            type: \"bot\",\n   394→            content: `Got it! Last question - when do you want to achieve this goal?`,\n   395→            options: chatStep === 3 ? [\"3 months\", \"6 months\", \"12 months\", \"No deadline\"] : undefined,\n   396→            field: \"deadline\"\n   397→        });\n   398→    }\n   399→\n   400→    // If we have deadline, add it and show completion\n   401→    if (chatStep >= 4) {\n   402→        const deadlineStr = formState.deadline <= 3 ? \"3 months\" :\n   403→                          formState.deadline <= 6 ? \"6 months\" :\n   404→                          formState.deadline <= 12 ? \"12 months\" : \"No deadline\";\n   405→        messages.push({\n   406→            id: msgId++,\n   407→            type: \"user\",\n   408→            content: deadlineStr\n   409→        });\n   410→        messages.push({\n   411→            id: msgId++,\n   412→            type: \"bot\",\n   413→            content: `Excellent! Based on your responses, I've generated a personalized learning path. Here's your roadmap:`\n   414→        });\n   415→    }\n   416→\n   417→    return messages;\n   418→};\n   419→\n   420→interface AIChatModeProps {\n   421→    formState: GoalFormState;\n   422→    onFormChange: (updates: Partial<GoalFormState>) => void;\n   423→}\n   424→\n   425→const AIChatMode = ({ formState, onFormChange }: AIChatModeProps) => {\n   426→    // Initialize messages based on current form state (for mode switching continuity)\n   427→    const initialChatStep = getInitialChatStep(formState);\n   428→    const [messages, setMessages] = useState<Message[]>(() => createInitialMessages(formState));\n   429→    const [inputValue, setInputValue] = useState(\"\");\n   430→    const [isTyping, setIsTyping] = useState(false);\n   431→    const [pathGenerated, setPathGenerated] = useState(initialChatStep >= 4);\n   432→    const [chatStep, setChatStep] = useState(initialChatStep);\n   433→\n   434→    // Use shared path calculator hook for consistent metrics\n   435→    const { estimatedWeeks, lessons, projects, totalHours, modules, topics } = usePathCalculator(formState);\n   436→\n   437→    const simulateResponse = (userMessage: string) => {\n   438→        setIsTyping(true);\n   439→\n   440→        const userMsg: Message = {\n   441→            id: messages.length + 1,\n   442→            type: \"user\",\n   443→            content: userMessage\n   444→        };\n   445→        setMessages(prev => [...prev, userMsg]);\n   446→\n   447→        setTimeout(() => {\n   448→            let botResponse: Message;\n   449→            const currentStep = chatStep;\n   450→\n   451→            if (currentStep === 0) {\n   452→                // User just answered the goal question\n   453→                onFormChange({ goal: userMessage });\n   454→                setChatStep(1);\n   455→                botResponse = {\n   456→                    id: messages.length + 2,\n   457→                    type: \"bot\",\n   458→                    content: `Great choice! \"${userMessage}\" is an exciting goal. How many hours per week can you dedicate to learning?`,\n   459→                    options: [\"5-10 hours\", \"10-20 hours\", \"20-30 hours\", \"30+ hours\"],\n   460→                    field: \"timeCommitment\"\n   461→                };\n   462→            } else if (currentStep === 1) {\n   463→                // User just answered the time commitment question\n   464→                const timeCommitment = parseTimeCommitment(userMessage);\n   465→                onFormChange({ timeCommitment });\n   466→                setChatStep(2);\n   467→                botResponse = {\n   468→                    id: messages.length + 2,\n   469→                    type: \"bot\",\n   470→                    content: `Perfect, ${userMessage} is a solid commitment. What's your current skill level?`,\n   471→                    options: [\"Complete Beginner\", \"Some Basics\", \"Intermediate\", \"Advanced\"],\n   472→                    field: \"learningStyle\"\n   473→                };\n   474→            } else if (currentStep === 2) {\n   475→                // User just answered the skill level question (maps to learningStyle)\n   476→                onFormChange({ learningStyle: userMessage });\n   477→                setChatStep(3);\n   478→                botResponse = {\n   479→                    id: messages.length + 2,\n   480→                    type: \"bot\",\n   481→                    content: `Got it! Last question - when do you want to achieve this goal?`,\n   482→                    options: [\"3 months\", \"6 months\", \"12 months\", \"No deadline\"],\n   483→                    field: \"deadline\"\n   484→                };\n   485→            } else if (currentStep === 3) {\n   486→                // User just answered the deadline question\n   487→                const deadline = parseDeadline(userMessage);\n   488→                onFormChange({ deadline });\n   489→                setChatStep(4);\n   490→                setPathGenerated(true);\n   491→                botResponse = {\n   492→                    id: messages.length + 2,\n   493→                    type: \"bot\",\n   494→                    content: `Excellent! Based on your responses, I've generated a personalized learning path. Here's your roadmap:`