  47→    isGenerating: boolean;\n    48→    /** Generated plan */\n    49→    plan: ResolvedPlan | null;\n    50→    /** Quick metrics (for live preview) */\n    51→    getQuickMetrics: (state: GoalPathResolverState) => PathMetrics;\n    52→    /** Error message if failed */\n    53→    error: string | null;\n    54→    /** Reset state */\n    55→    reset: () => void;\n    56→}\n    57→\n    58→// ============================================================================\n    59→// FOCUS AREA MAPPING\n    60→// ============================================================================\n    61→\n    62→const FOCUS_AREA_MAPPING: Record<string, string> = {\n    63→    frontend: \"frontend\",\n    64→    backend: \"backend\",\n    65→    devops: \"devops\",\n    66→    mobile: \"mobile\",\n    67→    data: \"data\",\n    68→    ai: \"ai\",\n    69→    security: \"security\",\n    70→    testing: \"testing\",\n    71→    // Legacy mappings\n    72→    fullstack: \"frontend\", // Will also add backend\n    73→    databases: \"backend\",\n    74→    games: \"mobile\",\n    75→};\n    76→\n    77→function mapFocusAreas(focus: string[]): string[] {\n    78→    const mapped = new Set<string>();\n    79→\n    80→    for (const area of focus) {\n    81→        const key = area.toLowerCase();\n    82→        if (FOCUS_AREA_MAPPING[key]) {\n    83→            mapped.add(FOCUS_AREA_MAPPING[key]);\n    84→\n    85→            // Special case: fullstack adds both frontend and backend\n    86→            if (key === \"fullstack\") {\n    87→                mapped.add(\"backend\");\n    88→            }\n    89→        } else {\n    90→            // Use as-is if no mapping\n    91→            mapped.add(key);\n    92→        }\n    93→    }\n    94→\n    95→    return Array.from(mapped);\n    96→}\n    97→\n    98→// ============================================================================\n    99→// LEARNING STYLE MAPPING\n   100→// ==========================================