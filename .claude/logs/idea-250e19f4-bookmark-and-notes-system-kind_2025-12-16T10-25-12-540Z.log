geSquare, ThumbsUp,\n    Copy, Check, Maximize2, Volume2\n} from \"lucide-react\";\nimport { PrismaticCard } from \"@/app/shared/components\";\nimport { cn } from \"@/app/shared/lib/utils\";\nimport { BookmarkButton, BookmarkIndicator } from \"@/app/features/bookmarks\";\n\nconst codeExample = `import { useState, useEffect } from 'react';\n\nfunction useLocalStorage<T>(key: string, initialValue: T) {\n  const [storedValue, setStoredValue] = useState<T>(() => {\n    try {\n      const item = window.localStorage.getItem(key);\n      return item ? JSON.parse(item) : initialValue;\n    } catch (error) {\n      return initialValue;\n    }\n  });\n\n  const setValue = (value: T | ((val: T) => T)) => {\n    try {\n      const valueToStore = value instanceof Function\n        ? value(storedValue)\n        : value;\n      setStoredValue(valueToStore);\n      window.localStorage.setItem(key, JSON.stringify(valueToStore));\n    } catch (error) {\n      console.error(error);\n    }\n  };\n\n  return [storedValue, setValue] as const;\n}`;\n\n// Course/Chapter metadata for bookmarks\nconst courseInfo = {\n    courseId: \"react-hooks\",\n    courseName: \"React Hooks Mastery\",\n    chapterId: \"custom-hooks\",\n    chapterTitle: \"Building Your First Custom Hook\",\n};\n\n// Variant A: Classic Video + Content Layout\nexport const VariantA = () => {\n    const [isPlaying, setIsPlaying] = useState(false);\n    const [copied, setCopied] = useState(false);\n    const [currentSection, setCurrentSection] = useState(0);\n\n    const sections = [\n        { id: \"intro\", title: \"Introduction\", time: \"0:00\", completed: true },\n        { id: \"understanding\", title: \"Understanding Hooks\", time: \"2:15\", completed: true },\n        { id: \"building\", title: \"Building Custom Hooks\", time: \"8:30\", completed: false },\n        { id: \"best-practices\", title: \"Best Practices\", time: \"15:45\", completed: false },\n        { id: \"quiz\", title: \"Quiz & Summary\", time: \"22:00\", completed: false },\n    ];\n\n    const handleCopy = () => {\n        navigator.clipboard.writeText(codeExample);\n        setCopied(true);\n        setTimeout(() => setCopied(false), 2000);\n    };\n\n    return (\n        <div className=\"grid grid-cols-1 lg:grid-cols-3 gap-6\">\n            {/* Main Content */}\n            <div className=\"lg:col-span-2 space-y-6\">\n                {/* Video Player */}\n                <PrismaticCard glowColor=\"indigo\">\n                    <div className=\"aspect-video bg-slate-900 rounded-t-2xl relative overflow-hidden\">\n                        {/* Animated Background */}\n                        <motion.div\n                            animate={{ scale: [1, 1.1, 1], rotate: [0, 5, 0] }}\n                            transition={{ duration: 20, repeat: Infinity }}\n                            className=\"absolute inset-0 bg-gradient-to-br from-indigo-900 via-slate-900 to-purple-900 opacity-80\"\n                        />\n                        <div className=\"absolute inset-0 bg-[url('https://grainy-gradients.vercel.app/noise.svg')] opacity-20\" />\n\n                        {/* Play Button */}\n                        <div className=\"absolute inset-0 flex items-center justify-center\">\n                            <motion.button\n                                whileHover={{ scale: 1.1 }}\n                                whileTap={{ scale: 0.95 }}\n                                onClick={() => setIsPlaying(!isPlaying)}\n                                className=\"w-20 h-20 bg-white/10 backdrop-blur-md rounded-full flex items-center justify-center text-white border border-white/20\"\n                            >\n                                {isPlaying ? <Pause size={32} /> : <Play size={32} className=\"ml-1\" />}\n                            </motion.button>\n                        </div>\n\n                        {/* Controls */}\n                        <div className=\"absolute bottom-0 left-0 right-0