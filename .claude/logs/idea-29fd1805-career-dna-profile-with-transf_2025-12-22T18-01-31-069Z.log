efault:\n                return \"generalist\";\n        }\n    }\n\n    // Check for fullstack pattern\n    if (categoryScores[\"frontend\"] && categoryScores[\"backend\"]) {\n        const frontendRatio = categoryScores[\"frontend\"] / totalScore;\n        const backendRatio = categoryScores[\"backend\"] / totalScore;\n        if (frontendRatio > 0.25 && backendRatio > 0.25) {\n            return \"full-stack-engineer\";\n        }\n    }\n\n    // Check for tech lead pattern (broad skills + soft skills)\n    if (entries.length >= 4 && categoryScores[\"soft-skills\"]) {\n        return \"tech-lead\";\n    }\n\n    return \"generalist\";\n}\n\n/**\n * Calculate career level based on total XP\n */\nfunction calculateCareerLevel(totalXp: number): number {\n    const thresholds = [0, 500, 1500, 3000, 5000, 8000, 12000, 18000, 25000, 35000];\n    for (let i = thresholds.length - 1; i >= 0; i--) {\n        if (totalXp >= thresholds[i]) {\n            return i + 1;\n        }\n    }\n    return 1;\n}\n\n/**\n * Build career DNA strands from credentials\n */\nfunction buildStrands(credentials: SkillCredential[]): CareerDNAStrand[] {\n    const strandMap: Record<SkillCategory, SkillCredential[]> = {} as Record<SkillCategory, SkillCredential[]>;\n\n    credentials.forEach((cred) => {\n        if (!strandMap[cred.skillCategory]) {\n            strandMap[cred.skillCategory] = [];\n        }\n        strandMap[cred.skillCategory].push(cred);\n    });\n\n    return Object.entries(strandMap).map(([category, skills]) => {\n        const categoryKey = category as SkillCategory;\n        const totalXp = skills.reduce((sum, s) => sum + s.xpEarned, 0);\n        const maxXp = 5000; // Max XP for 100% strength\n\n        return {\n            category: categoryKey,\n            displayName: getCategoryDisplayName(categoryKey),\n            skills,\n            strength: Math.min(100, Math.round((totalXp / maxXp) * 100)),\n            color: SKILL_CATEGORY_COLORS[categoryKey],\n        };\n    });\n}\n\nfunction getCategoryDisplayName(category: SkillCategory): string {\n    const names: Record<SkillCategory, string> = {\n        frontend: \"Frontend\",\n        backend: \"Backend\",\n        fullstack: \"Full Stack\",\n        database: \"Database\",\n        devops: \"DevOps\",\n        mobile: \"Mobile\",\n        \"ai-ml\": \"AI/ML\",\n        security: \"Security\",\n        cloud: \"Cloud\",\n        \"soft-skills\": \"Soft Skills\",\n    };\n    return names[category];\n}\n\n/**\n * Recalculate profile stats and archetype\n */\nfunction recalculateProfile(storage: CareerDNAStorage): void {\n    if (!storage.profile) return;\n\n    const credentials = Object.values(storage.credentials);\n    const verifiedCredentials = credentials.filter(\n        (c) => c.verificationStatus === \"verified\"\n    );\n\n    // Update profile with derived data\n    storage.profile.credentials = verifiedCredentials;\n    storage.profile.strands = buildStrands(verifiedCredentials);\n    storage.profile.totalXp = verifiedCredentials.reduce((sum, c) => sum + c.xpEarned, 0);\n    storage.profile.careerLevel = calculateCareerLevel(storage.profile.totalXp);\n    storage.profile.archetype = calculateArchetype(verifiedCredentials);\n    storage.profile.lastUpdatedAt = new Date().toISOString();\n\n    // Update stats\n    const credentialsByCategory: Record<SkillCategory, number> = {} as Record<SkillCategory, number>;\n    verifiedCredentials.forEach((c) => {\n        credentialsByCategory[c.skillCategory] = (credentialsByCategory[c.skillCategory] || 0) + 1;\n    });\n\n    storage.profile.stats = {\n        ...storage.profile.stats,\n        totalCredentials: verifiedCredentials.length,\n        credentialsByCategory,\n        lastActivityAt: new Date().toISOString(),\n    };\n}\n\n// ============================================================================\n// CREDENTIAL OPERATIONS\n// ============================================================================\n\n/