age = getStorage();\n    return storage.credentials[id] || null;\n}\n\n/**\n * Get credential by hash (for verification)\n */\nexport function getCredentialByHash(hash: string): SkillCredential | null {\n    const storage = getStorage();\n    return Object.values(storage.credentials).find(\n        (c) => c.credentialHash === hash\n    ) || null;\n}\n\n/**\n * Verify a credential\n */\nexport function verifyCredential(hashOrId: string): {\n    isValid: boolean;\n    credential?: SkillCredential;\n    message: string;\n} {\n    const storage = getStorage();\n\n    // Try to find by hash first\n    let credential = Object.values(storage.credentials).find(\n        (c) => c.credentialHash === hashOrId\n    );\n\n    // Try by ID if not found\n    if (!credential) {\n        credential = storage.credentials[hashOrId];\n    }\n\n    if (!credential) {\n        return {\n            isValid: false,\n            message: \"Credential not found. The verification code may be invalid.\",\n        };\n    }\n\n    if (credential.verificationStatus === \"revoked\") {\n        return {\n            isValid: false,\n            credential,\n            message: \"This credential has been revoked.\",\n        };\n    }\n\n    if (credential.verificationStatus === \"expired\") {\n        return {\n            isValid: false,\n            credential,\n            message: \"This credential has expired.\",\n        };\n    }\n\n    // Increment verification count\n    if (storage.profile) {\n        storage.profile.stats.verificationRequests++;\n        saveStorage(storage);\n    }\n\n    return {\n        isValid: true,\n        credential,\n        message: \"Credential verified successfully.\",\n    };\n}\n\n/**\n * Get credentials by category\n */\nexport function getCredentialsByCategory(category: SkillCategory): SkillCredential[] {\n    const storage = getStorage();\n    return Object.values(storage.credentials)\n        .filter((c) => c.skillCategory === category)\n        .sort((a, b) => new Date(b.issuedAt).getTime() - new Date(a.issuedAt).getTime());\n}\n\n/**\n * Increment share count for a credential\n */\nexport function incrementCredentialShareCount(credentialId: string): void {\n    const storage = getStorage();\n    if (storage.credentials[credentialId]) {\n        storage.credentials[credentialId].shareCount++;\n        if (storage.profile) {\n            storage.profile.stats.totalShares++;\n        }\n        saveStorage(storage);\n    }\n}\n\n// ============================================================================\n// SHARE HISTORY\n// ============================================================================\n\n/**\n * Record a share event\n */\nexport function recordShare(\n    platform: SharePlatform,\n    credentialIds: string[]\n): ShareHistoryEntry {\n    const storage = getStorage();\n\n    const entry: ShareHistoryEntry = {\n        id: generateId(),\n        sharedAt: new Date().toISOString(),\n        platform,\n        credentialIds,\n        views: 0,\n    };\n\n    storage.shareHistory.push(entry);\n\n    // Increment share counts\n    credentialIds.forEach((id) => {\n        if (storage.credentials[id]) {\n            storage.credentials[id].shareCount++;\n        }\n    });\n\n    if (storage.profile) {\n        storage.profile.stats.totalShares++;\n    }\n\n    saveStorage(storage);\n    return entry;\n}\n\n/**\n * Get share history\n */\nexport function getShareHistory(): ShareHistoryEntry[] {\n    const storage = getStorage();\n    return storage.shareHistory.sort(\n        (a, b) => new Date(b.sharedAt).getTime() - new Date(a.sharedAt).getTime()\n    );\n}\n\n// ============================================================================\n// DATA EXPORT/IMPORT\n// ============================================================================\n\n/**\n * Export all Career DNA data\n */\nexport function exportCareerDNAData(): string {\n    const storage = getStorage();\n    return JSON.stringify(storag