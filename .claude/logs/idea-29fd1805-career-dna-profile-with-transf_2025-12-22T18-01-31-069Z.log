specialist\"\n    | \"backend-specialist\"\n    | \"devops-engineer\"\n    | \"mobile-developer\"\n    | \"data-engineer\"\n    | \"security-specialist\"\n    | \"tech-lead\"\n    | \"generalist\";\n\n/**\n * Career DNA strand - a group of related skills\n */\nexport interface CareerDNAStrand {\n    /** Category this strand represents */\n    category: SkillCategory;\n    /** Display name */\n    displayName: string;\n    /** Skills in this strand */\n    skills: SkillCredential[];\n    /** Aggregate strength (0-100) */\n    strength: number;\n    /** Color for visualization */\n    color: string;\n}\n\n/**\n * The complete Career DNA Profile\n */\nexport interface CareerDNAProfile {\n    /** Unique profile identifier */\n    id: string;\n    /** Profile public key for verification */\n    publicKey: string;\n    /** Owner's display name */\n    displayName: string;\n    /** Owner's title/role */\n    title?: string;\n    /** Profile avatar URL */\n    avatarUrl?: string;\n    /** Career archetype derived from skill distribution */\n    archetype: CareerArchetype;\n    /** All skill strands */\n    strands: CareerDNAStrand[];\n    /** Total XP across all skills */\n    totalXp: number;\n    /** Career level (1-10) */\n    careerLevel: number;\n    /** All credentials */\n    credentials: SkillCredential[];\n    /** Profile created at */\n    createdAt: string;\n    /** Profile last updated */\n    lastUpdatedAt: string;\n    /** Profile visibility */\n    isPublic: boolean;\n    /** Custom profile URL slug */\n    profileSlug?: string;\n    /** Profile stats */\n    stats: CareerDNAStats;\n}\n\n/**\n * Statistics for a Career DNA profile\n */\nexport interface CareerDNAStats {\n    /** Total credentials earned */\n    totalCredentials: number;\n    /** Credentials by category */\n    credentialsByCategory: Record<SkillCategory, number>;\n    /** Profile views */\n    profileViews: number;\n    /** Times credentials were shared */\n    totalShares: number;\n    /** Verification requests */\n    verific
