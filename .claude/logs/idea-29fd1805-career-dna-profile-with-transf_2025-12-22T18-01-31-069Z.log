  <div>\n                                <h2 className=\"text-lg font-bold text-[var(--text-primary)]\">\n                                    Share Credentials\n                                </h2>\n                                <p className=\"text-sm text-[var(--text-muted)]\">\n                                    {selectedIds.size} of {credentials.length} selected\n                                </p>\n                            </div>\n                        </div>\n                        <div className=\"flex gap-2\">\n                            <button\n                                onClick={selectAll}\n                                className=\"px-3 py-1.5 text-xs font-medium rounded-lg bg-[var(--surface-inset)] text-[var(--text-secondary)] hover:bg-[var(--surface-overlay)] transition-colors\"\n                                data-testid=\"select-all-btn\"\n                            >\n                                Select All\n                            </button>\n                            {selectedIds.size > 0 && (\n                                <button\n                                    onClick={clearSelection}\n                                    className=\"px-3 py-1.5 text-xs font-medium rounded-lg bg-[var(--surface-inset)] text-[var(--text-secondary)] hover:bg-[var(--surface-overlay)] transition-colors\"\n                                    data-testid=\"clear-selection-btn\"\n                                >\n                                    Clear\n                                </button>\n                            )}\n                        </div>\n                    </div>\n\n                    {/* Credential Selection */}\n                    <div className=\"space-y-2\">\n                        {displayedCredentials.map((credential) => (\n                            <motion.button\n                                key={credential.id}\n                                onClick={() => toggleCredential(credential.id)}\n                                className={cn(\n                                    \"w-full flex items-center gap-3 p-3 rounded-xl border-2 transition-all text-left\",\n                                    selectedIds.has(credential.id)\n                                        ? \"border-[var(--color-cyan)] bg-[color-mix(in_srgb,var(--color-cyan)_5%,transparent)]\"\n                                        : \"border-transparent bg-[var(--surface-inset)] hover:bg-[var(--surface-overlay)]\"\n                                )}\n                                whileHover={{ scale: 1.01 }}\n                                whileTap={{ scale: 0.99 }}\n                                data-testid={`credential-select-${credential.id}`}\n                            >\n                                <div\n                                    className={cn(\n                                        \"w-5 h-5 rounded-md border-2 flex items-center justify-center transition-colors\",\n                                        selectedIds.has(credential.id)\n                                            ? \"border-[var(--color-cyan)] bg-[var(--color-cyan)]\"\n                                            : \"border-[var(--border-default)]\"\n                                    )}\n                                >\n                                    {selectedIds.has(credential.id) && (\n                                        <Check size={12} className=\"text-white\" />\n                                    )}\n                                </div>\n                                <Award\n                                    size={ICON_SIZES.md}\n                                    style={{ color: SKILL_CATEGORY_COLORS[credential.skillCategory] }}\n                                />\n                                <div className=\"flex-1 min-w-0\">\n                                    <div className=\"font-medium text-[var(--text-primary)] truncate\">\n                                        {credential.skillName}\n                                    </div>\n                                    <div className=\"text-xs text-[var(--text-muted)]\">\n                                        {credential.xpEarned} XP • {credential.proficiencyLevel}\n                                    </div>\n                                </div>\n                            </motion.button>\n                        ))}\n\n                        {credentials.length > 4 && (\n                            <button\n                                onClick={() => setShowAllCredentials(!showAllCredentials)}\n                                className=\"w-full flex items-center justify-center gap-2 py-2 text-sm text-[var(--text-muted)] hover:text-[var(--text-secondary)] transition-colors\"\n                                data-testid=\"show-more-credentials-btn\"\n                            >\n                                {showAllCredentials ? (\n                                    <>\n                                        <ChevronUp size={ICON_SIZES.sm} />\n                                        Show Less\n                                    </>\n                                ) : (\n                                    <>\n                                        <ChevronDown size={ICON_SIZES.sm} />\n                                        Show {credentials.length - 4} More\n                                    </>\n                                )}\n                            </button>\n                        )}\n                    </div>\n                </div>\n            </PrismaticCard>\n\n            {/* Share Platforms */}\n            <PrismaticCard glowColor=\"indigo\">\n                <div className=\"p-6\">\n                    <h3 className=\"font-bold text-[var(--text-primary)] mb-4\">Share To</h3>\n                    <div className=\"grid grid-cols-2 md:grid-cols-3 gap-3\">\n                        {SHARE_PLATFORMS.map((platform) => (\n                            <motion.button\n                                key={platform.id}\n                                onClick={() => handleShare(platform.id)}\n                                disabled={selectedIds.size === 0 || sharingPlatform !== null}\n                                className={cn(\n                                    \"flex flex-col items-center gap-2 p-4 rounded-xl border transition-all\",\n                                    selectedIds.size === 0\n                                        ? \"opacity-50 cursor-not-allowed border-[var(--border-default)] bg-[var(--surface-inset)]\"\n                                        : \"border-[var(--border-default)] bg-[var(--surface-inset)] hover:bg-[var(--surface-overlay)] hover:border-[var(--border-hover)]\"\n                                )}\n                                whileHover={selectedIds.size > 0 ? { scale: 1.02 } : {}}\n                                whileTap={selectedIds.size > 0 ? { scale: 0.98 } : {}}\n                                data-testid={`share-${platform.id}-btn`}\n                            >\n                                <div\n                                    className=\"w-10 h-10 rounded-xl flex items-center justify-center\"\n                                    style={{ backgroundColor: `${platform.color}20` }}\n                                >\n                                    {sharingPlatform === platform.id ? (\n                                        shareSuccess === platform.id ? (\n                                            <Check size={ICON_SIZES.md} style={{ color: \"var(--color-emerald)\" }} />\n                                        ) : (\n                                            <div\n                                                className=\"w-5 h-5 border-2 border-t-transparent rounded-full animate-spin\"\n                                                style={{ borderColor: platform.color }}\n                                            />\n                                        )\n                                    ) : (\n                                        <platform.icon size={ICON_SIZES.md} style={{ color: platform.color }} />\n                                    )}\n                                </div>\n                                <div className=\"text-center\">\n                                    <div className=\"text-sm font-medium text-[var(--text-primary)]\">\n                                        {platform.name}\n                                    </div>\n                                    <div className=\"text-xs text-[var(--text-muted)]\">\n                                        {platform.description}\n                                    </div>\n                                </div>\n                            </motion.button>\n                        ))}\n                    </div>\n                </div>\n            </PrismaticCard>\n\n            {/* Export Options */}\n            <PrismaticCard glowColor=\"emerald\">\n                <div className=\"p-6\">\n                    <h3 className=\"font-bold text-[var(--text-primary)] mb-4\">Export Options</h3>\n                    <div className=\"flex flex-col sm:flex-row gap-3\">\n                        <button\n                            onClick={handleExportPassport}\n                            className=\"flex-1 flex items-center justify-center gap-2 px-4 py-3 rounded-xl font-medium text-white transition-opacity hover:opacity-90\"\n                            style={{\n                                background: \"linear-gradient(to right, var(--color-emerald), var(--color-cyan))\",\n                            }}\n                            data-testid=\"export-passport-btn\"\n                        >\n                            <Download size={ICON_SIZES.sm} />\n                            Export Full Passport\n                        </button>\n                        <button\n                            onClick={() => {\n                                const data = exportData();\n                                const blob = new Blob([data], { type: \"application/json\" });\n                                const url = URL.createObjectURL(blob);\n                                const a = document.createElement(\"a\");\n                                a.href = url;\n                                a.download = `career-dna-backup-${Date.now()}.json`;\n                                document.body.appendChild(a);\n                                a.click();\n                                document.body.removeChild(a);\n                                URL.revokeObjectURL(url);\n                            }}\n                            className=\"flex-1 flex items-center justify-center gap-2 px-4 py-3 rounded-xl font-medium bg-[var(--surface-inset)] text-[var(--text-secondary)] hover:bg-[var(--surface-overlay)] transition-colors\"\n                            data-testid=\"backup-data-btn\"\n                        >\n                            <Shield size={ICON_SIZES.sm} />\n                            Backup All Data\n                        </button>\n                    </div>\n                </div>\n            </PrismaticCard>\n\n            {/* Partner Recognition */}\n            <PrismaticCard glowColor=\"purple\">\n                <div className=\"p-6\">\n                    <div className=\"flex items-center gap-2 mb-4\">\n                        <Sparkles size={ICON_SIZES.md} style={{ color: \"var(--color-purple)\" }} />\n                        <h3 className=\"font-bold text-[var(--text-primary)]\">Recognized By</h3>\n                    </div>\n                    <p className=\"text-sm text-[var(--text-muted)] mb-4\">\n                        These companies recognize Career DNA credentials\n                    </p>\n                    <div className=\"flex flex-wrap gap-3\">\n                        {PARTNER_COMPANIES.map((partner) => (\n                            <div\n                                key={partner.partnerId}\n                                className=\"flex items-center gap-2 px-3 py-2 rounded-lg bg-[var(--surface-inset)]\"\n                                data-testid={`partner-${partner.partnerId}`}\n                            >\n                                <div className=\"w-6 h-6 rounded bg-[var(--surface-overlay)] flex items-center justify-center text-xs font-bold text-[var(--text-secondary)]\">\n                                    {partner.partnerName.charAt(0)}\n                                </div>\n                                <span className=\"text-sm font-medium text-[var(--text-primary)]\">\n                                    {partner.partnerName}\n                                </span>\n                                {partner.recognitionLevel === \"verified-partner\" && (\n                                    <Shield size={ICON_SIZES.xs} style={{ color: \"var(--color-emerald)\" }} />\n                                )}\n                            </div>\n                        ))}\n                    </div>\n                </div>\n            </PrismaticCard>\n        </div>\n    );\n};\n\n// ============================================================================\n// Helper Functions\n// ============================================================================\n\nfunction generateShareUrl(platform: SharePlatform, credentials: SkillCredential[]): string {\n    const baseUrl = typeof window !== \"undefined\" ? window.location.origin : \"\";\n    const credentialSummary = credentials\n        .map((c) => `${c.skillName} (${c.proficiencyLevel})`)\n        .join(\", \");\n\n    const shareText = `I've earned ${credentials.length} verified skill credential${credentials.length !== 1 ? \"s\" : \"\"}: ${credentialSummary}. Verify at ${baseUrl}/verify`;\n\n    switch (platform) {\n        case \"linkedin\":\n            return `https://www.linkedin.com/sharing/share-offsite/?url=${en